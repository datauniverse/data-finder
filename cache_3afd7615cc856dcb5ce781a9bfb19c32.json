b'<!DOCTYPE html>\n<html class="client-nojs" lang="en" dir="ltr">\n<head>\n<meta charset="UTF-8"/>\n<title>Glyph - Wikipedia</title>\n<script>document.documentElement.className = document.documentElement.className.replace( /(^|\\s)client-nojs(\\s|$)/, "$1client-js$2" );</script>\n<script>(window.RLQ=window.RLQ||[]).push(function(){mw.config.set({"wgCanonicalNamespace":"","wgCanonicalSpecialPageName":false,"wgNamespaceNumber":0,"wgPageName":"Glyph","wgTitle":"Glyph","wgCurRevisionId":788640978,"wgRevisionId":788640978,"wgArticleId":12904,"wgIsArticle":true,"wgIsRedirect":false,"wgAction":"view","wgUserName":null,"wgUserGroups":["*"],"wgCategories":["Pages using web citations with no URL","Pages using citations with accessdate and no URL","Use dmy dates from July 2012","Articles needing additional references from June 2010","All articles needing additional references","All articles with unsourced statements","Articles with unsourced statements from November 2016","Articles with specifically marked weasel-worded phrases from March 2017","Wikipedia articles needing reorganization from June 2016","All articles that may contain original research","Articles that may contain original research from January 2017","Commons category with page title different than on Wikidata","Archaeological terminology","Graphemes","Glyphs","Infographics","Typographical symbols","Typography"],"wgBreakFrames":false,"wgPageContentLanguage":"en","wgPageContentModel":"wikitext","wgSeparatorTransformTable":["",""],"wgDigitTransformTable":["",""],"wgDefaultDateFormat":"dmy","wgMonthNames":["","January","February","March","April","May","June","July","August","September","October","November","December"],"wgMonthNamesShort":["","Jan","Feb","Mar","Apr","May","Jun","Jul","Aug","Sep","Oct","Nov","Dec"],"wgRelevantPageName":"Glyph","wgRelevantArticleId":12904,"wgRequestId":"WZ6@QgpAMEsAAIJfKAMAAAAK","wgIsProbablyEditable":true,"wgRelevantPageIsProbablyEditable":true,"wgRestrictionEdit":[],"wgRestrictionMov