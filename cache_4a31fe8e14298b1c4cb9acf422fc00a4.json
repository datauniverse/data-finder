n<td></td>\n<td></td>\n<td></td>\n<td></td>\n</tr>\n<tr>\n<td>Sn<sub>97</sub>Sb<sub>3</sub></td>\n<td>232/238<sup id="cite_ref-67" class="reference"><a href="#cite_note-67">[67]</a></sup></td>\n<td>\xe2\x80\x93</td>\n<td>no</td>\n<td></td>\n<td>97</td>\n<td></td>\n<td></td>\n<td></td>\n<td>3</td>\n<td></td>\n<td></td>\n<td></td>\n<td></td>\n<td></td>\n<td></td>\n</tr>\n<tr>\n<td>Sn<sub>99</sub>Sb<sub>1</sub></td>\n<td>232/235<sup id="cite_ref-68" class="reference"><a href="#cite_note-68">[68]</a></sup></td>\n<td>\xe2\x80\x93</td>\n<td>no</td>\n<td></td>\n<td>99</td>\n<td></td>\n<td></td>\n<td></td>\n<td>1</td>\n<td></td>\n<td></td>\n<td></td>\n<td></td>\n<td></td>\n<td></td>\n</tr>\n<tr>\n<td>Sn<sub>99</sub>Ag<sub>0.3</sub>Cu<sub>0.7</sub></td>\n<td></td>\n<td>\xe2\x80\x93</td>\n<td></td>\n<td></td>\n<td>99</td>\n<td></td>\n<td>0.3</td>\n<td>0.7</td>\n<td></td>\n<td></td>\n<td></td>\n<td></td>\n<td></td>\n<td></td>\n<td></td>\n</tr>\n<tr>\n<td>Sn<sub>96.2</sub>Ag<sub>2.5</sub>Cu<sub>0.8</sub>Sb<sub>0.5</sub></td>\n<td>217\xe2\x80\x93225 217<sup id="cite_ref-smarttec_21-25" class="reference"><a href="#cite_note-smarttec-21">[21]</a></sup></td>\n<td>\xe2\x80\x93</td>\n<td></td>\n<td><b>Ag03A</b>. Patented by <a href="/wiki/AIM_alliance" title="AIM alliance">AIM alliance</a>.</td>\n<td>96.2</td>\n<td></td>\n<td>2.5</td>\n<td>0.8</td>\n<td>0.5</td>\n<td></td>\n<td></td>\n<td></td>\n<td></td>\n<td></td>\n<td></td>\n</tr>\n<tr>\n<td>Sn<sub>88</sub>In<sub>8.0</sub>Ag<sub>3.5</sub>Bi<sub>0.5</sub></td>\n<td>197\xe2\x80\x93208</td>\n<td>\xe2\x80\x93</td>\n<td></td>\n<td>Patented by <a href="/wiki/Panasonic_Corporation" class="mw-redirect" title="Panasonic Corporation">Matsushita/Panasonic</a>.</td>\n<td>88</td>\n<td></td>\n<td>3.5</td>\n<td></td>\n<td></td>\n<td>0.5</td>\n<td>8</td>\n<td></td>\n<td></td>\n<td></td>\n<td></td>\n</tr>\n<tr>\n<td>Bi<sub>57</sub>Sn<sub>42</sub>Ag<sub>1</sub></td>\n<td>137/139 139/140<sup id="cite_ref-69" class="reference"><a href="#cite_note-69"