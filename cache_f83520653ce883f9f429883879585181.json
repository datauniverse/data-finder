b'<!DOCTYPE html>\n<html class="client-nojs" lang="en" dir="ltr">\n<head>\n<meta charset="UTF-8"/>\n<title>East Midlands English - Wikipedia</title>\n<script>document.documentElement.className = document.documentElement.className.replace( /(^|\\s)client-nojs(\\s|$)/, "$1client-js$2" );</script>\n<script>(window.RLQ=window.RLQ||[]).push(function(){mw.config.set({"wgCanonicalNamespace":"","wgCanonicalSpecialPageName":false,"wgNamespaceNumber":0,"wgPageName":"East_Midlands_English","wgTitle":"East Midlands English","wgCurRevisionId":779458141,"wgRevisionId":779458141,"wgArticleId":4813972,"wgIsArticle":true,"wgIsRedirect":false,"wgAction":"view","wgUserName":null,"wgUserGroups":["*"],"wgCategories":["Wikipedia articles incorporating a citation from the 1911 Encyclopaedia Britannica with Wikisource reference","Webarchive template wayback links","Use British English from September 2013","Use dmy dates from September 2013","All articles with unsourced statements","Articles with unsourced statements from April 2012","Articles with unsourced statements from July 2013","Articles with unsourced statements from February 2010","English culture","English language in England","Languages of the United Kingdom","Culture in Derbyshire","Leicestershire","Lincolnshire","Rutland","Culture in the East Midlands","Nottinghamshire"],"wgBreakFrames":false,"wgPageContentLanguage":"en","wgPageContentModel":"wikitext","wgSeparatorTransformTable":["",""],"wgDigitTransformTable":["",""],"wgDefaultDateFormat":"dmy","wgMonthNames":["","January","February","March","April","May","June","July","August","September","October","November","December"],"wgMonthNamesShort":["","Jan","Feb","Mar","Apr","May","Jun","Jul","Aug","Sep","Oct","Nov","Dec"],"wgRelevantPageName":"East_Midlands_English","wgRelevantArticleId":4813972,"wgRequestId":"WaJA-gpAEKkAAFIBDjIAAABY","wgIsProbablyEditable":true,"wgRelevantPageIsProbablyEditable":true,"wgRestrictionEdit":[],"wgRestrictionMove":[],"wgFlaggedRevsParams":{"tags":{}}