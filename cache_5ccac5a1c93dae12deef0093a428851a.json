b'<!DOCTYPE html>\n<html class="client-nojs" lang="en" dir="ltr">\n<head>\n<meta charset="UTF-8"/>\n<title>Computer cluster - Wikipedia</title>\n<script>document.documentElement.className = document.documentElement.className.replace( /(^|\\s)client-nojs(\\s|$)/, "$1client-js$2" );</script>\n<script>(window.RLQ=window.RLQ||[]).push(function(){mw.config.set({"wgCanonicalNamespace":"","wgCanonicalSpecialPageName":false,"wgNamespaceNumber":0,"wgPageName":"Computer_cluster","wgTitle":"Computer cluster","wgCurRevisionId":797306044,"wgRevisionId":797306044,"wgArticleId":18949896,"wgIsArticle":true,"wgIsRedirect":false,"wgAction":"view","wgUserName":null,"wgUserGroups":["*"],"wgCategories":["CS1 maint: Explicit use of et al.","CS1 maint: Date format","CS1 maint: Date and year","All articles lacking reliable references","Articles lacking reliable references from June 2017","All articles with unsourced statements","Articles with unsourced statements from October 2014","Articles to be expanded from October 2014","All articles to be expanded","Articles using small message boxes","Wikipedia articles needing clarification from October 2014","Articles with unsourced statements from November 2013","Wikipedia articles needing clarification from November 2013","Articles with unsourced statements from August 2013","Cluster computing","Parallel computing","Concurrent computing","Supercomputers","Local area networks","Classes of computers","Fault-tolerant computer systems"],"wgBreakFrames":false,"wgPageContentLanguage":"en","wgPageContentModel":"wikitext","wgSeparatorTransformTable":["",""],"wgDigitTransformTable":["",""],"wgDefaultDateFormat":"dmy","wgMonthNames":["","January","February","March","April","May","June","July","August","September","October","November","December"],"wgMonthNamesShort":["","Jan","Feb","Mar","Apr","May","Jun","Jul","Aug","Sep","Oct","Nov","Dec"],"wgRelevantPageName":"Computer_cluster","wgRelevantArticleId":18949896,"wgRequestId":"WaJj6gpAMFgAABBuRCQAAABB","wgI