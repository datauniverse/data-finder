tr>\n<tr>\n<td rowspan="2"><b>p, pp</b></td>\n<td>usually</td>\n<td><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA">/p/</span></td>\n<td><b>p</b>ill, ha<b>pp</b>y, sou<b>p</b>, cor<b>p</b>se, scri<b>p</b>t</td>\n<td>\xc3\x98</td>\n<td>cor<b>p</b>s, cou<b>p</b>, ras<b>p</b>berry, recei<b>p</b>t</td>\n</tr>\n<tr>\n<td>initially before n, s, t</td>\n<td>\xc3\x98</td>\n<td><b>pn</b>eumonia, <b>ps</b>yche, <b>pt</b>omaine</td>\n<td><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA">/p/</span></td>\n<td><b>p</b>sst</td>\n</tr>\n<tr>\n<td colspan="2"><b>ph, pph</b></td>\n<td><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA">/f/</span></td>\n<td><b>ph</b>otogra<b>ph</b>, sa<b>pph</b>ire</td>\n<td><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA">/v/</span><br />\n<span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA">/pf/</span></td>\n<td>Ste<b>ph</b>en<br />\ncam<b>ph</b>or</td>\n</tr>\n<tr>\n<td colspan="2"><b>q</b> (not before u)</td>\n<td><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA">/k/</span></td>\n<td>Ira<b>q</b>, I<b>q</b>aluit</td>\n<td></td>\n<td></td>\n</tr>\n<tr>\n<td rowspan="2"><b>r, rr, rh, rrh</b></td>\n<td>usually</td>\n<td><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA">/\xc9\xb9/</span></td>\n<td><b>r</b>ay, pa<b>rr</b>ot, <b>rh</b>yme, dia<b>rrh</b>oea</td>\n<td>\xc3\x98</td>\n<td>i<b>r</b>on</td>\n</tr>\n<tr>\n<td>\n<ul>\n<li>before consonant</li>\n<li>finally</li>\n<li>before final e</li>\n</ul>\n</td>\n<td>\xc3\x98 in <a href="/wiki/Non-rhotic" class="mw-redirect" title="Non-rhotic">non-rhotic</a><br />\ndialects like RP</td>\n<td>ca<b>r</b>t, bu<b>rr</b>, fi<b>r</b>, ca<b>r</b>e, walke<b>r</b>, tea<b>r</b>, hu<b>r</b>t, my<b>rrh</b></td>\n<td></td>\n<td></td>\n</tr>\n<tr>\n<td colspan="6">See below fo