r.load(["ext.cite.a11y","site","mediawiki.page.startup","mediawiki.user","mediawiki.hidpi","mediawiki.page.ready","jquery.tablesorter","mediawiki.toc","mediawiki.searchSuggest","ext.gadget.teahouse","ext.gadget.ReferenceTooltips","ext.gadget.watchlist-notice","ext.gadget.DRN-wizard","ext.gadget.charinsert","ext.gadget.refToolbar","ext.gadget.extra-toolbar-buttons","ext.gadget.switcher","ext.gadget.featured-articles-links","ext.centralauth.centralautologin","mmv.head","mmv.bootstrap.autostart","ext.visualEditor.desktopArticleTarget.init","ext.visualEditor.targetLoader","ext.eventLogging.subscriber","ext.wikimediaEvents","ext.navigationTiming","ext.uls.eventlogger","ext.uls.init","ext.uls.interface","ext.centralNotice.geoIP","ext.centralNotice.startUp","skins.vector.js"]);});</script>\n<link rel="stylesheet" href="/w/load.php?debug=false&amp;lang=en&amp;modules=ext.cite.styles%7Cext.uls.interlanguage%7Cext.visualEditor.desktopArticleTarget.noscript%7Cext.wikimediaBadges%7Cmediawiki.legacy.commonPrint%2Cshared%7Cmediawiki.sectionAnchor%7Cmediawiki.skinning.interface%7Cskins.vector.styles%7Cwikibase.client.init&amp;only=styles&amp;skin=vector"/>\n<script async="" src="/w/load.php?debug=false&amp;lang=en&amp;modules=startup&amp;only=scripts&amp;skin=vector"></script>\n<meta name="ResourceLoaderDynamicStyles" content=""/>\n<link rel="stylesheet" href="/w/load.php?debug=false&amp;lang=en&amp;modules=ext.gadget.charinsert-styles&amp;only=styles&amp;skin=vector"/>\n<link rel="stylesheet" href="/w/load.php?debug=false&amp;lang=en&amp;modules=site.styles&amp;only=styles&amp;skin=vector"/>\n<meta name="generator" content="MediaWiki 1.30.0-wmf.14"/>\n<meta name="referrer" content="origin-when-cross-origin"/>\n<meta property="og:image" content="https://upload.wikimedia.org/wikipedia/commons/e/ec/Proclamtion.jpg"/>\n<link rel="alternate" href="android-app://org.wikipedia/http/en.m.wikipedia.org/wiki/Ely_and_Littleport_riots_of_1816"/>\n<link rel="alternate" type="application/x-wiki" title="Edit this page" href="/w/index.php?title=Ely_and_Littleport_riots_of_1816&amp;action=edit"/>\n<link rel="edit" title="Edit this page" href="/w/index.php?title=Ely_and_Littleport_riots_of_1816&amp;action=edit"/>\n<link rel="apple-touch-icon" href="/static/apple-touch/wikipedia.png"/>\n<link rel="shortcut icon" href="/static/favicon/wikipedia.ico"/>\n<link rel="search" type="application/opensearchdescription+xml" href="/w/opensearch_desc.php" title="Wikipedia (en)"/>\n<link rel="EditURI" type="application/rsd+xml" href="//en.wikipedia.org/w/api.php?action=rsd"/>\n<link rel="license" href="//creativecommons.org/licenses/by-sa/3.0/"/>\n<link rel="canonical" href="https://en.wikipedia.org/wiki/Ely_and_Littleport_riots_of_1816"/>\n<link rel="dns-prefetch" href="//login.wikimedia.org"/>\n<link rel="dns-prefetch" href="//meta.wikimedia.org" />\n<!--[if lt IE 9]><script src="/resources/lib/html5shiv/html5shiv.min.js"></script><![endif]-->\n</head>\n<body class="mediawiki ltr sitedir-ltr mw-hide-empty-elt ns-0 ns-subject page-Ely_and_Littleport_riots_of_1816 rootpage-Ely_and_Littleport_riots_of_1816 skin-vector action-view">\t\t<div id="mw-page-base" class="noprint"></div>\n\t\t<div id="mw-head-base" class="noprint"></div>\n\t\t<div id="content" class="mw-body" role="main">\n\t\t\t<a id="top"></a>\n\n\t\t\t\t\t\t\t<div id="siteNotice" class="mw-body-content"><!-- CentralNotice --></div>\n\t\t\t\t\t\t<div class="mw-indicators mw-body-content">\n<div id="mw-indicator-good-star" class="mw-indicator"><a href="/wiki/Wikipedia:Good_articles" title="This is a good article. Click here for more information."><img alt="This is a good article. Click here for more information." src="//upload.wikimedia.org/wikipedia/en/thumb/9/94/Symbol_support_vote.svg/19px-Symbol_support_vote.svg.png" width="19" height="20" srcset="//upload.wikimedia.org/wikipedia/en/thumb/9/94/Symbol_support_vote.svg/29px-Symbol_support_vote.svg.png 1.5x, //upload.wikimedia.org/wikipedia/en/thumb/9/94/Symbol_support_vote.svg/39px-Symbol_support_vote.svg.png 2x" data-file-width="180" data-file-height="185" /></a></div>\n</div>\n\t\t\t<h1 id="firstHeading" class="firstHeading" lang="en">Ely and Littleport riots of 1816</h1>\n\t\t\t\t\t\t\t\t\t<div id="bodyContent" class="mw-body-content">\n\t\t\t\t\t\t\t\t\t<div id="siteSub" class="noprint">From Wikipedia, the free encyclopedia</div>\n\t\t\t\t\t\t\t\t<div id="contentSub"></div>\n\t\t\t\t\t\t\t\t\t\t\t\t<div id="jump-to-nav" class="mw-jump">\n\t\t\t\t\tJump to:\t\t\t\t\t<a href="#mw-head">navigation</a>, \t\t\t\t\t<a href="#p-search">search</a>\n\t\t\t\t</div>\n\t\t\t\t<div id="mw-content-text" lang="en" dir="ltr" class="mw-content-ltr"><div class="mw-parser-output"><table class="infobox vevent" style="width:22em">\n<caption class="summary">Ely and Littleport riots</caption>\n<tr>\n<td colspan="2" style="text-align:center;border-bottom:#aaa solid 1px"><a href="/wiki/File:Proclamtion.jpg" class="image" title="The MAGISTRATES agree, and do Order, that the OVERSEERS shall pay to each poor Family Two SHILLINGS per Head per Week, when FLOUR is Half-a-Crown a stone, such Allowance be raised in Proportion when the Price of Flour is higher, and that the Price of Labour shall be Two Shillings per Day, whether Married or Single, and that the Labourer shall be paid his full Wages by the Farmer who Hires him. No Person to be prosecuted for any thing that has been done to the present Time provided that every MAN immediately returns peaceably to his own Home. ELY, 23 May 1816."><img alt="The MAGISTRATES agree, and do Order, that the OVERSEERS shall pay to each poor Family Two SHILLINGS per Head per Week, when FLOUR is Half-a-Crown a stone, such Allowance be raised in Proportion when the Price of Flour is higher, and that the Price of Labour shall be Two Shillings per Day, whether Married or Single, and that the Labourer shall be paid his full Wages by the Farmer who Hires him. No Person to be prosecuted for any thing that has been done to the present Time provided that every MAN immediately returns peaceably to his own Home. ELY, 23 May 1816." src="//upload.wikimedia.org/wikipedia/commons/thumb/e/ec/Proclamtion.jpg/240px-Proclamtion.jpg" width="240" height="364" srcset="//upload.wikimedia.org/wikipedia/commons/thumb/e/ec/Proclamtion.jpg/360px-Proclamtion.jpg 1.5x, //upload.wikimedia.org/wikipedia/commons/thumb/e/ec/Proclamtion.jpg/480px-Proclamtion.jpg 2x" data-file-width="990" data-file-height="1500" /></a>\n<div>Printed bill 23 May 1816<sup id="cite_ref-FOOTNOTEStorey200985_1-0" class="reference"><a href="#cite_note-FOOTNOTEStorey200985-1">[1]</a></sup><br />\nprepared by <a href="/wiki/The_Reverend" title="The Reverend">the Reverend</a> <a href="/wiki/Justice_of_the_peace" title="Justice of the peace">magistrates</a><br />\nHenry Law, William Metcalfe and Peploe Ward<sup id="cite_ref-FOOTNOTEGoulden200827.E2.80.9328_2-0" class="reference"><a href="#cite_note-FOOTNOTEGoulden200827.E2.80.9328-2">[2]</a></sup></div>\n</td>\n</tr>\n<tr>\n<th scope="row">Date</th>\n<td style="text-align: left;">22 May 1816 \xe2\x80\x93 24&#160;May&#160;1816<span style="display:none">&#160;(<span class="dtend">1816-05-24</span>)</span></td>\n</tr>\n<tr>\n<th scope="row">Location</th>\n<td class="location" style="text-align: left;">Littleport and Ely in <a href="/wiki/Cambridgeshire" title="Cambridgeshire">Cambridgeshire</a> England</td>\n</tr>\n<tr>\n<th scope="row">Also known as</th>\n<td style="text-align: left;"><a href="/wiki/Littleport,_Cambridgeshire" title="Littleport, Cambridgeshire">Littleport</a> riots</td>\n</tr>\n<tr>\n<th scope="row">Participants</th>\n<td class="attendee" style="text-align: left;">Initially 56 citizens of Littleport<br />\nthen spread to <a href="/wiki/Ely,_Cambridgeshire" title="Ely, Cambridgeshire">Ely</a>;<br />\n82 people were arrested</td>\n</tr>\n<tr>\n<th scope="row">Outcome</th>\n<td class="description" style="text-align: left;">One rioter killed by troops;<br />\n23 men and one woman <a href="/wiki/Capital_punishment" title="Capital punishment">condemned</a>; five men <a href="/wiki/Hanging" title="Hanging">hanged</a> on 28&#160;June&#160;1816<span style="display:none">&#160;(<span class="dtend">1816-06-28</span>)</span>;<br />\nsentences on the other 19 varied from 12 months in<br />\nprison to <a href="/wiki/Penal_transportation" title="Penal transportation">transportation</a> for life</td>\n</tr>\n</table>\n<p>The <b>Ely and Littleport riots of 1816</b>, also known as the <b>Ely riots</b> or <b>Littleport riots</b>, occurred between 22 and 24 May 1816 in <a href="/wiki/Littleport,_Cambridgeshire" title="Littleport, Cambridgeshire">Littleport</a>, Cambridgeshire. The riots were caused by high unemployment and rising grain costs, much like the general unrest which spread throughout England following the <a href="/wiki/Napoleonic_Wars" title="Napoleonic Wars">Napoleonic Wars</a>.</p>\n<p>The Littleport riot broke out when a group of residents met at The Globe Inn. Fuelled by alcohol, they left the inn and began intimidating wealthier Littleport residents, demanding money and destroying property. The riot spread to <a href="/wiki/Ely,_Cambridgeshire" title="Ely, Cambridgeshire">Ely</a> where <a href="/wiki/Justice_of_the_peace" title="Justice of the peace">magistrates</a> attempted to calm the protests by ordering poor relief and fixing a minimum wage; see printed bill (reproduced at right). The following day, encouraged by <a href="/wiki/Robert_Jenkinson,_2nd_Earl_of_Liverpool" title="Robert Jenkinson, 2nd Earl of Liverpool">Lord Liverpool</a>\'s government, a <a href="/wiki/Militia_(United_Kingdom)" title="Militia (United Kingdom)">militia</a> of the citizens of Ely, led by <a href="/wiki/Sir_Henry_Dudley,_1st_Baronet" title="Sir Henry Dudley, 1st Baronet">Sir Henry Bate Dudley</a> and backed by the <a href="/wiki/1st_The_Royal_Dragoons" title="1st The Royal Dragoons">1st The Royal Dragoons</a>, rounded up the rioters. In the ensuing altercation at The George and Dragon in Littleport, a trooper was injured, one rioter was killed, and at least one went on the run.</p>\n<p><a href="/wiki/Edward_Christian" title="Edward Christian">Edward Christian</a>, brother of <a href="/wiki/Fletcher_Christian" title="Fletcher Christian">Fletcher Christian</a>, had been appointed Chief Justice of the <a href="/wiki/Isle_of_Ely" title="Isle of Ely">Isle of Ely</a> in 1800 by the <a href="/wiki/Bishop_of_Ely" title="Bishop of Ely">Bishop of Ely</a>. As the Chief Justice, Christian was entitled to try the rioters alone. The government, in this case via the <a href="/wiki/Home_Secretary" title="Home Secretary">Home Secretary</a>, <a href="/wiki/Henry_Addington,_1st_Viscount_Sidmouth" title="Henry Addington, 1st Viscount Sidmouth">Lord Sidmouth</a>, nevertheless appointed a Special Commission, consisting of Justice Abbott and Justice Burrough. The rioters were tried in the <a href="/wiki/Assizes_(England_and_Wales)" class="mw-redirect" title="Assizes (England and Wales)">assizes</a> at Ely during the week commencing June 1816. 23 men and one woman were condemned, of which five were subsequently hanged. General unrest and riots such as that at Littleport may have been a factor in the government passing the <a href="/wiki/Vagrancy_Act_1824" title="Vagrancy Act 1824">Vagrancy Act of 1824</a> and subsequently the <a href="/wiki/Metropolitan_Police_Act_1829" title="Metropolitan Police Act 1829">Metropolitan Police Act of 1829</a>.</p>\n<p></p>\n<div id="toc" class="toc">\n<div class="toctitle">\n<h2>Contents</h2>\n</div>\n<ul>\n<li class="toclevel-1 tocsection-1"><a href="#Background"><span class="tocnumber">1</span> <span class="toctext">Background</span></a></li>\n<li class="toclevel-1 tocsection-2"><a href="#Rioting"><span class="tocnumber">2</span> <span class="toctext">Rioting</span></a>\n<ul>\n<li class="toclevel-2 tocsection-3"><a href="#Preceding_events_in_the_region"><span class="tocnumber">2.1</span> <span class="toctext">Preceding events in the region</span></a></li>\n<li class="toclevel-2 tocsection-4"><a href="#Littleport"><span class="tocnumber">2.2</span> <span class="toctext">Littleport</span></a></li>\n<li class="toclevel-2 tocsection-5"><a href="#Ely"><span class="tocnumber">2.3</span> <span class="toctext">Ely</span></a></li>\n</ul>\n</li>\n<li class="toclevel-1 tocsection-6"><a href="#Restoring_order"><span class="tocnumber">3</span> <span class="toctext">Restoring order</span></a></li>\n<li class="toclevel-1 tocsection-7"><a href="#Trial"><span class="tocnumber">4</span> <span class="toctext">Trial</span></a>\n<ul>\n<li class="toclevel-2 tocsection-8"><a href="#Special_Commission"><span class="tocnumber">4.1</span> <span class="toctext">Special Commission</span></a></li>\n<li class="toclevel-2 tocsection-9"><a href="#Monday"><span class="tocnumber">4.2</span> <span class="toctext">Monday</span></a></li>\n<li class="toclevel-2 tocsection-10"><a href="#Tuesday"><span class="tocnumber">4.3</span> <span class="toctext">Tuesday</span></a></li>\n<li class="toclevel-2 tocsection-11"><a href="#Wednesday"><span class="tocnumber">4.4</span> <span class="toctext">Wednesday</span></a></li>\n<li class="toclevel-2 tocsection-12"><a href="#Thursday"><span class="tocnumber">4.5</span> <span class="toctext">Thursday</span></a></li>\n<li class="toclevel-2 tocsection-13"><a href="#Friday"><span class="tocnumber">4.6</span> <span class="toctext">Friday</span></a></li>\n<li class="toclevel-2 tocsection-14"><a href="#Sentences"><span class="tocnumber">4.7</span> <span class="toctext">Sentences</span></a></li>\n<li class="toclevel-2 tocsection-15"><a href="#Execution"><span class="tocnumber">4.8</span> <span class="toctext">Execution</span></a></li>\n</ul>\n</li>\n<li class="toclevel-1 tocsection-16"><a href="#Aftermath"><span class="tocnumber">5</span> <span class="toctext">Aftermath</span></a></li>\n<li class="toclevel-1 tocsection-17"><a href="#See_also"><span class="tocnumber">6</span> <span class="toctext">See also</span></a></li>\n<li class="toclevel-1 tocsection-18"><a href="#References"><span class="tocnumber">7</span> <span class="toctext">References</span></a></li>\n<li class="toclevel-1 tocsection-19"><a href="#External_links"><span class="tocnumber">8</span> <span class="toctext">External links</span></a></li>\n</ul>\n</div>\n<p></p>\n<h2><span class="mw-headline" id="Background">Background</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=Ely_and_Littleport_riots_of_1816&amp;action=edit&amp;section=1" title="Edit section: Background">edit</a><span class="mw-editsection-bracket">]</span></span></h2>\n<p>In 1815, the government increased taxation<sup id="cite_ref-Cornlaw_3-0" class="reference"><a href="#cite_note-Cornlaw-3">[i]</a></sup> on imported wheat and grain to help pay for the costs of the Napoleonic Wars (1803\xe2\x80\x931815).<sup id="cite_ref-BBCRiotousLittleport_4-0" class="reference"><a href="#cite_note-BBCRiotousLittleport-4">[3]</a></sup> <a href="/wiki/English_Poor_Laws" title="English Poor Laws">Poor laws</a>, such as the <a href="/wiki/Speenhamland_system" title="Speenhamland system">Speenhamland system</a>, were designed to help alleviate financial distress of the poorer communities, but such systems helped to keep wages artificially low as the farmers knew labourers\' wages would be supplemented by the system. Basic commodities, like cereals and bread, became heavily over-priced, creating widespread social unrest. The worst hit were the families of the men returning from the <a href="/wiki/Battle_of_Waterloo" title="Battle of Waterloo">Battle of Waterloo</a> (1815) who arrived home at a time when unemployment was already high.<sup id="cite_ref-BBCRiotousLittleport_4-1" class="reference"><a href="#cite_note-BBCRiotousLittleport-4">[3]</a></sup> One reply to a questionnaire circulated by the <a href="/wiki/Board_of_Agriculture_(1793%E2%80%931822)" title="Board of Agriculture (1793\xe2\x80\x931822)">Board of Agriculture</a> in February, March, and April 1816 reported that "the state of the labouring poor is very deplorable, and arises entirely from the want of employment, which they are willing to seek, but the farmer cannot afford to furnish."<sup id="cite_ref-FOOTNOTEGoulden2008p.21_.28Goulden_did_not_attribute_this_to_Wing.29_5-0" class="reference"><a href="#cite_note-FOOTNOTEGoulden2008p.21_.28Goulden_did_not_attribute_this_to_Wing.29-5">[4]</a></sup><sup id="cite_ref-FOOTNOTEWing181641_6-0" class="reference"><a href="#cite_note-FOOTNOTEWing181641-6">[5]</a></sup></p>\n<p>In early 1816, a <a href="/wiki/Imperial_units#qtr" title="Imperial units">quarter</a> (28 pounds) of wheat cost 52&#160;shillings (\xc2\xa3179),<sup id="cite_ref-8" class="reference"><a href="#cite_note-8">[ii]</a></sup> rising through 76&#160;shillings (\xc2\xa3262) in May to 103&#160;shillings (\xc2\xa3356) in December. Average wages for the period remained static at 8\xe2\x80\x939&#160;shillings (\xc2\xa328\xe2\x80\x93\xc2\xa331), per week.<sup id="cite_ref-FOOTNOTEPugh195345_9-0" class="reference"><a href="#cite_note-FOOTNOTEPugh195345-9">[7]</a></sup> In 1815, a pound of bread was quoted at over 4&#160;shillings (\xc2\xa314) and predicted to rise to over 5&#160;shillings (\xc2\xa317).<sup id="cite_ref-11" class="reference"><a href="#cite_note-11">[iii]</a></sup><sup id="cite_ref-13" class="reference"><a href="#cite_note-13">[v]</a></sup></p>\n<h2><span class="mw-headline" id="Rioting">Rioting</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=Ely_and_Littleport_riots_of_1816&amp;action=edit&amp;section=2" title="Edit section: Rioting">edit</a><span class="mw-editsection-bracket">]</span></span></h2>\n<h3><span class="mw-headline" id="Preceding_events_in_the_region">Preceding events in the region</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=Ely_and_Littleport_riots_of_1816&amp;action=edit&amp;section=3" title="Edit section: Preceding events in the region">edit</a><span class="mw-editsection-bracket">]</span></span></h3>\n<p>There was rioting in the first months of 1816 in <a href="/wiki/West_Suffolk" title="West Suffolk">West Suffolk</a>, Norfolk and Cambridgeshire. On 16 May riots broke out in <a href="/wiki/Bury_St_Edmunds" title="Bury St Edmunds">Bury St Edmunds</a> (known locally as Bury) and <a href="/wiki/Brandon,_Suffolk" title="Brandon, Suffolk">Brandon</a> in West Suffolk and also in <a href="/wiki/Hockwold_cum_Wilton" title="Hockwold cum Wilton">Hockwold</a>, <a href="/wiki/Feltwell" title="Feltwell">Feltwell</a> and <a href="/wiki/Norwich" title="Norwich">Norwich</a> in Norfolk.<sup id="cite_ref-FOOTNOTEGoulden200821.E2.80.9322_14-0" class="reference"><a href="#cite_note-FOOTNOTEGoulden200821.E2.80.9322-14">[9]</a></sup></p>\n<p>On the morning of 20 May, a meeting was held in <a href="/wiki/Southery" title="Southery">Southery</a>, Norfolk. The group, including a Thomas Sindall,<sup id="cite_ref-FOOTNOTEGoulden2008p..C2.A029_Goulden_uses_.27.27Sindal.27.27_here_but_Johnson_.281893.29_p._21_and_Peacock_.281965.29_p._111_both_use_.27.27Sindall.27.27_15-0" class="reference"><a href="#cite_note-FOOTNOTEGoulden2008p..C2.A029_Goulden_uses_.27.27Sindal.27.27_here_but_Johnson_.281893.29_p._21_and_Peacock_.281965.29_p._111_both_use_.27.27Sindall.27.27-15">[10]</a></sup> marched through <a href="/wiki/Denver,_Norfolk" title="Denver, Norfolk">Denver</a> to <a href="/wiki/Downham_Market" title="Downham Market">Downham Market</a> to meet with the magistrates at their weekly meeting at The Crown <a href="/wiki/Pub" title="Pub">public house</a>.<sup id="cite_ref-FOOTNOTEGoulden200822_16-0" class="reference"><a href="#cite_note-FOOTNOTEGoulden200822-16">[11]</a></sup> Sindall was the only person known to have been at both the riots at Downham Market and Littleport.<sup id="cite_ref-FOOTNOTEPeacock196589_17-0" class="reference"><a href="#cite_note-FOOTNOTEPeacock196589-17">[12]</a></sup> He was killed by troopers at Littleport\xe2\x80\x94see below.<sup id="cite_ref-FOOTNOTEPeacock1965111_18-0" class="reference"><a href="#cite_note-FOOTNOTEPeacock1965111-18">[13]</a></sup> The mob of 1,500, mainly men but some women, besieged The Crown until the magistrates agreed to allow a deputation of eight rioters inside to make their pleas: to have work and two-shillings (\xc2\xa37) per day. The magistrates acceded to these demands, but they had already called the <a href="/wiki/Yeomanry" title="Yeomanry">yeoman</a> cavalry from <a href="/wiki/Upwell" title="Upwell">Upwell</a>, who arrived at 5&#160;pm. Backed by the troops, the <a href="/wiki/Riot_Act" title="Riot Act">Riot Act</a> was then read in the market place by Reverend Dering,<sup id="cite_ref-Dering_20-0" class="reference"><a href="#cite_note-Dering-20">[vi]</a></sup> causing further tussles, which subsided after arrests started to be made.<sup id="cite_ref-FOOTNOTEGoulden200822.E2.80.9323_21-0" class="reference"><a href="#cite_note-FOOTNOTEGoulden200822.E2.80.9323-21">[15]</a></sup></p>\n<p>At the Norfolk and Norwich Assizes in August, nine men and six women were sentenced to death. Thirteen of those sentences were commuted, and two of the Downham rioters, Daniel Harwood and Thomas Thody, were hanged on the afternoon of 31 August 1816.<sup id="cite_ref-FOOTNOTEPeacock196593_22-0" class="reference"><a href="#cite_note-FOOTNOTEPeacock196593-22">[16]</a></sup></p>\n<h3><span class="mw-headline" id="Littleport">Littleport</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=Ely_and_Littleport_riots_of_1816&amp;action=edit&amp;section=4" title="Edit section: Littleport">edit</a><span class="mw-editsection-bracket">]</span></span></h3>\n<div class="thumb tright">\n<div class="thumbinner" style="width:222px;"><a href="/wiki/File:Thomas_Rowlandson_English_Travelling.jpg" class="image"><img alt="" src="//upload.wikimedia.org/wikipedia/commons/thumb/d/db/Thomas_Rowlandson_English_Travelling.jpg/220px-Thomas_Rowlandson_English_Travelling.jpg" width="220" height="147" class="thumbimage" srcset="//upload.wikimedia.org/wikipedia/commons/thumb/d/db/Thomas_Rowlandson_English_Travelling.jpg/330px-Thomas_Rowlandson_English_Travelling.jpg 1.5x, //upload.wikimedia.org/wikipedia/commons/thumb/d/db/Thomas_Rowlandson_English_Travelling.jpg/440px-Thomas_Rowlandson_English_Travelling.jpg 2x" data-file-width="640" data-file-height="427" /></a>\n<div class="thumbcaption">\n<div class="magnify"><a href="/wiki/File:Thomas_Rowlandson_English_Travelling.jpg" class="internal" title="Enlarge"></a></div>\nA <a href="/wiki/Chaise" title="Chaise">post-chaise</a>, perhaps similar to this one, carrying Hugh Robert Evans was stopped and robbed by William Beamiss the younger on 22 May 1816.<sup id="cite_ref-FOOTNOTEJohnson189315_23-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189315-23">[17]</a></sup> Beamiss was subsequently indicted for highway robbery.<sup id="cite_ref-FOOTNOTEJohnson189352_24-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189352-24">[18]</a></sup><br />\nPainting by <a href="/wiki/Thomas_Rowlandson" title="Thomas Rowlandson">Thomas Rowlandson</a>, 1785</div>\n</div>\n</div>\n<p>Littleport is a large village in Cambridgeshire with a population in 1811 of 1,847.<sup id="cite_ref-2001census_25-0" class="reference"><a href="#cite_note-2001census-25">[19]</a></sup> It is just under 11 miles (18&#160;km) south-south-west of Downham Market and just over 4 miles (6&#160;km) north-north-east of Ely.</p>\n<p>On 22 May 1816, a group of 56&#160;residents met at The Globe Inn in Littleport to discuss the lack of work and rising grain costs. Fuelled by alcohol, the residents directed their anger at local farmer Henry Martin. He had been overseer of the poor in 1814 and was not well liked by the parishioners.<sup id="cite_ref-FOOTNOTEPeacock196595_26-0" class="reference"><a href="#cite_note-FOOTNOTEPeacock196595-26">[20]</a></sup> One man went to get a horn from Burgess, the <a href="/wiki/Lightermen" class="mw-redirect" title="Lightermen">lighterman</a>,<sup id="cite_ref-28" class="reference"><a href="#cite_note-28">[vii]</a></sup> and started blowing it outside The Globe Inn, gathering hundreds of villagers to join the first group, and the riot commenced.<sup id="cite_ref-FOOTNOTEJohnson189313_29-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189313-29">[22]</a></sup></p>\n<p>The rioters began at Mingey\'s shop, where stones were thrown through the windows, and then they invaded Mr Clarke\'s property and threw his belongings into the street.<sup id="cite_ref-FOOTNOTEJohnson189313_29-1" class="reference"><a href="#cite_note-FOOTNOTEJohnson189313-29">[22]</a></sup> Next, at Josiah Dewey\'s place, the Reverend John Vachell<sup id="cite_ref-CCED100468_30-0" class="reference"><a href="#cite_note-CCED100468-30">[23]</a></sup> and his wife arrived to try to calm the rioters.<sup id="cite_ref-FOOTNOTEJohnson189313_29-2" class="reference"><a href="#cite_note-FOOTNOTEJohnson189313-29">[22]</a></sup> Vachell had been vicar of St George\'s since 1795 and was also a magistrate; he was an unpopular man, as he dealt harshly with even minor offences.<sup id="cite_ref-FOOTNOTEGoulden200824.E2.80.9326_31-0" class="reference"><a href="#cite_note-FOOTNOTEGoulden200824.E2.80.9326-31">[24]</a></sup> He read or tried to read the Riot Act without effect, as the crowd "told him to go home."<sup id="cite_ref-FOOTNOTEJohnson189313_29-3" class="reference"><a href="#cite_note-FOOTNOTEJohnson189313-29">[22]</a></sup></p>\n<p>The rioters next visited the premises of disabled 90-year-old Mr Sindall, throwing his furniture into the street; his housekeeper, Mrs Hutt, was intimidated by a rioter wielding a butcher\'s cleaver.<sup id="cite_ref-FOOTNOTEJohnson189313.E2.80.9314_32-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189313.E2.80.9314-32">[25]</a></sup> After stopping at the place of Mr Little, "a nice old gentleman," who gave the mob \xc2\xa32 (\xc2\xa3138), they continued to Robert Speechly\'s and demolished his furniture.<sup id="cite_ref-FOOTNOTEJohnson189313.E2.80.9314_32-1" class="reference"><a href="#cite_note-FOOTNOTEJohnson189313.E2.80.9314-32">[25]</a></sup> Next they broke into the house of Rebecca Waddelow looking for Harry Martin, her grandson. He had seen them coming and escaped out the back.<sup id="cite_ref-FOOTNOTEWarren19979_33-0" class="reference"><a href="#cite_note-FOOTNOTEWarren19979-33">[26]</a></sup> Rebecca Cutlack was visiting at the time, and they robbed her and removed property worth between \xc2\xa3100 and \xc2\xa3200<sup id="cite_ref-FOOTNOTEWarren19979_33-1" class="reference"><a href="#cite_note-FOOTNOTEWarren19979-33">[26]</a></sup> (\xc2\xa36,903\xe2\x80\x93\xc2\xa313,807).</p>\n<p>At about 11&#160;pm,<sup id="cite_ref-FOOTNOTEGoulden200826_34-0" class="reference"><a href="#cite_note-FOOTNOTEGoulden200826-34">[27]</a></sup> the rioters arrived at the house of the Reverend John Vachell, who, after threatening to shoot anyone who entered his house, was disarmed when three&#160;men rushed him. He fled on foot with his wife and two daughters towards Ely.<sup id="cite_ref-FOOTNOTEBurke181771_35-0" class="reference"><a href="#cite_note-FOOTNOTEBurke181771-35">[28]</a></sup> After Vachell had left, the rioters destroyed his goods and <a href="/wiki/Personal_property" title="Personal property">chattels</a> and stole some of his silverware. Vachell was later to sue the <a href="/wiki/Hundreds_of_Cambridgeshire" title="Hundreds of Cambridgeshire">Hundred of Ely</a> for the damages under the Riot Act.<sup id="cite_ref-FOOTNOTEGoulden200827_36-0" class="reference"><a href="#cite_note-FOOTNOTEGoulden200827-36">[29]</a></sup> He received over \xc2\xa3708<sup id="cite_ref-FOOTNOTEJohnson189382_37-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189382-37">[30]</a></sup> (\xc2\xa348,875), an award which was challenged in the press, as many people complained about the size of the resulting district levies used to pay for it.<sup id="cite_ref-FOOTNOTEPeacock196599_38-0" class="reference"><a href="#cite_note-FOOTNOTEPeacock196599-38">[31]</a></sup> The rioters then stopped a <a href="/wiki/Chaise" title="Chaise">post-chaise</a><sup id="cite_ref-39" class="reference"><a href="#cite_note-39">[32]</a></sup> returning with Hugh Robert Evans senior and Henry Martin from a <a href="/wiki/Turnpike_trusts_in_the_United_Kingdom" class="mw-redirect" title="Turnpike trusts in the United Kingdom">Turnpike Trust</a> meeting in Downham. They robbed Evans of 14&#160;shillings<sup id="cite_ref-FOOTNOTEWarren199740_40-0" class="reference"><a href="#cite_note-FOOTNOTEWarren199740-40">[33]</a></sup> (\xc2\xa348) before allowing them both to proceed.<sup id="cite_ref-FOOTNOTEJohnson189315_23-1" class="reference"><a href="#cite_note-FOOTNOTEJohnson189315-23">[17]</a></sup> On reaching Ely, Evans alerted the magistrates who sent a carriage for Reverend Vachell, which collected him and his family walking towards Ely.<sup id="cite_ref-FOOTNOTEJohnson189315_23-2" class="reference"><a href="#cite_note-FOOTNOTEJohnson189315-23">[17]</a></sup></p>\n<h3><span class="mw-headline" id="Ely">Ely</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=Ely_and_Littleport_riots_of_1816&amp;action=edit&amp;section=5" title="Edit section: Ely">edit</a><span class="mw-editsection-bracket">]</span></span></h3>\n<div class="thumb tleft">\n<div class="thumbinner" style="width:222px;"><a href="/wiki/File:Thomas_Rowlandson_A_review_in_a_market_place.jpg" class="image"><img alt="" src="//upload.wikimedia.org/wikipedia/commons/thumb/7/73/Thomas_Rowlandson_A_review_in_a_market_place.jpg/220px-Thomas_Rowlandson_A_review_in_a_market_place.jpg" width="220" height="142" class="thumbimage" srcset="//upload.wikimedia.org/wikipedia/commons/thumb/7/73/Thomas_Rowlandson_A_review_in_a_market_place.jpg/330px-Thomas_Rowlandson_A_review_in_a_market_place.jpg 1.5x, //upload.wikimedia.org/wikipedia/commons/thumb/7/73/Thomas_Rowlandson_A_review_in_a_market_place.jpg/440px-Thomas_Rowlandson_A_review_in_a_market_place.jpg 2x" data-file-width="640" data-file-height="414" /></a>\n<div class="thumbcaption">\n<div class="magnify"><a href="/wiki/File:Thomas_Rowlandson_A_review_in_a_market_place.jpg" class="internal" title="Enlarge"></a></div>\nA market-place, similar to <a href="/wiki/Ely,_Cambridgeshire" title="Ely, Cambridgeshire">Ely</a> in 1816, where the <a href="/wiki/1st_The_Royal_Dragoons" title="1st The Royal Dragoons">1st The Royal Dragoons</a> paraded on 24 May 1816.<sup id="cite_ref-FOOTNOTEJohnson189319_41-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189319-41">[34]</a></sup><br />\nPainting by <a href="/wiki/Thomas_Rowlandson" title="Thomas Rowlandson">Thomas Rowlandson</a>, 1790</div>\n</div>\n</div>\n<p>Ely, Cambridgeshire, is a city<sup id="cite_ref-44" class="reference"><a href="#cite_note-44">[viii]</a></sup> with an 1811 population of 4,249 people.<sup id="cite_ref-2001census_25-1" class="reference"><a href="#cite_note-2001census-25">[19]</a></sup> The city is nearly 15 miles (24&#160;km) north-north-east of Cambridge and 67 miles (108&#160;km)<sup id="cite_ref-46" class="reference"><a href="#cite_note-46">[ix]</a></sup> north-north-east of London. When Vachell arrived in Ely, he alerted fellow clergymen and magistrates Reverend William Metcalfe<sup id="cite_ref-47" class="reference"><a href="#cite_note-47">[38]</a></sup> and Reverend Henry Law who dispatched Thomas Archer, as a messenger, to Bury, 26 miles (42&#160;km) to the east.<sup id="cite_ref-FOOTNOTEStorey200985_1-1" class="reference"><a href="#cite_note-FOOTNOTEStorey200985-1">[1]</a></sup></p>\n<p>The rioters in Littleton had in the interim stolen a wagon and horses from Henry Tansley and equipped it with fowling guns<sup id="cite_ref-49" class="reference"><a href="#cite_note-49">[x]</a></sup> front and back.<sup id="cite_ref-FOOTNOTEJohnson189316_50-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189316-50">[40]</a></sup> Most of the Littleton mob, armed with guns and pitch-forks, then began the march to Ely, arriving three-quarters of a mile (1.2&#160;km) north of the city between 5&#160;am and 6&#160;am on 23 May.<sup id="cite_ref-FOOTNOTEJohnson189315_23-3" class="reference"><a href="#cite_note-FOOTNOTEJohnson189315-23">[17]</a></sup><sup id="cite_ref-FOOTNOTEWarren199733_51-0" class="reference"><a href="#cite_note-FOOTNOTEWarren199733-51">[41]</a></sup> The Reverend William Metcalfe met them, read the Riot Act, and asked what the mob required.<sup id="cite_ref-FOOTNOTEStorey200985_1-2" class="reference"><a href="#cite_note-FOOTNOTEStorey200985-1">[1]</a></sup> On being told that they wanted "the price of a stone of flour per day" and that "our children are starving, give us a living wage," the Reverend agreed but stated that he would have to converse with the other magistrates. He asked everyone to return to Littleport, but they marched on. Metcalfe implored them to go to the market place and many did go there, where they were joined by Ely citizens.<sup id="cite_ref-FOOTNOTEStorey200985_1-3" class="reference"><a href="#cite_note-FOOTNOTEStorey200985-1">[1]</a></sup> Recognising the needs of the rioters, the Ely magistrates, the Reverends William Metcalfe, Peploe Ward and Henry Law drafted a response, offering poor families two-shillings per head per week and ordering farmers to pay two-shillings (\xc2\xa37) per day wages.<sup id="cite_ref-FOOTNOTEGoulden200827_36-1" class="reference"><a href="#cite_note-FOOTNOTEGoulden200827-36">[29]</a></sup><sup id="cite_ref-FOOTNOTEStorey200985_1-4" class="reference"><a href="#cite_note-FOOTNOTEStorey200985-1">[1]</a></sup> On hearing the proclamation, the mob cheered. The magistrates then "gave the men some beer, told them not to get drunk and tried to persuade them to go home". Some took the advice, whilst others continued the rampage, intimidating shopkeepers, millers and bankers and stealing from some. However, most of the rioters, marching with their wagons and guns, left the city for Littleport before the arrival of the military from Bury.<sup id="cite_ref-FOOTNOTEJohnson189317.E2.80.9318_52-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189317.E2.80.9318-52">[42]</a></sup></p>\n<p>Meanwhile, the magistrates delegated Henry Law to go to London to discuss the matter with Lord Sidmouth, the Home Secretary. On the way, Law stopped at the barracks of the <a href="/wiki/Royston,_Hertfordshire" title="Royston, Hertfordshire">Royston</a> troop of volunteer yeomanry cavalry and requested they go to Ely.<sup id="cite_ref-FOOTNOTEPeacock1965107_53-0" class="reference"><a href="#cite_note-FOOTNOTEPeacock1965107-53">[43]</a></sup> Law was unable to convince Sidmouth of the seriousness of the situation, and Sidmouth asked Reverend Sir Henry Bate Dudley<sup id="cite_ref-FOOTNOTEODNB2010Hannah_Barker:Dudley.2C_Sir_Henry_Bate.2C_baronet__.281745.E2.80.931824.29_54-0" class="reference"><a href="#cite_note-FOOTNOTEODNB2010Hannah_Barker:Dudley.2C_Sir_Henry_Bate.2C_baronet_.281745.E2.80.931824.29-54">[44]</a></sup><sup id="cite_ref-CCED15046_55-0" class="reference"><a href="#cite_note-CCED15046-55">[45]</a></sup> to return with Law and report on the matter.<sup id="cite_ref-FOOTNOTEGoulden200828_56-0" class="reference"><a href="#cite_note-FOOTNOTEGoulden200828-56">[46]</a></sup></p>\n<h2><span class="mw-headline" id="Restoring_order">Restoring order</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=Ely_and_Littleport_riots_of_1816&amp;action=edit&amp;section=6" title="Edit section: Restoring order">edit</a><span class="mw-editsection-bracket">]</span></span></h2>\n<div class="thumb tright">\n<div class="thumbinner" style="width:172px;"><a href="/wiki/File:Gainsborough_The-Rev.-Sir-Henry-Bate-Dudley.jpg" class="image"><img alt="" src="//upload.wikimedia.org/wikipedia/commons/thumb/5/59/Gainsborough_The-Rev.-Sir-Henry-Bate-Dudley.jpg/170px-Gainsborough_The-Rev.-Sir-Henry-Bate-Dudley.jpg" width="170" height="205" class="thumbimage" srcset="//upload.wikimedia.org/wikipedia/commons/thumb/5/59/Gainsborough_The-Rev.-Sir-Henry-Bate-Dudley.jpg/255px-Gainsborough_The-Rev.-Sir-Henry-Bate-Dudley.jpg 1.5x, //upload.wikimedia.org/wikipedia/commons/thumb/5/59/Gainsborough_The-Rev.-Sir-Henry-Bate-Dudley.jpg/340px-Gainsborough_The-Rev.-Sir-Henry-Bate-Dudley.jpg 2x" data-file-width="838" data-file-height="1012" /></a>\n<div class="thumbcaption">\n<div class="magnify"><a href="/wiki/File:Gainsborough_The-Rev.-Sir-Henry-Bate-Dudley.jpg" class="internal" title="Enlarge"></a></div>\n<a href="/wiki/Sir_Henry_Dudley,_1st_Baronet" title="Sir Henry Dudley, 1st Baronet">Sir Henry Bate-Dudley</a>, <a href="/wiki/Baronet" title="Baronet">bart</a>., "the fighting parson",<sup id="cite_ref-57" class="reference"><a href="#cite_note-57">[47]</a></sup> led the troops rounding up the rioters in <a href="/wiki/Littleport,_Cambridgeshire" title="Littleport, Cambridgeshire">Littleport</a> on 24 May 1816.<sup id="cite_ref-FOOTNOTEJohnson189319_41-1" class="reference"><a href="#cite_note-FOOTNOTEJohnson189319-41">[34]</a></sup><br />\nPainting <i>c</i>.&#160;1780 by <a href="/wiki/Thomas_Gainsborough" title="Thomas Gainsborough">Thomas Gainsborough</a></div>\n</div>\n</div>\n<p>A detachment of 18 men of the 1st The Royal Dragoons, commanded by Captain Methuen,<sup id="cite_ref-FOOTNOTEPeacock1965108_58-0" class="reference"><a href="#cite_note-FOOTNOTEPeacock1965108-58">[48]</a></sup> arrived in Ely from Bury on 23 May in the late afternoon. They marched through the streets as a show of force, remaining all night.<sup id="cite_ref-FOOTNOTEJohnson189319_41-2" class="reference"><a href="#cite_note-FOOTNOTEJohnson189319-41">[34]</a></sup> The following afternoon, 24 May, the troops marched on Littleport, led by Sir Henry Bate Dudley and John Bacon, a <a href="/wiki/Bow_Street_Runners" title="Bow Street Runners">Bow Street constable</a>. They were followed by the Royston troop of volunteer yeomanry cavalry summoned earlier by Henry Law,<sup id="cite_ref-FOOTNOTEPeacock1965107_53-1" class="reference"><a href="#cite_note-FOOTNOTEPeacock1965107-53">[43]</a></sup> and a <a href="/wiki/Militia_(United_Kingdom)" title="Militia (United Kingdom)">militia</a> of gentlemen and inhabitants of Ely.<sup id="cite_ref-FOOTNOTEStorey200985_1-5" class="reference"><a href="#cite_note-FOOTNOTEStorey200985-1">[1]</a></sup></p>\n<p>Before arrival at the Ely Road, a small detachment of troops were ordered across the Hemp Field to enter the village from the east.<sup id="cite_ref-59" class="reference"><a href="#cite_note-59">[xi]</a></sup> The larger group then charged at a hard gallop down the Mill Street incline through to Main Street.<sup id="cite_ref-60" class="reference"><a href="#cite_note-60">[xii]</a></sup><sup id="cite_ref-FOOTNOTEPugh195345_9-1" class="reference"><a href="#cite_note-FOOTNOTEPugh195345-9">[7]</a></sup><sup id="cite_ref-FOOTNOTEBurke181771_35-1" class="reference"><a href="#cite_note-FOOTNOTEBurke181771-35">[28]</a></sup><sup id="cite_ref-FOOTNOTEJohnson189319.E2.80.9320_61-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189319.E2.80.9320-61">[49]</a></sup> The rioters were found making a stand in The George and Dragon near the west end of Station Road. The militia were called to the front when the rioters would not come out after being ordered to by Bate Dudley. Thomas South, shooting from a window, hit trooper Wallace in the forearm. The militia got the rioters out of the public house and assembled them in the street, surrounded by the troopers. Thomas Sindall attempted to take a musket from trooper William Porter but was not successful. Sindall ran away and when he did not stop after being called on to do so by Porter, he was shot through the back of the head. Thomas Sindall was killed; he was the only person known to be at both Downham Market and Littleport.<sup id="cite_ref-FOOTNOTEGoulden200829_62-0" class="reference"><a href="#cite_note-FOOTNOTEGoulden200829-62">[50]</a></sup> The result of this shooting was to subdue the rest of the rioters.<sup id="cite_ref-FOOTNOTEJohnson189321_63-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189321-63">[51]</a></sup> Those captured were taken to Ely gaol and the rest of the rioters were rounded up.</p>\n<p>The home secretary, Lord Sidmouth, had dispatched three&#160;troops of cavalry (100 men), two six\xe2\x80\x93pounder cannons and three&#160;companies of the <a href="/wiki/69th_(South_Lincolnshire)_Regiment_of_Foot" title="69th (South Lincolnshire) Regiment of Foot">69th (South Lincolnshire) Regiment of Foot</a> under <a href="/wiki/John_Byng,_1st_Earl_of_Strafford" title="John Byng, 1st Earl of Strafford">Major General Byng</a> to help capture the leading rioters.<sup id="cite_ref-FOOTNOTEHenry200214_64-0" class="reference"><a href="#cite_note-FOOTNOTEHenry200214-64">[52]</a></sup><sup id="cite_ref-FOOTNOTEPeacock1965114_65-0" class="reference"><a href="#cite_note-FOOTNOTEPeacock1965114-65">[53]</a></sup><sup id="cite_ref-FOOTNOTEJohnson189322_66-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189322-66">[54]</a></sup></p>\n<p>Two rioters were hidden in <a href="/wiki/Lakenheath" title="Lakenheath">Lakenheath</a> by a labourer who eventually betrayed them for \xc2\xa35 each (\xc2\xa3345).<sup id="cite_ref-FOOTNOTEJohnson189322_66-1" class="reference"><a href="#cite_note-FOOTNOTEJohnson189322-66">[54]</a></sup> One rioter, William Gotobed, a bricklayer, escaped and was eventually pardoned a few years later.<sup id="cite_ref-FOOTNOTEPeacock196550_67-0" class="reference"><a href="#cite_note-FOOTNOTEPeacock196550-67">[55]</a></sup> He returned to Littleport after seven years and then went to America.<sup id="cite_ref-FOOTNOTEJohnson189322.E2.80.9323_68-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189322.E2.80.9323-68">[56]</a></sup> The rioting spread to nearby areas such as <a href="/wiki/Little_Downham" title="Little Downham">Little Downham</a>, Cambridgeshire, although such areas were not as badly affected. It took until 10 June before the areas were finally cleared of trouble and all of the rioters had been captured.<sup id="cite_ref-FOOTNOTEJohnson189323_69-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189323-69">[57]</a></sup></p>\n<h2><span class="mw-headline" id="Trial">Trial</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=Ely_and_Littleport_riots_of_1816&amp;action=edit&amp;section=7" title="Edit section: Trial">edit</a><span class="mw-editsection-bracket">]</span></span></h2>\n<p>The assizes for the 82 persons, 73 of whom were in prison and nine on bail, lasted from Monday 17 June 1816 through to the following Saturday.<sup id="cite_ref-FOOTNOTEWarren1997pp..C2.A01.2C_5_.26_63_70-0" class="reference"><a href="#cite_note-FOOTNOTEWarren1997pp..C2.A01.2C_5_.26_63-70">[58]</a></sup></p>\n<h3><span class="mw-headline" id="Special_Commission">Special Commission</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=Ely_and_Littleport_riots_of_1816&amp;action=edit&amp;section=8" title="Edit section: Special Commission">edit</a><span class="mw-editsection-bracket">]</span></span></h3>\n<p>Since 970 AD, and until 1837<sup id="cite_ref-71" class="reference"><a href="#cite_note-71">[xiii]</a></sup> the Bishop of Ely retained exclusive jurisdiction in civil and criminal matters,<sup id="cite_ref-FOOTNOTELewis1831131.E2.80.93132_72-0" class="reference"><a href="#cite_note-FOOTNOTELewis1831131.E2.80.93132-72">[59]</a></sup> and was also keeper of the records (<a href="/wiki/Custos_rotulorum" title="Custos rotulorum">Custos rotulorum</a>).<sup id="cite_ref-73" class="reference"><a href="#cite_note-73">[60]</a></sup> As part of this right, the Bishop appointed a Chief Justice of the Isle of Ely; Edward Christian had held the post since 1800.<sup id="cite_ref-FOOTNOTEODNB2010Michael_H._Hoffheimer:Edward_.28bap._1758.2C_d._1823.29_74-0" class="reference"><a href="#cite_note-FOOTNOTEODNB2010Michael_H._Hoffheimer:Edward_.28bap._1758.2C_d._1823.29-74">[61]</a></sup> In these special assizes, the crown, via Lord Sidmouth, created a Special Commission.<sup id="cite_ref-75" class="reference"><a href="#cite_note-75">[62]</a></sup> Sidmouth appointed two judges, Mr Justice Abbott<sup id="cite_ref-FOOTNOTEODNB2010Michael_Lobban:Abbott.2C_Charles_.281762.E2.80.931832.29_76-0" class="reference"><a href="#cite_note-FOOTNOTEODNB2010Michael_Lobban:Abbott.2C_Charles_.281762.E2.80.931832.29-76">[63]</a></sup> and Mr Justice Burrough<sup id="cite_ref-FOOTNOTEODNB2010Michael_Lobban:Burrough.2C_Sir_James_.281749.E2.80.931837.29_77-0" class="reference"><a href="#cite_note-FOOTNOTEODNB2010Michael_Lobban:Burrough.2C_Sir_James_.281749.E2.80.931837.29-77">[64]</a></sup> to preside over it. Christian, nevertheless, felt he should attend and indeed was in attendance throughout.<sup id="cite_ref-FOOTNOTEWarren19971_78-0" class="reference"><a href="#cite_note-FOOTNOTEWarren19971-78">[65]</a></sup> After the trial Christian said, "It was suggested to me in London, ... that it would be more conducive to the great object of the Commission, ... if I declined my rotation of duty, and left the trial of all the prisoners to them [the appointed commissioners]."<sup id="cite_ref-FOOTNOTEJohnson189375_79-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189375-79">[66]</a></sup></p>\n<h3><span class="mw-headline" id="Monday">Monday</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=Ely_and_Littleport_riots_of_1816&amp;action=edit&amp;section=9" title="Edit section: Monday">edit</a><span class="mw-editsection-bracket">]</span></span></h3>\n<p>Shortly after 10&#160;am, the three judges went to the courthouse, in the market place at that time, and the Special Commission was read.<sup id="cite_ref-FOOTNOTEJohnson189325_80-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189325-80">[67]</a></sup> The judges then breakfasted at the bishop\'s palace, after which a service was held in the cathedral and attended by 50 of the principal inhabitants of Ely. The sermon, preached by Henry Bate Dudley, was "that the law is not made for a righteous man, but for the lawless and disobedient".<sup id="cite_ref-82" class="reference"><a href="#cite_note-82">[xiv]</a></sup><sup id="cite_ref-FOOTNOTEJohnson189325_80-1" class="reference"><a href="#cite_note-FOOTNOTEJohnson189325-80">[67]</a></sup></p>\n<p>After the service, around 1&#160;pm, the court reconvened; the Grand Jury was sworn in, and Mr W. Dunn Gardner elected the foreman.<sup id="cite_ref-FOOTNOTEJohnson189365_83-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189365-83">[69]</a></sup> Mr Gurney, Mr Bolland and Mr Richardson, were counsel for the crown, led by Mr William Hobhouse,<sup id="cite_ref-FOOTNOTEWarren1997pp..C2.A07.2C_15.E2.80.9316_84-0" class="reference"><a href="#cite_note-FOOTNOTEWarren1997pp..C2.A07.2C_15.E2.80.9316-84">[70]</a></sup><sup id="cite_ref-FOOTNOTEGoulden200829_62-1" class="reference"><a href="#cite_note-FOOTNOTEGoulden200829-62">[50]</a></sup> treasury solicitor.<sup id="cite_ref-CCRO_85-0" class="reference"><a href="#cite_note-CCRO-85">[71]</a></sup> For the defence, Mr Hunt was counsel for Jefferson, Wyebrow, Harley, Pricke, Cooper, Freeman and Jessop;<sup id="cite_ref-FOOTNOTEWarren1997p..C2.A012_86-0" class="reference"><a href="#cite_note-FOOTNOTEWarren1997p..C2.A012-86">[72]</a></sup> Mr Hart was counsel for John Easey, Joseph Easey, Benton, Layton, Atkin, Hobbs and another.<sup id="cite_ref-FOOTNOTEWarren1997pp..C2.A044_87-0" class="reference"><a href="#cite_note-FOOTNOTEWarren1997pp..C2.A044-87">[73]</a></sup></p>\n<p>Mr Justice Abbott addressed the court:</p>\n<blockquote class="templatequote">\n<p><i>Gentlemen of the Grand Jury</i>,\xe2\x80\x94You have been called together at this unusual period, and with the present solemnities, in consequence of some very daring acts of outrage committed by various misguided individuals in this town and its immediate neighbourhood, which must still be fresh in your recollection. In contemplating the nature of these atrocities, it is impossible to consider without commendation, the conductors of those prompt and efficacious measures by which, after it had domineered for several days together, the spirit of tumult and devastation was finally subdued.<sup id="cite_ref-FOOTNOTEJohnson189325.E2.80.9330_88-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189325.E2.80.9330-88">[74]</a></sup></p>\n</blockquote>\n<p>He then went on to direct the jury at length, commenting at one point that these disturbances "seem to have been the necessity of an advance in the wages of husbandry; but the circumstances of some among the offenders do not correspond with the supposition of such an object".<sup id="cite_ref-FOOTNOTEWarren19972_89-0" class="reference"><a href="#cite_note-FOOTNOTEWarren19972-89">[75]</a></sup> He was probably referring to John Dennis, <a href="/wiki/Victualler" title="Victualler">licensed victualler</a> of Littleport.<sup id="cite_ref-FOOTNOTEWarren199728_90-0" class="reference"><a href="#cite_note-FOOTNOTEWarren199728-90">[76]</a></sup> He outlined the three capital offences that would be presented, namely burglary, robbery from the person and stealing in a dwelling house. He defined burglary as breaking into a house at night with intent, robbery from the person as extortion by violence or threat thereof, and stealing in a dwelling house as an actual act of stealing, not just the threat. He reminded the jury that when offences are carried out by a mob, it is the whole mob that is guilty, not just the person doing the deed.<sup id="cite_ref-FOOTNOTEWarren19973.E2.80.934_91-0" class="reference"><a href="#cite_note-FOOTNOTEWarren19973.E2.80.934-91">[77]</a></sup> He closed:</p>\n<blockquote class="templatequote">\n<p>It is of the highest importance to the peace and safety, not only of this isle, but of the surrounding country, that all who are present on this solemn inquiry, and all who read the account of its proceedings (and there are few parts of the kingdom in which it will not be read) may be convinced by the awful lesson which may here be taught, that whatever wild or chimerical notions may prevail of the power of an armed multitude, the law is too strong for its assailants; and that, however triumphant or destructive their sway for a few days, those who defy the law, will ultimately be compelled to submit either to its justice or its mercy.<sup id="cite_ref-FOOTNOTEWarren19975_92-0" class="reference"><a href="#cite_note-FOOTNOTEWarren19975-92">[78]</a></sup></p>\n</blockquote>\n<p>The court adjourned until the following day.<sup id="cite_ref-FOOTNOTEWarren19973.E2.80.935_93-0" class="reference"><a href="#cite_note-FOOTNOTEWarren19973.E2.80.935-93">[79]</a></sup></p>\n<h3><span class="mw-headline" id="Tuesday">Tuesday</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=Ely_and_Littleport_riots_of_1816&amp;action=edit&amp;section=10" title="Edit section: Tuesday">edit</a><span class="mw-editsection-bracket">]</span></span></h3>\n<p>On Tuesday 18 June the court opened at 9&#160;am; 12 defendants were charged with breaking into the house of Rebecca Waddelow in Littleport and stealing personal property and money. After hearing evidence, the judges held a discussion. Mr Justice Abbott informed the jury that a mistake had been made in the indictment, directing the jury to acquit. All 12 defendants were pronounced not guilty.<sup id="cite_ref-FOOTNOTEJohnson189330.C2.A039_94-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189330.C2.A039-94">[80]</a></sup></p>\n<p>Seven defendants were then indicted for breaking into the house of Josiah Dewey in Littleport, stealing money and property and assaulting Dewey.<sup id="cite_ref-FOOTNOTEJohnson189339_95-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189339-95">[81]</a></sup> Mr Gurney addressed the jury.<sup id="cite_ref-FOOTNOTEJohnson189339_95-1" class="reference"><a href="#cite_note-FOOTNOTEJohnson189339-95">[81]</a></sup> Three witnesses testified. The jury recorded a verdict of guilty against Chevall,<sup id="cite_ref-Chevall_100-0" class="reference"><a href="#cite_note-Chevall-100">[xv]</a></sup> Easey, Jessop, South and Benton; Dann and Torrington were acquitted.<sup id="cite_ref-FOOTNOTEJohnson189339.E2.80.9340_101-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189339.E2.80.9340-101">[86]</a></sup> The court adjourned at 5&#160;pm.</p>\n<h3><span class="mw-headline" id="Wednesday">Wednesday</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=Ely_and_Littleport_riots_of_1816&amp;action=edit&amp;section=11" title="Edit section: Wednesday">edit</a><span class="mw-editsection-bracket">]</span></span></h3>\n<p>On Wednesday 19 June the court opened at 9&#160;am; the indictment on three of the defendants was read for forcibly entering the house of Robert Speechly in Littleport and stealing personal property.<sup id="cite_ref-FOOTNOTEJohnson189340_102-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189340-102">[87]</a></sup> Mr Gurney addressed the jury, who later pronounced all defendants guilty.<sup id="cite_ref-FOOTNOTEJohnson189341_103-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189341-103">[88]</a></sup></p>\n<p>Five defendants were then indicted for forcing their way in the house of Rev. John Vachell in Littleport, threatening him and stealing \xc2\xa32.<sup id="cite_ref-FOOTNOTEJohnson189341.E2.80.9342_104-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189341.E2.80.9342-104">[89]</a></sup> Mr Gurney addressed the jury.<sup id="cite_ref-FOOTNOTEJohnson189342_105-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189342-105">[90]</a></sup> Two witnesses testified. A verdict of guilty was returned against Harley and Newell whilst Warner and Stibbard were acquitted.<sup id="cite_ref-FOOTNOTEJohnson189343_106-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189343-106">[91]</a></sup></p>\n<p>Five defendants were then indicted for stealing \xc2\xa350 from the house of Robert Edwards in Ely.<sup id="cite_ref-FOOTNOTEJohnson189343_106-1" class="reference"><a href="#cite_note-FOOTNOTEJohnson189343-106">[91]</a></sup> Mr Gurney addressed the jury at length describing the events occurring in the town of Ely on the morning of the 23 May 1816.<sup id="cite_ref-FOOTNOTEJohnson189343.E2.80.9345_107-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189343.E2.80.9345-107">[92]</a></sup> Eight witnesses were called. The jury returned guilty verdicts against Dennis, Jefferson, and Rutter. Hopkin and Cammell were given not guilty verdicts.<sup id="cite_ref-FOOTNOTEJohnson189344.E2.80.9350_108-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189344.E2.80.9350-108">[93]</a></sup></p>\n<h3><span class="mw-headline" id="Thursday">Thursday</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=Ely_and_Littleport_riots_of_1816&amp;action=edit&amp;section=12" title="Edit section: Thursday">edit</a><span class="mw-editsection-bracket">]</span></span></h3>\n<p>On Thursday morning 20 June two defendants were indicted for stealing silverware from the home of Rev. J. Vachell in Littleport and one was charged with receiving the stolen goods.<sup id="cite_ref-FOOTNOTEJohnson189350_109-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189350-109">[94]</a></sup> Four witnesses called were Elizabeth Carter following which Mr Justice Burrough summed up.<sup id="cite_ref-FOOTNOTEJohnson189351.E2.80.9352_110-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189351.E2.80.9352-110">[95]</a></sup> Lavender was pronounced guilty of the theft and Beamiss and Butcher received not guilty verdicts.<sup id="cite_ref-FOOTNOTEJohnson189352_24-1" class="reference"><a href="#cite_note-FOOTNOTEJohnson189352-24">[18]</a></sup></p>\n<p>John Gaultrip was next, also indicted for stealing spoons at the vicarage. As the evidence was contradictory, Gaultrip was acquitted by the jury.<sup id="cite_ref-FOOTNOTEJohnson189352_24-2" class="reference"><a href="#cite_note-FOOTNOTEJohnson189352-24">[18]</a></sup></p>\n<p>Next at the bar was William Beamiss indicted for highway robbery of Hugh Robert Evans, of Ely.<sup id="cite_ref-FOOTNOTEJohnson189352.E2.80.9353_111-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189352.E2.80.9353-111">[96]</a></sup> Two witness were called.<sup id="cite_ref-FOOTNOTEJohnson189353_112-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189353-112">[97]</a></sup> Mr Justice Abbott summed up and the jury returned a verdict of guilty on the prisoner Beamiss.<sup id="cite_ref-FOOTNOTEJohnson189354_113-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189354-113">[98]</a></sup></p>\n<p>Then nine defendants were charged with threatening W. Cooper of Ely and stealing from him.<sup id="cite_ref-FOOTNOTEJohnson189354_113-1" class="reference"><a href="#cite_note-FOOTNOTEJohnson189354-113">[98]</a></sup> Mr Gurney addressed the jury, explaining the circumstances of the disturbances carried out by the mob in Ely. Nine witnesses were called.<sup id="cite_ref-FOOTNOTEJohnson189354.E2.80.9358_114-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189354.E2.80.9358-114">[99]</a></sup> Mr Justice Abbott spoke to the jury for a long time. The jury retired for fifteen minutes. On returning, it pronounced as guilty Dennis, Jessop, Atkin, Layton, Hobbs, Pricke, Cooper, and Jefferson. It acquitted Freeman.<sup id="cite_ref-FOOTNOTEJohnson189359_115-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189359-115">[100]</a></sup></p>\n<p>Six defendants were then charged with stealing from George Stevens in Ely.<sup id="cite_ref-FOOTNOTEJohnson189359_115-1" class="reference"><a href="#cite_note-FOOTNOTEJohnson189359-115">[100]</a></sup> Three witness were examined following which Dennis, Layton, Atkin, and Cammell were all pronounced guilty by the jury.<sup id="cite_ref-FOOTNOTEJohnson189359_115-2" class="reference"><a href="#cite_note-FOOTNOTEJohnson189359-115">[100]</a></sup> Capital convictions against Aaron Chevall<sup id="cite_ref-Chevall_100-1" class="reference"><a href="#cite_note-Chevall-100">[xv]</a></sup> and William Beamiss, for robbing Henry Tansley of two \xc2\xa31 notes, were also returned by the jury.<sup id="cite_ref-FOOTNOTEJohnson189359_115-3" class="reference"><a href="#cite_note-FOOTNOTEJohnson189359-115">[100]</a></sup> The court adjourned at 6&#160;pm.<sup id="cite_ref-FOOTNOTEWarren199749_116-0" class="reference"><a href="#cite_note-FOOTNOTEWarren199749-116">[101]</a></sup></p>\n<h3><span class="mw-headline" id="Friday">Friday</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=Ely_and_Littleport_riots_of_1816&amp;action=edit&amp;section=13" title="Edit section: Friday">edit</a><span class="mw-editsection-bracket">]</span></span></h3>\n<p>On Friday morning 21 June, seven prisoners were brought to the bar on a similar charge as on Tuesday (breaking into the house of Rebecca Waddelow in Littleport and stealing personal property and money).<sup id="cite_ref-FOOTNOTEJohnson189360_117-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189360-117">[102]</a></sup> The prisoners all pleaded not guilty.<sup id="cite_ref-FOOTNOTEWarren199751_118-0" class="reference"><a href="#cite_note-FOOTNOTEWarren199751-118">[103]</a></sup> Mr Gurney addressed the jury explaining that these were the same persons indicted on Tuesday for the same offence. He explained to the jury that the earlier indictment incorrectly described the house to be the property of Rebecca Waddelow, leading to the prisoners\' acquittal. He also explained that in the original indictment, other persons had been charged, but as these other persons had since been convicted of capital offences, they had been removed from the indictment.<sup id="cite_ref-FOOTNOTEWarren199751.E2.80.9352_119-0" class="reference"><a href="#cite_note-FOOTNOTEWarren199751.E2.80.9352-119">[104]</a></sup> The witness were all brought forward again and made their statements. They said much the same as previously, though this took much of the day.<sup id="cite_ref-FOOTNOTEWarren199752_120-0" class="reference"><a href="#cite_note-FOOTNOTEWarren199752-120">[105]</a></sup> In summing up, Mr Abbott went on a length closing with "the duty which it belonged to them [the jury] to execute \xe2\x80\x93 confident that, from experience which the court had now of them, their verdict would be such as good sense would dictate, and the public justice of the country require".<sup id="cite_ref-FOOTNOTEWarren199752.E2.80.9357_121-0" class="reference"><a href="#cite_note-FOOTNOTEWarren199752.E2.80.9357-121">[106]</a></sup> It took the jury five&#160;minutes to find Walker, Butcher and Crow guilty and Nicholas, Wilson and Jefferson not guilty.<sup id="cite_ref-FOOTNOTEWarren199757_122-0" class="reference"><a href="#cite_note-FOOTNOTEWarren199757-122">[107]</a></sup></p>\n<p>Brought to the bar next was Henry Benson, a farmer who was out on bail, charged with inciting to riot.<sup id="cite_ref-FOOTNOTEWarren199749_116-1" class="reference"><a href="#cite_note-FOOTNOTEWarren199749-116">[101]</a></sup><sup id="cite_ref-FOOTNOTEJohnson189360_117-1" class="reference"><a href="#cite_note-FOOTNOTEJohnson189360-117">[102]</a></sup> Benson was held in surety for \xc2\xa3400 plus two other sureties for \xc2\xa3200 each. He was to appear for trial at the next assizes.<sup id="cite_ref-124" class="reference"><a href="#cite_note-124">[xvi]</a></sup><sup id="cite_ref-FOOTNOTEWarren199757_122-1" class="reference"><a href="#cite_note-FOOTNOTEWarren199757-122">[107]</a></sup> Richard Cooper the elder and Richard Cooper the younger were also bound over to the next assizes.<sup id="cite_ref-FOOTNOTEWarren199757_122-2" class="reference"><a href="#cite_note-FOOTNOTEWarren199757-122">[107]</a></sup> Father and son, William Beamiss the elder and the younger, were then brought in and charged with assaulting and stealing from Robert Cheesewright the younger, of Littleport, in the Isle of Ely.<sup id="cite_ref-FOOTNOTEWarren199758_125-0" class="reference"><a href="#cite_note-FOOTNOTEWarren199758-125">[109]</a></sup> Mr Gurney addressed the jury. Mr Burrough summed up and the jury gave their guilty verdict to both prisoners shortly after.<sup id="cite_ref-FOOTNOTEWarren199758.E2.80.9359_126-0" class="reference"><a href="#cite_note-FOOTNOTEWarren199758.E2.80.9359-126">[110]</a></sup></p>\n<p>Next, between 20 and 30 prisoners were brought to the bar and "indicted for having committed various felonies and misdemeanour\'s at Littleport on 22, 23, and 24 of May last".<sup id="cite_ref-FOOTNOTEJohnson189362_127-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189362-127">[111]</a></sup> A few were put to the next assizes and the remainder chose to be tried immediately. Mr Gurney addressed the jury explaining that his Majesty\'s government was keen not to put to the jury more cases than was necessary. If the prisoners now at the bar offered a small surety and remained on good behaviour, there would be no more said.<sup id="cite_ref-FOOTNOTEWarren199760_128-0" class="reference"><a href="#cite_note-FOOTNOTEWarren199760-128">[112]</a></sup> Mr Abbott spoke to the prisoners at length, finally demanding that each of them offer surety of \xc2\xa350 and find two more sureties each of \xc2\xa310 or one more each of \xc2\xa320.<sup id="cite_ref-FOOTNOTEPeacock1965126_123-1" class="reference"><a href="#cite_note-FOOTNOTEPeacock1965126-123">[108]</a></sup> Mr Abbott then thanked the jury for their services.<sup id="cite_ref-FOOTNOTEWarren199762_129-0" class="reference"><a href="#cite_note-FOOTNOTEWarren199762-129">[113]</a></sup> The court adjourned.<sup id="cite_ref-FOOTNOTEWarren199762_129-1" class="reference"><a href="#cite_note-FOOTNOTEWarren199762-129">[113]</a></sup></p>\n<h3><span class="mw-headline" id="Sentences">Sentences</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=Ely_and_Littleport_riots_of_1816&amp;action=edit&amp;section=14" title="Edit section: Sentences">edit</a><span class="mw-editsection-bracket">]</span></span></h3>\n<dl>\n<dt>Saturday</dt>\n</dl>\n<p>On Saturday 22 June 1816 six&#160;men were acquitted; 10 were <a href="/wiki/Conditional_discharge" class="mw-redirect" title="Conditional discharge">discharged</a>; 36 were bailed; 23 men and one woman were <a href="/wiki/Capital_punishment" title="Capital punishment">condemned</a> of whom 18 men and the woman, Sarah Hobbs,<sup id="cite_ref-FOOTNOTEBlakeman200115_130-0" class="reference"><a href="#cite_note-FOOTNOTEBlakeman200115-130">[114]</a></sup> had their sentences <a href="/wiki/Commutation_of_sentence" class="mw-redirect" title="Commutation of sentence">commuted</a>.<sup id="cite_ref-132" class="reference"><a href="#cite_note-132">[xvii]</a></sup><sup id="cite_ref-FOOTNOTEWatson1827401_131-1" class="reference"><a href="#cite_note-FOOTNOTEWatson1827401-131">[115]</a></sup> Of the 19 persons commuted, nine were sentenced to <a href="/wiki/Penal_transportation" title="Penal transportation">penal transportation</a>.<sup id="cite_ref-133" class="reference"><a href="#cite_note-133">[xviii]</a></sup> Seven of those nine sailed on the <a href="/wiki/Convict_ship" title="Convict ship">convict ship</a> <a href="/wiki/Sir_William_Bensley_(1802_Indiaman)" title="Sir William Bensley (1802 Indiaman)"><i>Sir William Bensley</i></a>, which departed for <a href="/wiki/New_South_Wales" title="New South Wales">New South Wales</a> on 9 October 1816,<sup id="cite_ref-SirWilliamBensley_134-0" class="reference"><a href="#cite_note-SirWilliamBensley-134">[116]</a></sup> captained by Lew E Williams.<sup id="cite_ref-135" class="reference"><a href="#cite_note-135">[117]</a></sup> Records exist for the following known to have sailed: James Newell, Richard Jessop, John Jefferson, Joseph Easey, Aaron Chevell,<sup id="cite_ref-Chevall_100-2" class="reference"><a href="#cite_note-Chevall-100">[xv]</a></sup> all of Littleport and all transported for life; Richard Rutter of Littleport for 14&#160;years; John Easey of Littleport for seven&#160;years.<sup id="cite_ref-SirWilliamBensley_134-1" class="reference"><a href="#cite_note-SirWilliamBensley-134">[116]</a></sup> Two others were also sentenced to transportation for seven&#160;years, Mark Benton and John Walker, both of Littleport.<sup id="cite_ref-FOOTNOTEWarren199768_98-1" class="reference"><a href="#cite_note-FOOTNOTEWarren199768-98">[84]</a></sup> 10, including Sarah Hobbs, were committed to Ely gaol for 12 months; five were from Littleport and five, including Sarah Hobbs were from Ely.<sup id="cite_ref-FOOTNOTEWarren199768_98-2" class="reference"><a href="#cite_note-FOOTNOTEWarren199768-98">[84]</a></sup></p>\n<p>The five&#160;condemned without commutation were all from Littleport. They were William Beamiss&#160;(42) the elder, shoemaker, for stealing from the persons of Henry Tansley and Robert Cheeseright, both of Littleport; George Crow&#160;(23), labourer, for stealing in the dwelling-house of Rebecca Waddelow and Henry Martin, both of Littleport; John Dennis&#160;(32), a publican, for stealing from the persons of William Cooper, Robert Edwards, and George Stevens, all of Ely; Isaac Harley junior&#160;(33), labourer, for stealing from the person of the Reverend John Vachell, of Littleport; and Thomas South the younger&#160;(22), labourer, for stealing in the dwelling-house of Josiah Dewey and Robert Speechley, both of Littleport.<sup id="cite_ref-FOOTNOTEWarren199767_136-0" class="reference"><a href="#cite_note-FOOTNOTEWarren199767-136">[118]</a></sup><sup id="cite_ref-137" class="reference"><a href="#cite_note-137">[119]</a></sup><sup id="cite_ref-Age_140-0" class="reference"><a href="#cite_note-Age-140">[xix]</a></sup><sup id="cite_ref-Occupation_142-0" class="reference"><a href="#cite_note-Occupation-142">[xx]</a></sup></p>\n<table class="wikitable sortable collapsible collapsed">\n<caption>Summary of rioters condemned 22 June 1816<sup id="cite_ref-FOOTNOTEPeacock1965174.E2.80.93176_141-1" class="reference"><a href="#cite_note-FOOTNOTEPeacock1965174.E2.80.93176-141">[122]</a></sup></caption>\n<tr>\n<th scope="col">Name</th>\n<th scope="col">Age<sup id="cite_ref-Age_140-1" class="reference"><a href="#cite_note-Age-140">[xix]</a></sup></th>\n<th scope="col">Occupation<sup id="cite_ref-Occupation_142-1" class="reference"><a href="#cite_note-Occupation-142">[xx]</a></sup></th>\n<th scope="col">Village</th>\n<th scope="col">Counts</th>\n<th scope="col">Sentence</th>\n</tr>\n<tr>\n<td>Beamis, William; the elder</td>\n<td align="right">42</td>\n<td>shoemaker</td>\n<td>Littleport</td>\n<td align="right">2</td>\n<td>Executed</td>\n</tr>\n<tr>\n<td>Crow, George</td>\n<td align="right">23</td>\n<td>labourer</td>\n<td>Littleport</td>\n<td align="right">1</td>\n<td>Executed</td>\n</tr>\n<tr>\n<td>Dennis, John</td>\n<td align="right">32</td>\n<td>publican</td>\n<td>Littleport</td>\n<td align="right">3</td>\n<td>Executed</td>\n</tr>\n<tr>\n<td>Harley, Isaac; the junior</td>\n<td align="right">33</td>\n<td>labourer</td>\n<td>Littleport</td>\n<td align="right">1</td>\n<td>Executed</td>\n</tr>\n<tr>\n<td>South, Thomas; the younger</td>\n<td align="right">22</td>\n<td>labourer</td>\n<td>Littleport</td>\n<td align="right">2</td>\n<td>Executed</td>\n</tr>\n<tr>\n<td>Chevell, Aaron<sup id="cite_ref-Chevall_100-3" class="reference"><a href="#cite_note-Chevall-100">[xv]</a></sup></td>\n<td align="right">28</td>\n<td>labourer</td>\n<td>Littleport</td>\n<td align="right">2</td>\n<td>Transported life</td>\n</tr>\n<tr>\n<td>Easey, Joseph</td>\n<td align="right">35</td>\n<td>labourer</td>\n<td>Littleport</td>\n<td align="right">1</td>\n<td>Transported life</td>\n</tr>\n<tr>\n<td>Jefferson, John</td>\n<td align="right">42</td>\n<td>labourer</td>\n<td>Littleport</td>\n<td align="right">2</td>\n<td>Transported life</td>\n</tr>\n<tr>\n<td>Jessop, Richard</td>\n<td align="right">21</td>\n<td>labourer</td>\n<td>Littleport</td>\n<td align="right">2</td>\n<td>Transported life</td>\n</tr>\n<tr>\n<td>Newell, James</td>\n<td align="right"><sup id="cite_ref-NoAge_143-0" class="reference"><a href="#cite_note-NoAge-143">[xxi]</a></sup></td>\n<td>labourer</td>\n<td>Littleport</td>\n<td align="right">1</td>\n<td>Transported life</td>\n</tr>\n<tr>\n<td>Rutter, Richard</td>\n<td align="right"><sup id="cite_ref-NoAge_143-1" class="reference"><a href="#cite_note-NoAge-143">[xxi]</a></sup></td>\n<td>labourer</td>\n<td>Littleport</td>\n<td align="right">1</td>\n<td>Transported 14 years</td>\n</tr>\n<tr>\n<td>Benton, Mark</td>\n<td align="right">60</td>\n<td>labourer</td>\n<td>Littleport</td>\n<td align="right">1</td>\n<td>Transported 7 years</td>\n</tr>\n<tr>\n<td>Easey, John</td>\n<td align="right">28</td>\n<td>labourer</td>\n<td>Littleport</td>\n<td align="right">1</td>\n<td>Transported 7 years</td>\n</tr>\n<tr>\n<td>Walker, John</td>\n<td align="right">30</td>\n<td>labourer</td>\n<td>Littleport</td>\n<td align="right">1</td>\n<td>Transported 7 years</td>\n</tr>\n<tr>\n<td>Atkin, William</td>\n<td align="right">28</td>\n<td>carpenter<sup id="cite_ref-FOOTNOTEPeacock196550_67-1" class="reference"><a href="#cite_note-FOOTNOTEPeacock196550-67">[55]</a></sup></td>\n<td>Ely</td>\n<td align="right">2</td>\n<td>12 months</td>\n</tr>\n<tr>\n<td>Beamiss, William; the younger</td>\n<td align="right">18</td>\n<td>shoemaker</td>\n<td>Littleport</td>\n<td align="right">2</td>\n<td>12 months</td>\n</tr>\n<tr>\n<td>Butcher, Robert</td>\n<td align="right"><sup id="cite_ref-NoAge_143-2" class="reference"><a href="#cite_note-NoAge-143">[xxi]</a></sup></td>\n<td>labourer</td>\n<td>Littleport</td>\n<td align="right">1</td>\n<td>12 months</td>\n</tr>\n<tr>\n<td>Cammell, James</td>\n<td align="right">32</td>\n<td>labourer</td>\n<td>Littleport</td>\n<td align="right">1</td>\n<td>12 months</td>\n</tr>\n<tr>\n<td>Cooper, John</td>\n<td align="right"><sup id="cite_ref-NoAge_143-3" class="reference"><a href="#cite_note-NoAge-143">[xxi]</a></sup></td>\n<td>no occupation</td>\n<td>Ely</td>\n<td align="right">1</td>\n<td>12 months</td>\n</tr>\n<tr>\n<td>Crabb, Robert</td>\n<td align="right">40</td>\n<td>labourer</td>\n<td>Littleport</td>\n<td align="right">1</td>\n<td>12 months</td>\n</tr>\n<tr>\n<td>Dann, William</td>\n<td align="right">43</td>\n<td>labourer</td>\n<td>Littleport</td>\n<td align="right">1</td>\n<td>12 months</td>\n</tr>\n<tr>\n<td>Hobbs, Sarah</td>\n<td align="right">24</td>\n<td>labourer</td>\n<td>Ely</td>\n<td align="right">1</td>\n<td>12 months</td>\n</tr>\n<tr>\n<td>Layton, Aaron</td>\n<td align="right">25</td>\n<td>bricklayer</td>\n<td>Ely</td>\n<td align="right">2</td>\n<td>12 months</td>\n</tr>\n<tr>\n<td>Pricke, John</td>\n<td align="right"><sup id="cite_ref-NoAge_143-4" class="reference"><a href="#cite_note-NoAge-143">[xxi]</a></sup></td>\n<td>potter</td>\n<td>Ely</td>\n<td align="right">1</td>\n<td>12 months</td>\n</tr>\n</table>\n<h3><span class="mw-headline" id="Execution">Execution</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=Ely_and_Littleport_riots_of_1816&amp;action=edit&amp;section=15" title="Edit section: Execution">edit</a><span class="mw-editsection-bracket">]</span></span></h3>\n<div class="thumb tright">\n<div class="thumbinner" style="width:172px;"><a href="/wiki/File:Memorial_to_executed_Ely_and_Littleport_rioters.jpg" class="image"><img alt="Memorial plaque which reads: Here lye interred in one grave the bodies of William Beavis, George Crow, John Dennis, Isaac Harley and Thomas South who were all executed at Ely on the 28th day of June 1816 having been convicted at the special assizes held there of diverse robberies during the riots at Ely &amp; Littleport in the month of May in that year. May their awful fate be a warning to others." src="//upload.wikimedia.org/wikipedia/commons/thumb/a/ad/Memorial_to_executed_Ely_and_Littleport_rioters.jpg/170px-Memorial_to_executed_Ely_and_Littleport_rioters.jpg" width="170" height="219" class="thumbimage" srcset="//upload.wikimedia.org/wikipedia/commons/thumb/a/ad/Memorial_to_executed_Ely_and_Littleport_rioters.jpg/255px-Memorial_to_executed_Ely_and_Littleport_rioters.jpg 1.5x, //upload.wikimedia.org/wikipedia/commons/thumb/a/ad/Memorial_to_executed_Ely_and_Littleport_rioters.jpg/340px-Memorial_to_executed_Ely_and_Littleport_rioters.jpg 2x" data-file-width="1320" data-file-height="1701" /></a>\n<div class="thumbcaption">\n<div class="magnify"><a href="/wiki/File:Memorial_to_executed_Ely_and_Littleport_rioters.jpg" class="internal" title="Enlarge"></a></div>\nMemorial plaque, on the south wall of the west tower of St Mary\'s Church, <a href="/wiki/Ely,_Cambridgeshire" title="Ely, Cambridgeshire">Ely, Cambridgeshire</a>, to the five Ely and <a href="/wiki/Littleport,_Cambridgeshire" title="Littleport, Cambridgeshire">Littleport</a> rioters executed in Ely on 28 June 1816<sup id="cite_ref-FOOTNOTEJohnson189379_144-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189379-144">[123]</a></sup></div>\n</div>\n</div>\n<p>On Friday 28 June 1816 at 9&#160;am, the condemned men, William Beamiss, George Crow, John Dennis, Isaac Harley and Thomas South, were driven from the gaol at Ely market place in a black-draped cart<sup id="cite_ref-FOOTNOTEWarren199777.E2.80.9380_145-0" class="reference"><a href="#cite_note-FOOTNOTEWarren199777.E2.80.9380-145">[124]</a></sup> and two&#160;horses costing five-pound five-shillings<sup id="cite_ref-FOOTNOTEJohnson189379_144-1" class="reference"><a href="#cite_note-FOOTNOTEJohnson189379-144">[123]</a></sup> (\xc2\xa3362) accompanied by the bishop\'s gaol chaplain, John Griffin,<sup id="cite_ref-Griffin_146-0" class="reference"><a href="#cite_note-Griffin-146">[125]</a></sup> in a hired chaise and pair costing 13&#160;shillings (\xc2\xa345).<sup id="cite_ref-FOOTNOTEJohnson189380_147-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189380-147">[126]</a></sup> In submitting his expenses on 29 June, chief bailiff F. Bagge noted "We have no power of pressing a cart for the purpose, and \'tis a difficult matter to get one, people feel\'s so much upon the occasion".<sup id="cite_ref-FOOTNOTEJohnson189379_144-2" class="reference"><a href="#cite_note-FOOTNOTEJohnson189379-144">[123]</a></sup></p>\n<p>The men arrived at the gallows at Parnell pits around 11&#160;am, and were hanged after praying with the crowd for some time.<sup id="cite_ref-FOOTNOTEWarren199778_148-0" class="reference"><a href="#cite_note-FOOTNOTEWarren199778-148">[127]</a></sup> Griffin was unofficially given the ropes, which cost one-pound five-shillings (\xc2\xa386),<sup id="cite_ref-FOOTNOTEJohnson189379_144-3" class="reference"><a href="#cite_note-FOOTNOTEJohnson189379-144">[123]</a></sup> after hanging, which he kept; he left a collection to his housekeeper, who sold them as a cure for sore throats.<sup id="cite_ref-Griffin_146-1" class="reference"><a href="#cite_note-Griffin-146">[125]</a></sup> Following the hanging, the bodies were placed in coffins and displayed in a cottage in Gaol Street, where many people came to visit.<sup id="cite_ref-FOOTNOTEJohnson189379_144-4" class="reference"><a href="#cite_note-FOOTNOTEJohnson189379-144">[123]</a></sup> They were buried the next day in St Mary\'s Church, Ely, with the vicar\'s blessing.<sup id="cite_ref-FOOTNOTEJohnson189378.E2.80.9379_149-0" class="reference"><a href="#cite_note-FOOTNOTEJohnson189378.E2.80.9379-149">[128]</a></sup><sup id="cite_ref-FOOTNOTEGoulden200832_150-0" class="reference"><a href="#cite_note-FOOTNOTEGoulden200832-150">[129]</a></sup> As a warning to others, a stone plaque (pictured) was installed on the west side of St Mary\'s Church;<sup id="cite_ref-FOOTNOTEStorey200985_1-6" class="reference"><a href="#cite_note-FOOTNOTEStorey200985-1">[1]</a></sup> it concludes, "May their awful Fate be a warning to others".<sup id="cite_ref-FOOTNOTEGoulden200833_151-0" class="reference"><a href="#cite_note-FOOTNOTEGoulden200833-151">[130]</a></sup></p>\n<p>In 1816, there were a total of 83 people executed in England: 80 men, including the five Littleport rioters, and three women.<sup id="cite_ref-152" class="reference"><a href="#cite_note-152">[131]</a></sup></p>\n<h2><span class="mw-headline" id="Aftermath">Aftermath</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=Ely_and_Littleport_riots_of_1816&amp;action=edit&amp;section=16" title="Edit section: Aftermath">edit</a><span class="mw-editsection-bracket">]</span></span></h2>\n<p>A few days after the execution, the ten condemned prisoners who had had their sentences commuted to twelve months\' imprisonment were transported to the prison <a href="/wiki/Hulk_(ship)" class="mw-redirect" title="Hulk (ship)">hulk</a> <i>Justitia</i>, moored at <a href="/wiki/Woolwich" title="Woolwich">Woolwich</a> on the <a href="/wiki/River_Thames" title="River Thames">River Thames</a>.<sup id="cite_ref-FOOTNOTEBlakeman200115_130-1" class="reference"><a href="#cite_note-FOOTNOTEBlakeman200115-130">[114]</a></sup> Such ships were used as holding areas prior to convicts being transferred to a regular vessel for penal transportation to, at this time, Australia.<sup id="cite_ref-Justitia_153-0" class="reference"><a href="#cite_note-Justitia-153">[132]</a></sup> Residents of Ely tried to hold meetings to complain at this apparent extension of the prisoners\' sentences. Despite, or because of, media attention\xe2\x80\x94newspapers of the time took sides depending whether they supported the government or not\xe2\x80\x94the prisoners were returned to Ely gaol; it may all have been a simple mistake by the clerk of the assizes.<sup id="cite_ref-FOOTNOTEPeacock1965130.E2.80.93131_154-0" class="reference"><a href="#cite_note-FOOTNOTEPeacock1965130.E2.80.93131-154">[133]</a></sup></p>\n<p>On 3 April 1816, lieutenant-colonel <a href="/wiki/William_Sorell" title="William Sorell">William Sorell</a> was appointed <a href="/wiki/Governor_of_Tasmania" title="Governor of Tasmania">lieutenant-governor of Van Diemen\'s Land</a>, now <a href="/wiki/Tasmania" title="Tasmania">Tasmania</a>. He sailed on the <i>Sir William Bensley</i>, the same ship transporting the rioters sentenced to penal transportation. Leaving England on 9 October 1816, the ship arrived in New South Wales 152 days later on 10 March 1817. Soon after, Sorrel sailed to <a href="/wiki/Hobart" title="Hobart">Hobart</a> arriving on 8 April 1817, where he distinguished himself as the third lieutenant-governor.<sup id="cite_ref-155" class="reference"><a href="#cite_note-155">[134]</a></sup></p>\n<p>The Reverend John Vachell stayed on as vicar of St George\'s Littleport in title until 1830; he appointed a <a href="/wiki/Curate" title="Curate">curate</a>, George Britton Jermyn from 1817.<sup id="cite_ref-156" class="reference"><a href="#cite_note-156">[135]</a></sup><sup id="cite_ref-157" class="reference"><a href="#cite_note-157">[136]</a></sup> Some of the St George\'s church registers were destroyed during the riots. The remaining registers start from 1754 (marriages), 1756 (burials), and 1783 (baptism).<sup id="cite_ref-FOOTNOTEPugh195395.E2.80.93102_158-0" class="reference"><a href="#cite_note-FOOTNOTEPugh195395.E2.80.93102-158">[137]</a></sup> General unrest and riots such as that at Littleport may have been a factor<sup id="cite_ref-BBCRiotousLittleport_4-2" class="reference"><a href="#cite_note-BBCRiotousLittleport-4">[3]</a></sup> in the government passing the Vagrancy Act of 1824.<sup id="cite_ref-159" class="reference"><a href="#cite_note-159">[xxii]</a></sup> Due in part to some difficulties in enforcing the law and to continued public unease, the Metropolitan Police Act of 1829<sup id="cite_ref-160" class="reference"><a href="#cite_note-160">[xxiii]</a></sup> was created leading to the first <a href="/wiki/Law_enforcement_in_the_United_Kingdom" title="Law enforcement in the United Kingdom">modern police force</a>.<sup id="cite_ref-FOOTNOTEBell199412_139-1" class="reference"><a href="#cite_note-FOOTNOTEBell199412-139">[121]</a></sup></p>\n<h2><span class="mw-headline" id="See_also">See also</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=Ely_and_Littleport_riots_of_1816&amp;action=edit&amp;section=17" title="Edit section: See also">edit</a><span class="mw-editsection-bracket">]</span></span></h2>\n<ul>\n<li><a href="/wiki/Year_Without_a_Summer" title="Year Without a Summer">Year Without a Summer</a>\xe2\x80\x941816 summer had severe <a href="/wiki/Climate" title="Climate">climate</a> abnormalities resulting in major food shortages across the <a href="/wiki/Northern_Hemisphere" title="Northern Hemisphere">Northern Hemisphere</a></li>\n<li><a href="/wiki/Swing_riots" class="mw-redirect" title="Swing riots">Swing riots</a>\xe2\x80\x94Widespread agricultural worker unrest in Southern England during 1830</li>\n<li><a href="/wiki/2011_England_riots" title="2011 England riots">2011 England riots</a>\xe2\x80\x94London and some cities around the UK during 6\xe2\x80\x9310 August 2011</li>\n<li><i><a href="/wiki/The_Fool_(Edward_Bond_play)" class="mw-redirect" title="The Fool (Edward Bond play)">The Fool</a></i>\xe2\x80\x94a play by <a href="/wiki/Edward_Bond" title="Edward Bond">Edward Bond</a>, first performed in 1975, charting the life of rural poet <a href="/wiki/John_Clare" title="John Clare">John Clare</a> and involving him in the Littleport riots<sup id="cite_ref-TheFool_161-0" class="reference"><a href="#cite_note-TheFool-161">[138]</a></sup></li>\n</ul>\n<h2><span class="mw-headline" id="References">References</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=Ely_and_Littleport_riots_of_1816&amp;action=edit&amp;section=18" title="Edit section: References">edit</a><span class="mw-editsection-bracket">]</span></span></h2>\n<dl>\n<dt>Footnotes</dt>\n</dl>\n<div class="reflist columns references-column-width" style="-moz-column-width: 30em; -webkit-column-width: 30em; column-width: 30em; list-style-type: lower-roman;">\n<ol class="references">\n<li id="cite_note-Cornlaw-3"><span class="mw-cite-backlink"><b><a href="#cite_ref-Cornlaw_3-0">^</a></b></span> <span class="reference-text"><a href="/wiki/Corn_Laws" title="Corn Laws">Corn Laws</a>:Importation Act 1815 (55 Geo. 3 c. 26)</span></li>\n<li id="cite_note-8"><span class="mw-cite-backlink"><b><a href="#cite_ref-8">^</a></b></span> <span class="reference-text">All monetary figures in this article are inflated<sup id="cite_ref-inflation-UK_7-0" class="reference"><a href="#cite_note-inflation-UK-7">[6]</a></sup> to 2017 values using <a href="/wiki/Retail_Prices_Index_(United_Kingdom)" class="mw-redirect" title="Retail Prices Index (United Kingdom)">RPI</a> as described in Choosing the Best Indicator to Measure Relative Worth</span></li>\n<li id="cite_note-11"><span class="mw-cite-backlink"><b><a href="#cite_ref-11">^</a></b></span> <span class="reference-text">One bushell is 56 pounds (25&#160;kg) making 90&#160;one-pound (0.45&#160;kg) loaves of whole wheat bread. One quarter is 28 pounds (13&#160;kg) making 45 loaves of whole wheat bread<sup id="cite_ref-10" class="reference"><a href="#cite_note-10">[8]</a></sup></span></li>\n<li id="cite_note-12"><span class="mw-cite-backlink"><b><a href="#cite_ref-12">^</a></b></span> <span class="reference-text">"<i>Quartern loaf</i> <i>n.</i> <b>1. b</b>: A quarter of a pound (in later use Eng. regional)"\xe2\x80\x94<cite id="CITEREFReference-OED-quartern" class="citation"><a rel="nofollow" class="external text" href="http://oed.com/search?searchType=dictionary&amp;q=quartern">"quartern"</a>. <i><a href="/wiki/Oxford_English_Dictionary" title="Oxford English Dictionary">Oxford English Dictionary</a></i> (3rd ed.). <a href="/wiki/Oxford_University_Press" title="Oxford University Press">Oxford University Press</a>. September 2005.</cite><span title="ctx_ver=Z39.88-2004&amp;rfr_id=info%3Asid%2Fen.wikipedia.org%3AEly+and+Littleport+riots+of+1816&amp;rft.atitle=quartern&amp;rft.btitle=Oxford+English+Dictionary&amp;rft.date=2005-09&amp;rft.edition=3rd&amp;rft.genre=bookitem&amp;rft.pub=Oxford+University+Press&amp;rft_id=http%3A%2F%2Foed.com%2Fsearch%3FsearchType%3Ddictionary%26q%3Dquartern&amp;rft_val_fmt=info%3Aofi%2Ffmt%3Akev%3Amtx%3Abook" class="Z3988"><span style="display:none;">&#160;</span></span>&#160;<span style="font-size:0.95em; font-size: 90%; color: #555">(Subscription or <a rel="nofollow" class="external text" href="https://global.oup.com/oxforddnb/info/freeodnb/libraries/">UK public library membership</a> required.)</span></span></li>\n<li id="cite_note-13"><span class="mw-cite-backlink"><b><a href="#cite_ref-13">^</a></b></span> <span class="reference-text"><cite class="citation book"><a href="/wiki/Charles_Grey,_2nd_Earl_Grey" title="Charles Grey, 2nd Earl Grey">Earl Grey</a>,&#160;He said "At the present price of 60s. the quartern loaf was 1s. and therefore at 80s. one third more, the price of the quartern loaf must be 1s. 4d." (13 March 1815). <a rel="nofollow" class="external text" href="http://hansard.millbanksystems.com/lords/1815/mar/13/state-of-the-corn-laws#column_137">"State of the Corn Laws"</a>. <i><a href="/wiki/Hansard" title="Hansard">Parliamentary Debates (Hansard)</a></i>. House of Commons. col.&#160;137.</cite><span title="ctx_ver=Z39.88-2004&amp;rfr_id=info%3Asid%2Fen.wikipedia.org%3AEly+and+Littleport+riots+of+1816&amp;rft.atitle=State+of+the+Corn+Laws&amp;rft.au=Earl+Grey&amp;rft.btitle=Parliamentary+Debates+%28Hansard%29&amp;rft.date=1815-03-13&amp;rft.genre=bookitem&amp;rft.pages=col.-137&amp;rft.pub=House+of+Commons&amp;rft_id=http%3A%2F%2Fhansard.millbanksystems.com%2Flords%2F1815%2Fmar%2F13%2Fstate-of-the-corn-laws%23column_137&amp;rft_val_fmt=info%3Aofi%2Ffmt%3Akev%3Amtx%3Abook" class="Z3988"><span style="display:none;">&#160;</span></span><sup id="cite_ref-12" class="reference"><a href="#cite_note-12">[iv]</a></sup></span></li>\n<li id="cite_note-Dering-20"><span class="mw-cite-backlink"><b><a href="#cite_ref-Dering_20-0">^</a></b></span> <span class="reference-text">Possibly Rector of <a href="/wiki/West_Deeping" title="West Deeping">West Deeping</a>, now in <a href="/wiki/Lincolnshire" title="Lincolnshire">Lincolnshire</a><sup id="cite_ref-CCED56506_19-0" class="reference"><a href="#cite_note-CCED56506-19">[14]</a></sup></span></li>\n<li id="cite_note-28"><span class="mw-cite-backlink"><b><a href="#cite_ref-28">^</a></b></span> <span class="reference-text">The lighterman took people from <a href="/wiki/Littleport,_Cambridgeshire" title="Littleport, Cambridgeshire">Littleport</a> to <a href="/wiki/Downham_Market" title="Downham Market">Downham Market</a> on pleasure cruises in his <a href="/wiki/Lighter_(barge)" title="Lighter (barge)">lighter</a><sup id="cite_ref-27" class="reference"><a href="#cite_note-27">[21]</a></sup></span></li>\n<li id="cite_note-44"><span class="mw-cite-backlink"><b><a href="#cite_ref-44">^</a></b></span> <span class="reference-text">Ely has long been thought of as a city. See for example 1800 John Aikin "England Delineated".<sup id="cite_ref-FOOTNOTEAikin1800206_42-0" class="reference"><a href="#cite_note-FOOTNOTEAikin1800206-42">[35]</a></sup> It was not until 1 April 1974 that it was granted city status by <a href="/wiki/Letters_patent" title="Letters patent">Letters patent</a><sup id="cite_ref-43" class="reference"><a href="#cite_note-43">[36]</a></sup></span></li>\n<li id="cite_note-46"><span class="mw-cite-backlink"><b><a href="#cite_ref-46">^</a></b></span> <span class="reference-text">67 miles (108&#160;km) according to <a href="/wiki/John_Cary" title="John Cary">John Cary</a>\'s coach routes<sup id="cite_ref-FOOTNOTECary1817707_45-0" class="reference"><a href="#cite_note-FOOTNOTECary1817707-45">[37]</a></sup> but nearly 65 miles (105&#160;km) by 21st century routes</span></li>\n<li id="cite_note-49"><span class="mw-cite-backlink"><b><a href="#cite_ref-49">^</a></b></span> <span class="reference-text">a large twin-bore shotgun used to kill wild-fowl "about 7\xe2\x80\x9310 feet (2\xe2\x80\x933&#160;m) in length, carry about 2 pounds (1&#160;kg) of shot, and will kill at a distance of 150 yards (140&#160;m)"<sup id="cite_ref-FOOTNOTEPeacock1965101_48-0" class="reference"><a href="#cite_note-FOOTNOTEPeacock1965101-48">[39]</a></sup></span></li>\n<li id="cite_note-59"><span class="mw-cite-backlink"><b><a href="#cite_ref-59">^</a></b></span> <span class="reference-text"><span class="plainlinks nourlexpansion"><a class="external text" href="//tools.wmflabs.org/geohack/geohack.php?pagename=Ely_and_Littleport_riots_of_1816&amp;params=52.4524_N_0.3008_E_"><span class="geo-nondefault"><span class="geo-dms" title="Maps, aerial photos, and other data for this location"><span class="latitude">52\xc2\xb027\xe2\x80\xb209\xe2\x80\xb3N</span> <span class="longitude">0\xc2\xb018\xe2\x80\xb203\xe2\x80\xb3E</span></span></span><span class="geo-multi-punct">\xef\xbb\xbf / \xef\xbb\xbf</span><span class="geo-default"><span class="geo-dec" title="Maps, aerial photos, and other data for this location">52.4524\xc2\xb0N 