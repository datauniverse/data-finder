tr>\n<tr>\n<td>live</td>\n<td>lithe</td>\n<td><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA">\xcb\x88la\xc9\xaav</span></td>\n</tr>\n<tr>\n<td>loaf</td>\n<td>loath</td>\n<td><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA">\xcb\x88l\xc9\x99\xca\x8af</span></td>\n</tr>\n<tr>\n<td>loaves</td>\n<td>loathes</td>\n<td><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA">\xcb\x88l\xc9\x99\xca\x8avz</span></td>\n</tr>\n<tr>\n<td>miff</td>\n<td>myth</td>\n<td><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA">\xcb\x88m\xc9\xaaf</span></td>\n</tr>\n<tr>\n<td>never</td>\n<td>nether</td>\n<td><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA">\xcb\x88n\xc9\x9bv\xc9\x99(r)</span></td>\n</tr>\n<tr>\n<td>oaf</td>\n<td>oath</td>\n<td><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA">\xcb\x88\xc9\x99\xca\x8af</span></td>\n</tr>\n<tr>\n<td>phi</td>\n<td>thigh</td>\n<td><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA">\xcb\x88fa\xc9\xaa</span></td>\n</tr>\n<tr>\n<td>Ralph</td>\n<td>wraith</td>\n<td><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA">\xcb\x88re\xc9\xaaf</span></td>\n<td>Some accents pronounce <i>Ralph</i> as <span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA">/\xcb\x88r\xc3\xa6lf/</span>, <span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA">/\xcb\x88r\xc9\x91\xcb\x90lf/</span> or <span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA">/\xcb\x88r\xc9\x91\xcb\x90f/</span></td>\n</tr>\n<tr>\n<td>Ralph</td>\n<td>wrath</td>\n<td><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA">\xcb\x88r\xc9\x91\xcb\x90f</span></td>\n<td>Some accents pronounce <i>Ralph</i> as <s