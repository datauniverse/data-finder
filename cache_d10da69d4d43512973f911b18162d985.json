b'<!DOCTYPE html>\n<html class="client-nojs" lang="en" dir="ltr">\n<head>\n<meta charset="UTF-8"/>\n<title>Smelting - Wikipedia</title>\n<script>document.documentElement.className = document.documentElement.className.replace( /(^|\\s)client-nojs(\\s|$)/, "$1client-js$2" );</script>\n<script>(window.RLQ=window.RLQ||[]).push(function(){mw.config.set({"wgCanonicalNamespace":"","wgCanonicalSpecialPageName":false,"wgNamespaceNumber":0,"wgPageName":"Smelting","wgTitle":"Smelting","wgCurRevisionId":795865475,"wgRevisionId":795865475,"wgArticleId":28716,"wgIsArticle":true,"wgIsRedirect":false,"wgAction":"view","wgUserName":null,"wgUserGroups":["*"],"wgCategories":["Use dmy dates from July 2012","Articles needing additional references from February 2008","All articles needing additional references","Articles needing expert attention with no reason or talk parameter","Articles needing expert attention from May 2009","All articles needing expert attention","Mining articles needing expert attention","History articles needing expert attention","Articles needing additional references from August 2017","All articles with unsourced statements","Articles with unsourced statements from May 2009","Articles with unsourced statements from July 2017","Articles with unsourced statements from July 2008","Articles with unsourced statements from August 2017","Smelting"],"wgBreakFrames":false,"wgPageContentLanguage":"en","wgPageContentModel":"wikitext","wgSeparatorTransformTable":["",""],"wgDigitTransformTable":["",""],"wgDefaultDateFormat":"dmy","wgMonthNames":["","January","February","March","April","May","June","July","August","September","October","November","December"],"wgMonthNamesShort":["","Jan","Feb","Mar","Apr","May","Jun","Jul","Aug","Sep","Oct","Nov","Dec"],"wgRelevantPageName":"Smelting","wgRelevantArticleId":28716,"wgRequestId":"WZTaFQpAEKkAAHgyXwYAAABQ","wgIsProbablyEditable":true,"wgRelevantPageIsProbablyEditable":true,"wgRestrictionEdit":[],"wgRestrictionMove":[],"wgFlaggedRe