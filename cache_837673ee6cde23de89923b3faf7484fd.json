","ext.gadget.teahouse","ext.gadget.ReferenceTooltips","ext.gadget.watchlist-notice","ext.gadget.DRN-wizard","ext.gadget.charinsert","ext.gadget.refToolbar","ext.gadget.extra-toolbar-buttons","ext.gadget.switcher","ext.gadget.featured-articles-links","ext.centralauth.centralautologin","ext.visualEditor.desktopArticleTarget.init","ext.visualEditor.targetLoader","ext.eventLogging.subscriber","ext.wikimediaEvents","ext.navigationTiming","ext.uls.eventlogger","ext.uls.init","ext.uls.interface","ext.centralNotice.geoIP","ext.centralNotice.startUp","skins.vector.js"]);});</script>\n<link rel="stylesheet" href="/w/load.php?debug=false&amp;lang=en&amp;modules=ext.cite.styles%7Cext.uls.interlanguage%7Cext.visualEditor.desktopArticleTarget.noscript%7Cext.wikimediaBadges%7Cmediawiki.legacy.commonPrint%2Cshared%7Cmediawiki.sectionAnchor%7Cmediawiki.skinning.interface%7Cskins.vector.styles%7Cwikibase.client.init&amp;only=styles&amp;skin=vector"/>\n<script async="" src="/w/load.php?debug=false&amp;lang=en&amp;modules=startup&amp;only=scripts&amp;skin=vector"></script>\n<meta name="ResourceLoaderDynamicStyles" content=""/>\n<link rel="stylesheet" href="/w/load.php?debug=false&amp;lang=en&amp;modules=ext.gadget.charinsert-styles&amp;only=styles&amp;skin=vector"/>\n<link rel="stylesheet" href="/w/load.php?debug=false&amp;lang=en&amp;modules=site.styles&amp;only=styles&amp;skin=vector"/>\n<meta name="generator" content="MediaWiki 1.30.0-wmf.15"/>\n<meta name="referrer" content="origin-when-cross-origin"/>\n<link rel="alternate" href="android-app://org.wikipedia/http/en.m.wikipedia.org/wiki/Kalix_dialect"/>\n<link rel="alternate" type="application/x-wiki" title="Edit this page" href="/w/index.php?title=Kalix_dialect&amp;action=edit"/>\n<link rel="edit" title="Edit this page" href="/w/index.php?title=Kalix_dialect&amp;action=edit"/>\n<link rel="apple-touch-icon" href="/static/apple-touch/wikipedia.png"/>\n<link rel="shortcut icon" href="/static/favicon/wikipedia.ico"/>\n<link rel="search" type="application/opensearchdescription+xml" href="/w/opensearch_desc.php" title="Wikipedia (en)"/>\n<link rel="EditURI" type="application/rsd+xml" href="//en.wikipedia.org/w/api.php?action=rsd"/>\n<link rel="license" href="//creativecommons.org/licenses/by-sa/3.0/"/>\n<link rel="canonical" href="https://en.wikipedia.org/wiki/Kalix_dialect"/>\n<link rel="dns-prefetch" href="//login.wikimedia.org"/>\n<link rel="dns-prefetch" href="//meta.wikimedia.org" />\n<!--[if lt IE 9]><script src="/resources/lib/html5shiv/html5shiv.min.js"></script><![endif]-->\n</head>\n<body class="mediawiki ltr sitedir-ltr mw-hide-empty-elt ns-0 ns-subject page-Kalix_dialect rootpage-Kalix_dialect skin-vector action-view">\t\t<div id="mw-page-base" class="noprint"></div>\n\t\t<div id="mw-head-base" class="noprint"></div>\n\t\t<div id="content" class="mw-body" role="main">\n\t\t\t<a id="top"></a>\n\n\t\t\t\t\t\t\t<div id="siteNotice" class="mw-body-content"><!-- CentralNotice --></div>\n\t\t\t\t\t\t<div class="mw-indicators mw-body-content">\n</div>\n\t\t\t<h1 id="firstHeading" class="firstHeading" lang="en">Kalix dialect</h1>\n\t\t\t\t\t\t\t\t\t<div id="bodyContent" class="mw-body-content">\n\t\t\t\t\t\t\t\t\t<div id="siteSub" class="noprint">From Wikipedia, the free encyclopedia</div>\n\t\t\t\t\t\t\t\t<div id="contentSub"></div>\n\t\t\t\t\t\t\t\t\t\t\t\t<div id="jump-to-nav" class="mw-jump">\n\t\t\t\t\tJump to:\t\t\t\t\t<a href="#mw-head">navigation</a>, \t\t\t\t\t<a href="#p-search">search</a>\n\t\t\t\t</div>\n\t\t\t\t<div id="mw-content-text" lang="en" dir="ltr" class="mw-content-ltr"><div class="mw-parser-output"><table class="infobox" style="width:22em">\n<tr>\n<th colspan="2" class="above" style="text-align:center;font-size:125%;font-weight:bold;font-size:130%; color: black; background-color:  #c9ffd9;">Kalix</th>\n</tr>\n<tr>\n<td colspan="2" style="text-align:center;font-size:110%; color: black; background-color:  #c9ffd9;"><i>k\xc3\xb6lism\xc3\xa5le</i></td>\n</tr>\n<tr>\n<th scope="row" style="padding-top:0.225em;line-height:1.1em; padding-right:0.5em;">Pronunciation</th>\n<td style="line-height:1.35em;"><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA"><a href="/wiki/Help:IPA" title="Help:IPA">[kj\xc5\x93\xc9\xad\xc9\xaas\xcb\x88m\xc9\x94\xcb\x90\xc9\xad\xc9\x9b]</a></span></td>\n</tr>\n<tr>\n<th scope="row" style="padding-top:0.225em;line-height:1.1em; padding-right:0.5em;">Native&#160;to</th>\n<td style="line-height:1.35em;">Sweden</td>\n</tr>\n<tr>\n<th scope="row" style="padding-top:0.225em;line-height:1.1em; padding-right:0.5em;">Region</th>\n<td style="line-height:1.35em;"><a href="/wiki/Kalix_Municipality" title="Kalix Municipality">Kalix Municipality</a></td>\n</tr>\n<tr>\n<th scope="row" style="padding-top:0.225em;line-height:1.1em; padding-right:0.5em;">\n<div style="padding:0.1em 0;line-height:1.2em;">Native speakers</div>\n</th>\n<td style="line-height:1.35em;">5,000\xe2\x80\x9310,000&#160;(<i>date missing</i>)<sup class="noprint Inline-Template Template-Fact" style="white-space:nowrap;">[<i><a href="/wiki/Wikipedia:Citation_needed" title="Wikipedia:Citation needed"><span title="This claim needs references to reliable sources. (July 2013)">citation needed</span></a></i>]</sup></td>\n</tr>\n<tr>\n<th scope="row" style="padding-top:0.225em;line-height:1.1em; padding-right:0.5em;">\n<div style="padding:0.1em 0;line-height:1.2em;"><span class="wrap"><a href="/wiki/Language_family" title="Language family">Language family</a></span></div>\n</th>\n<td style="line-height:1.35em;">\n<div style="text-align:left;"><a href="/wiki/Indo-European_languages" title="Indo-European languages">Indo-European</a>\n<ul style="line-height:100%; margin-left:1.35em;padding-left:0">\n<li><a href="/wiki/Germanic_languages" title="Germanic languages">Germanic</a>\n<ul style="line-height:100%;margin-left:0.45em;padding-left:0;">\n<li><a href="/wiki/North_Germanic_languages" title="North Germanic languages">North Germanic</a>\n<ul style="line-height:100%;margin-left:0.45em;padding-left:0;">\n<li><a href="/wiki/Swedish_language" title="Swedish language">Swedish</a>\n<ul style="line-height:100%;margin-left:0.45em;padding-left:0;">\n<li><a href="/wiki/North_Swedish" class="mw-redirect" title="North Swedish">North Swedish</a>\n<ul style="line-height:100%;margin-left:0.45em;padding-left:0;">\n<li><a href="/wiki/Westrobothnian" title="Westrobothnian">Westrobothnian</a>\n<ul style="line-height:100%;margin-left:0.45em;padding-left:0;">\n<li><b>Kalix</b></li>\n</ul>\n</li>\n</ul>\n</li>\n</ul>\n</li>\n</ul>\n</li>\n</ul>\n</li>\n</ul>\n</div>\n</td>\n</tr>\n<tr>\n<th scope="row" style="padding-top:0.225em;line-height:1.1em; padding-right:0.5em;">\n<div style="padding:0.1em 0;line-height:1.2em;"><span class="wrap"><a href="/wiki/Writing_system" title="Writing system">Writing system</a></span></div>\n</th>\n<td style="line-height:1.35em;">Kalix alphabet (<a href="/wiki/Latin_script" title="Latin script">Latin script</a>)</td>\n</tr>\n<tr>\n<th colspan="2" style="text-align:center;color: black; background-color:  #c9ffd9;">Language codes</th>\n</tr>\n<tr>\n<th scope="row" style="padding-top:0.225em;line-height:1.1em; padding-right:0.5em;"><span class="nowrap"><a href="/wiki/ISO_639-3" title="ISO 639-3">ISO 639-3</a></span></th>\n<td style="line-height:1.35em;">\xe2\x80\x93</td>\n</tr>\n<tr>\n<th scope="row" style="padding-top:0.225em;line-height:1.1em; padding-right:0.5em;"><a href="/wiki/Glottolog" title="Glottolog">Glottolog</a></th>\n<td style="line-height:1.35em;"><i>None</i></td>\n</tr>\n<tr>\n<td colspan="2" class="noprint selfref" style="text-align:center;background-color:#ddddff;padding:0.3em 0.5em;text-align:left;line-height:1.3;"><b>This article contains <a href="/wiki/International_Phonetic_Alphabet" title="International Phonetic Alphabet">IPA</a> phonetic symbols.</b> Without proper <a href="/wiki/International_Phonetic_Alphabet#Fonts" title="International Phonetic Alphabet">rendering support</a>, you may see <a href="/wiki/Replacement_character" class="mw-redirect" title="Replacement character">question marks, boxes, or other symbols</a> instead of <a href="/wiki/Unicode" title="Unicode">Unicode</a> characters. For an introductory guide on IPA symbols, see <a href="/wiki/Help:IPA" title="Help:IPA">Help:IPA</a>.</td>\n</tr>\n</table>\n<p><b>Kalix</b> (<span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA">kj\xc5\x93\xc9\xad\xc9\xaasm\xc9\x94\xcb\x90\xc9\xad\xc9\x9b</span> / k\xc3\xb6lism\xc3\xa5le<sup id="cite_ref-ol2000_1-0" class="reference"><a href="#cite_note-ol2000-1">[1]</a></sup>) is a divergent <a href="/wiki/Swedish_dialect" class="mw-redirect" title="Swedish dialect">Swedish dialect</a> spoken in the <a href="/wiki/Kalix_Municipality" title="Kalix Municipality">Kalix Municipality</a> along with <a href="/wiki/Sami_languages" title="Sami languages">Sami</a>, <a href="/wiki/Finnish_language" title="Finnish language">Finnish</a>, <a href="/wiki/Me%C3%A4nkieli" class="mw-redirect" title="Me\xc3\xa4nkieli">Me\xc3\xa4nkieli</a> and the national standard language <a href="/wiki/Standard_Swedish" title="Standard Swedish">Swedish</a>. Like other <a href="/wiki/Scandinavian_languages" class="mw-redirect" title="Scandinavian languages">Scandinavian languages</a>, the Kalix dialect originates in <a href="/wiki/Proto-Norse_language" title="Proto-Norse language">Proto-Norse</a> and dialects of <a href="/wiki/Old_Norse" title="Old Norse">Old Norse</a>, spoken by immigrating Germanic settlers during the <a href="/wiki/Viking_Age" title="Viking Age">Viking Age</a>. It has three <a href="/wiki/Grammatical_gender" title="Grammatical gender">grammatical genders</a>, two plural forms of <a href="/wiki/Definiteness" title="Definiteness">indefinite</a> nouns, and broad usage of definite nouns. Nouns are also inflected differently in <a href="/wiki/Dative_case" title="Dative case">dative</a> and <a href="/wiki/Accusative_case" title="Accusative case">accusative</a> case, and there are three forms of expressing <a href="/wiki/Genitive_case" title="Genitive case">genitive</a>. Most <a href="/wiki/Verb" title="Verb">verbs</a> are conjugated differently in singular and plural, while most <a href="/wiki/Adjective" title="Adjective">adjectives</a> are not. Some adjectives can though be serially joined with nouns and some have two plural forms. A pleonastic article is also always used before people\'s and pet\'s names.<sup id="cite_ref-rutberg1924_2-0" class="reference"><a href="#cite_note-rutberg1924-2">[2]</a></sup></p>\n<p></p>\n<div id="toc" class="toc">\n<div class="toctitle">\n<h2>Contents</h2>\n</div>\n<ul>\n<li class="toclevel-1 tocsection-1"><a href="#History"><span class="tocnumber">1</span> <span class="toctext">History</span></a>\n<ul>\n<li class="toclevel-2 tocsection-2"><a href="#Germanic_settlement"><span class="tocnumber">1.1</span> <span class="toctext">Germanic settlement</span></a></li>\n<li class="toclevel-2 tocsection-3"><a href="#Colonization"><span class="tocnumber">1.2</span> <span class="toctext">Colonization</span></a></li>\n<li class="toclevel-2 tocsection-4"><a href="#Modern_history"><span class="tocnumber">1.3</span> <span class="toctext">Modern history</span></a></li>\n</ul>\n</li>\n<li class="toclevel-1 tocsection-5"><a href="#Documentation"><span class="tocnumber">2</span> <span class="toctext">Documentation</span></a></li>\n<li class="toclevel-1 tocsection-6"><a href="#Geographic_relation_to_Uralic_languages"><span class="tocnumber">3</span> <span class="toctext">Geographic relation to Uralic languages</span></a></li>\n<li class="toclevel-1 tocsection-7"><a href="#Germanic_dialect_continuum"><span class="tocnumber">4</span> <span class="toctext">Germanic dialect continuum</span></a></li>\n<li class="toclevel-1 tocsection-8"><a href="#Phonology"><span class="tocnumber">5</span> <span class="toctext">Phonology</span></a></li>\n<li class="toclevel-1 tocsection-9"><a href="#Grammar"><span class="tocnumber">6</span> <span class="toctext">Grammar</span></a>\n<ul>\n<li class="toclevel-2 tocsection-10"><a href="#Noun_gender"><span class="tocnumber">6.1</span> <span class="toctext">Noun gender</span></a></li>\n<li class="toclevel-2 tocsection-11"><a href="#Definite_and_indefinite_nouns"><span class="tocnumber">6.2</span> <span class="toctext">Definite and indefinite nouns</span></a></li>\n<li class="toclevel-2 tocsection-12"><a href="#Case"><span class="tocnumber">6.3</span> <span class="toctext">Case</span></a></li>\n<li class="toclevel-2 tocsection-13"><a href="#Verbs"><span class="tocnumber">6.4</span> <span class="toctext">Verbs</span></a></li>\n<li class="toclevel-2 tocsection-14"><a href="#Adjectives"><span class="tocnumber">6.5</span> <span class="toctext">Adjectives</span></a></li>\n<li class="toclevel-2 tocsection-15"><a href="#Pleonastic_article"><span class="tocnumber">6.6</span> <span class="toctext">Pleonastic article</span></a></li>\n</ul>\n</li>\n<li class="toclevel-1 tocsection-16"><a href="#Writing_systems.2C_orthography"><span class="tocnumber">7</span> <span class="toctext">Writing systems, orthography</span></a></li>\n<li class="toclevel-1 tocsection-17"><a href="#References"><span class="tocnumber">8</span> <span class="toctext">References</span></a></li>\n</ul>\n</div>\n<p></p>\n<h2><span class="mw-headline" id="History">History</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=Kalix_dialect&amp;action=edit&amp;section=1" title="Edit section: History">edit</a><span class="mw-editsection-bracket">]</span></span></h2>\n<p>While <a href="/wiki/Sami_people" title="Sami people">Sami</a> cultures have been present around Kalix for several thousand years, the Kalix dialect is a development from <a href="/wiki/Germanic_languages" title="Germanic languages">Germanic</a> speaking settlers, arriving along the coast of the <a href="/wiki/Scandinavian_Peninsula" title="Scandinavian Peninsula">Scandinavian peninsula</a>. The <a href="/wiki/Kalix_River" title="Kalix River">Kalix river</a> is called \'G\xc3\xa1l\xc3\xa1seatnu\' in the <a href="/wiki/Northern_Sami_language" class="mw-redirect" title="Northern Sami language">Northern Sami language</a> and "k\xc3\xb6lis" in Kalix (spelled "Chalis" by <a href="/wiki/Olaus_Magnus" title="Olaus Magnus">Olaus Magnus</a> in 1539). The name similarity strongly suggests that it is of Sami origin, and that the first arriving Germanic speaking settlers thus were in contact with Sami people, already present in the area.</p>\n<h3><span class="mw-headline" id="Germanic_settlement">Germanic settlement</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=Kalix_dialect&amp;action=edit&amp;section=2" title="Edit section: Germanic settlement">edit</a><span class="mw-editsection-bracket">]</span></span></h3>\n<p>Different theories exist as of how exactly the Kalix river valley came to be settled by Germanic speakers. Related Germanic language is also spoken further to the southeast, with areas with mainly <a href="/wiki/Finnic_languages" title="Finnic languages">Finnic</a> speakers in between. This suggests movement along both sides of the <a href="/wiki/Bothnian_Bay" title="Bothnian Bay">Bothnian Bay</a>, and a relatively peaceful relationship between the three different groups, Germanic, Sami and Finnic, long before all of them fell under state control.</p>\n<p>The Germanic settlers spoke a north dialectal development of <a href="/wiki/Proto-Norse_language" title="Proto-Norse language">Proto-Norse</a>, related to, but not equal to the <a href="/wiki/Old_Norse" title="Old Norse">Old Norse</a> spoken by Vikings many hundred kilometers down the Scandinavian coast. Old Norse is rather well preserved in <a href="/wiki/Runestone" title="Runestone">runestones</a> and later also in a Bible translation. But few runic inscriptions have been found north of <a href="/wiki/Svealand" title="Svealand">Svealand</a>, and none at all in what is now the counties of <a href="/wiki/V%C3%A4sterbotten_County" title="V\xc3\xa4sterbotten County">V\xc3\xa4sterbotten</a> and <a href="/wiki/Norrbotten_County" title="Norrbotten County">Norrbotten</a>. This suggests that the farming settlers finally reaching Kalix had little or no contact with Vikings during the Viking age, and most probably already by then had developed different linguistic features, some of which are still preserved in the modern Kalix dialect.</p>\n<h3><span class="mw-headline" id="Colonization">Colonization</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=Kalix_dialect&amp;action=edit&amp;section=3" title="Edit section: Colonization">edit</a><span class="mw-editsection-bracket">]</span></span></h3>\n<p><a href="/wiki/Christianity" title="Christianity">Christianity</a> came to the relatively non-organized and free Germanic settlers, who until then might have been practising variants of <a href="/wiki/Norse_mythology" title="Norse mythology">Norse mythology</a>. The magnificent Kalix stone church has been dated to the mid-15th century, but it probably had a wooden building as predecessor. The area must anyway have had a substantial population by that time to fill the church. This population probably spoke an early form of the Kalix dialect. Priests began registering all family relationships in the villages, and since this new era we have better knowledge of the local history, also from preserved documents and maps used for taxation. Colonization escalated under the <a href="/wiki/Swedish_Empire" title="Swedish Empire">Swedish Empire</a>.</p>\n<h3><span class="mw-headline" id="Modern_history">Modern history</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=Kalix_dialect&amp;action=edit&amp;section=4" title="Edit section: Modern history">edit</a><span class="mw-editsection-bracket">]</span></span></h3>\n<p>The Swedish school came to Kalix in the 1850s, with the goal of teaching everyone to read, write, speak and understand standard Swedish with its <a href="/wiki/Swedish_grammar" title="Swedish grammar">grammar</a>. This was a rather peaceful <a href="/wiki/Language_education" title="Language education">language education</a>, but in the early 1930s parents were told to speak standard Swedish to their children. This praxis has been criticized by later research in <a href="/wiki/Multilingualism" title="Multilingualism">multilingualism</a>, and had a huge influence on many small societies like Kalix. The same standardization of language took place in many parts of Sweden.</p>\n<h2><span class="mw-headline" id="Documentation">Documentation</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=Kalix_dialect&amp;action=edit&amp;section=5" title="Edit section: Documentation">edit</a><span class="mw-editsection-bracket">]</span></span></h2>\n<p>The oldest preserved manuscripts in the Kalix dialect is an 1879 description of the area,<sup id="cite_ref-kfn02-3_3-0" class="reference"><a href="#cite_note-kfn02-3-3">[3]</a></sup> a text which is used as a standard of genuinity. The Kalix dialect was first described by a thesis work<sup id="cite_ref-rutberg1924_2-1" class="reference"><a href="#cite_note-rutberg1924-2">[2]</a></sup> by Hulda Rutberg, starting the year 1908 and ventilated at Uppsala University in 1924. The book contains many words and an extensive description of phonology and grammar. The language is also covered in later documentation,<sup id="cite_ref-.C3.96vre_Norrlands_bygdem.C3.A5l_4-0" class="reference"><a href="#cite_note-.C3.96vre_Norrlands_bygdem.C3.A5l-4">[4]</a></sup> and by many recordings from the 1960s.<sup id="cite_ref-sofi_5-0" class="reference"><a href="#cite_note-sofi-5">[5]</a></sup> The work of communities such as <i>F\xc3\xb6reningen Kalix Bygdem\xc3\xa5l</i>, founded 1992, has kept collecting words and expressions to an extensive word lexicon,<sup id="cite_ref-ol2000_1-1" class="reference"><a href="#cite_note-ol2000-1">[1]</a></sup><sup id="cite_ref-ol90_6-0" class="reference"><a href="#cite_note-ol90-6">[6]</a></sup> and is still active today. Recent projects have made the general public more aware and interested, where young people in Kalix e.g. present the dialec ton the Internet.<sup id="cite_ref-tkl_7-0" class="reference"><a href="#cite_note-tkl-7">[7]</a></sup></p>\n<h2><span class="mw-headline" id="Geographic_relation_to_Uralic_languages">Geographic relation to Uralic languages</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=Kalix_dialect&amp;action=edit&amp;section=6" title="Edit section: Geographic relation to Uralic languages">edit</a><span class="mw-editsection-bracket">]</span></span></h2>\n<p>Kalix is the easternmost river valley traditionally completely settled by Germanic-language speakers, while the nearby <a href="/wiki/Tornio" title="Tornio">Tornio</a> river valley was settled by <a href="/wiki/Finnic_languages" title="Finnic languages">Finnic language</a> <a href="/wiki/Me%C3%A4nkieli" class="mw-redirect" title="Me\xc3\xa4nkieli">Me\xc3\xa4nkieli</a> speakers, evidently arriving along the coastline from the southeast. A sharp language border is found between the villages <a href="/wiki/Haparanda" title="Haparanda">S\xc3\xa4ivis</a> and <a href="/wiki/Sangis" title="Sangis">Sangis</a>, where the latter traditionally uses the Kalix dialect. To the north, the <a href="/wiki/Northern_Sami_language" class="mw-redirect" title="Northern Sami language">Northern Sami language</a> has been spoken. Thus, the Kalix dialect has traditionally been literally surrounded by <a href="/wiki/Uralic_languages" title="Uralic languages">Uralic languages</a>, which however has had remarkably few influences.</p>\n<h2><span class="mw-headline" id="Germanic_dialect_continuum">Germanic dialect continuum</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=Kalix_dialect&amp;action=edit&amp;section=7" title="Edit section: Germanic dialect continuum">edit</a><span class="mw-editsection-bracket">]</span></span></h2>\n<p>The Kalix dialect is part of a Germanic and Nordic <a href="/wiki/Dialect_continuum" title="Dialect continuum">dialect continuum</a> on the <a href="/wiki/Scandinavian_Peninsula" title="Scandinavian Peninsula">Scandinavian Peninsula</a>, and more locally along the coastline of the <a href="/wiki/Bothnian_Bay" title="Bothnian Bay">Bothnian Bay</a>. The Kalix dialect is very much related to nearby languages/dialects westwards and southwards <a href="/wiki/R%C3%A5ne%C3%A5" title="R\xc3\xa5ne\xc3\xa5">R\xc3\xa5ne\xc3\xa5</a>, <a href="/wiki/Lule%C3%A5" title="Lule\xc3\xa5">Lule\xc3\xa5</a>, <a href="/wiki/Pite%C3%A5" title="Pite\xc3\xa5">Pite\xc3\xa5</a>, <a href="/wiki/Skellefte%C3%A5" title="Skellefte\xc3\xa5">Skellefte\xc3\xa5</a>, <a href="/wiki/Ume%C3%A5" title="Ume\xc3\xa5">Ume\xc3\xa5</a> northwards <a href="/wiki/Overkalix" class="mw-redirect" title="Overkalix">Overkalix</a> etc., basically <a href="/wiki/Mutual_intelligibility" title="Mutual intelligibility">mutually intelligible</a> with the Kalix dialect as they all are part of a broader <a href="/wiki/Westrobothnian" title="Westrobothnian">Westrobothnian</a> dialect continuum. The Overkalix dialect differs mainly in a few vocal shifts, and the reason why the two areas has developed distinct characteristics is believed to be that the Kalix river makes a geographic formation near the villages Morj\xc3\xa4rv and \xc3\x96vermorj\xc3\xa4rv, which has led to a small communication barrier. Eastwards there are several dialects spoken in the archipelago in Finland, especially in <a href="/wiki/Ostrobothnia_(region)" title="Ostrobothnia (region)">Ostrobothnia</a>, which has obvious similarities with the Kalix dialect, making it evident that people have moved in both directions along the coast of the Bothnian bay. Grammatical similarities can also be found as far away as in <a href="/wiki/Troms" title="Troms">Troms</a>, at the north border of Scandinavia, not present in standard Swedish nor Norwegian bokm\xc3\xa5l, which proves Germanic contact by foot between the two coasts. The pleonastic<sup id="cite_ref-rutberg1924_2-2" class="reference"><a href="#cite_note-rutberg1924-2">[2]</a></sup> determined article, a "he" or "she" always put before people\'s names, is a good example.<sup id="cite_ref-nordia_8-0" class="reference"><a href="#cite_note-nordia-8">[8]</a></sup></p>\n<h2><span class="mw-headline" id="Phonology">Phonology</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=Kalix_dialect&amp;action=edit&amp;section=8" title="Edit section: Phonology">edit</a><span class="mw-editsection-bracket">]</span></span></h2>\n<p>The Kalix dialect has, according to Rutberg,<sup id="cite_ref-rutberg1924_2-3" class="reference"><a href="#cite_note-rutberg1924-2">[2]</a></sup> 18 vowel monophtongs, 10 vowel diphthongs, and 29 consonants. It is also identified by its very common diacritic accent, where a vowel is repeated and stressed twice. Many vowels can be represented by distinct IPA characters, some of which are listed in the table below:</p>\n<table class="wikitable">\n<tr>\n<th><a href="/wiki/International_Phonetic_Alphabet" title="International Phonetic Alphabet">IPA</a></th>\n<th>Ex.IPA</th>\n<th>Ex.Latin</th>\n<th>Translation</th>\n</tr>\n<tr>\n<td><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA"><a href="/wiki/Close_front_unrounded_vowel" title="Close front unrounded vowel">i</a></span></td>\n<td>i\xcb\x90ln</td>\n<td>i:ln</td>\n<td>the fire</td>\n</tr>\n<tr>\n<td><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA"><a href="/wiki/Near-close_near-front_unrounded_vowel" title="Near-close near-front unrounded vowel">\xc9\xaa</a></span></td>\n<td>h\xc9\xaan</td>\n<td>hin</td>\n<td>here</td>\n</tr>\n<tr>\n<td><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA"><a href="/wiki/Close_front_rounded_vowel" title="Close front rounded vowel">y</a></span></td>\n<td>sny\xcb\x90n</td>\n<td>sny:n</td>\n<td>the snow</td>\n</tr>\n<tr>\n<td><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA"><a href="/wiki/Near-close_near-front_rounded_vowel" title="Near-close near-front rounded vowel">\xca\x8f</a></span></td>\n<td>\xca\x8fv\xc9\xaa</td>\n<td>yvi</td>\n<td>over</td>\n</tr>\n<tr>\n<td><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA"><a href="/wiki/Close-mid_front_unrounded_vowel" title="Close-mid front unrounded vowel">e</a></span></td>\n<td>ve:r</td>\n<td>ve:r</td>\n<td>weather</td>\n</tr>\n<tr>\n<td><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA"><a href="/wiki/Open-mid_front_unrounded_vowel" title="Open-mid front unrounded vowel">\xc9\x9b</a></span></td>\n<td>m\xc9\x9bst\xc9\x9b</td>\n<td>meste</td>\n<td>almost</td>\n</tr>\n<tr>\n<td><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA"><a href="/wiki/Near-open_front_unrounded_vowel" title="Near-open front unrounded vowel">\xc3\xa6</a></span></td>\n<td>\xca\x9d\xc3\xa6\xc9\xbe</td>\n<td>j\xc3\xa4r</td>\n<td>is (singular)</td>\n</tr>\n<tr>\n<td><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA"><a href="/wiki/Close-mid_front_rounded_vowel" title="Close-mid front rounded vowel">\xc3\xb8</a></span></td>\n<td>r\xc3\xb8\xcb\x90</td>\n<td>r\xc3\xb6:</td>\n<td>red (singular)</td>\n</tr>\n<tr>\n<td><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA"><a href="/wiki/Open-mid_front_rounded_vowel" title="Open-mid front rounded vowel">\xc5\x93</a></span></td>\n<td>n\xc5\x93</td>\n<td>n\xc3\xb6</td>\n<td>now</td>\n</tr>\n<tr>\n<td><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA"><a href="/wiki/Close_central_rounded_vowel" title="Close central rounded vowel">\xca\x89</a></span></td>\n<td>h\xca\x89l</td>\n<td>hul</td>\n<td>was going to</td>\n</tr>\n<tr>\n<td><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA"><a href="/wiki/Near-close_near-back_rounded_vowel" title="Near-close near-back rounded vowel">\xca\x8a</a></span></td>\n<td>\xca\x9d\xca\x8a</td>\n<td>jo</td>\n<td>yes/well</td>\n</tr>\n<tr>\n<td><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA"><a href="/wiki/Open_front_unrounded_vowel" title="Open front unrounded vowel">a</a></span></td>\n<td>anar</td>\n<td>anar</td>\n<td>another</td>\n</tr>\n<tr>\n<td><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA"><a href="/wiki/Open_back_unrounded_vowel" title="Open back unrounded vowel">\xc9\x91</a></span></td>\n<td>l\xc9\x91\xcb\x90k</td>\n<td>la:k</td>\n<td>long</td>\n</tr>\n<tr>\n<td><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA"><a href="/wiki/Open_back_rounded_vowel" title="Open back rounded vowel">\xc9\x92</a></span></td>\n<td>k\xc9\x92m</td>\n<td>kom</td>\n<td>came</td>\n</tr>\n<tr>\n<td><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA"><a href="/wiki/Open-mid_back_rounded_vowel" title="Open-mid back rounded vowel">\xc9\x94</a></span></td>\n<td>g\xc9\x94\xcb\x90\xc9\xb3</td>\n<td>g\xc3\xa5:rn</td>\n<td>the yard</td>\n</tr>\n</table>\n<h2><span class="mw-headline" id="Grammar">Grammar</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=Kalix_dialect&amp;action=edit&amp;section=9" title="Edit section: Grammar">edit</a><span class="mw-editsection-bracket">]</span></span></h2>\n<p>The Kalix dialect has an extensive <a href="/wiki/Inflection" title="Inflection">inflection</a>, with many characteristics similar to the <a href="/wiki/German_language" title="German language">German language</a>.</p>\n<h3><span class="mw-headline" id="Noun_gender">Noun gender</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=Kalix_dialect&amp;action=edit&amp;section=10" title="Edit section: Noun gender">edit</a><span class="mw-editsection-bracket">]</span></span></h3>\n<p>Three <a href="/wiki/Grammatical_gender" title="Grammatical gender">grammatical genders</a> exist:</p>\n<ul>\n<li>Feminine: e.g. "ha:ta" (the hand), "nagla" (the nail), "\xc3\xa5:dra" (the vein), "sk\xc3\xb6uldra" (the shoulder), "p\xc3\xa5p:a" (the father), "m\xc3\xa5m:a" (the mother), "kjat:a" (the cat). But also "kuno" (the woman), "stuo" (the cottage), "s\xc3\xb6go" (the saga).</li>\n<li>Masculine: e.g. "st\xc3\xa5oLn" (the stool), "f\xc3\xa5otn" (the foot), "armen" (the arm), "armboan" (the elbow), "t\xc3\xa5omen" (the thumb), "m\xc3\xa5on" (the mouth).</li>\n<li>Neuter: "\xc3\xb6e"/"\xc3\xb6ge" (the eye), "\xc3\xb6re" (the ear), "h\xc3\xb6ure" (the head), "bene" (the leg), "feingre" (the finger), "kni:e" (the knee), "b\xc3\xa5n:e" (the child).</li>\n</ul>\n<p>Basically, words that in their definite form end with an "n" are masculine, an "e" is neuter, and all vowel except "e" are feminine.</p>\n<p>General ending for words following the nouns are in feminine "-ar", masculine "-en", neuter "-e" or "-t", and plural "-er". Ex.</p>\n<ul>\n<li>Feminine: "he j\xc3\xa4r menar stuo" (it is my cottage) "h\xc3\xb6 ha:ar eingar p\xc3\xa5:\xc3\xa5p" (she had no dad), "hukar kuno?" (which woman?), "woLar viko" (every week)</li>\n<li>Masculine: "men st\xc3\xa5oL" (my stool), "anworn da" (every second day), "in tuken f\xc3\xa5ot" (such a foot)</li>\n<li>Neuter: "i l\xc3\xa4tet b\xc3\xa5n" (a little child), "tuke schwammeL" (such bullshit), "i anne \xc3\xa5:r" (another year)</li>\n<li>Plural: "tuker st\xc3\xa4into" (such girls), "huker d\xc3\xa5:a?" (which days?), "einger feingro" (no fingers)</li>\n</ul>\n<h3><span class="mw-headline" id="Definite_and_indefinite_nouns">Definite and indefinite nouns</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=Kalix_dialect&amp;action=edit&amp;section=11" title="Edit section: Definite and indefinite nouns">edit</a><span class="mw-editsection-bracket">]</span></span></h3>\n<p>The definite noun form is used in a broader sense than in other Scandinavian languages, widespread in all dialects spoken in northern Scandinavia.<sup id="cite_ref-dahl_gram_9-0" class="reference"><a href="#cite_note-dahl_gram-9">[9]</a></sup> Some examples: "je sk\xc3\xa5 n\xc3\xa5:\xc3\xa5p i gr\xc3\xb6ut ve bera" \xe2\x80\x93 I\'ll pick some (the)berries, "kunin j\xc3\xa5ra \xc3\xa4t som k\xc3\xa5ran" \xe2\x80\x93 (the)women are not like (the)men. Definiteviness can be divided into four categories depending on the noun\'s plural form. Examples of usage with the feminine word "i fLa:ask" (a bottle / a flask):</p>\n<ul>\n<li>Enumerating indefinite, equal to singular or differs on accent only: "je ha:ar to fLa:ask" (I had two flasks), "i d\xc3\xb6usin fLa:ask" (a dozen flasks), "je ha fLe:r fLa:ask \xc3\xa4n di:" (I have more flasks than you), "ma:ak fLa:ask" (many flasks).</li>\n<li>Non-enumerating indefinite, "-o" ending: "he j\xc3\xa4r naer/einger/in del fLasko ini tj\xc3\xa4laro" (there are some/no/some flasks in the cellar), "aar fLasko" (other flasks), "tuker fLasko" (such flasks), "he j\xc3\xa4r la:ka fLasko ini tj\xc3\xa4laro" (there are long flasks in the cellar).</li>\n<li>Definite usage, "-en" ending: "he j\xc3\xa4r mytji fLasken ini tj\xc3\xa4laro" (there are a lot of flasks in the cellar), "\xc3\xa5:\xc3\xa5ll fLasken j\xc3\xa5ra b\xc3\xa5:\xc3\xa5rt" (all flasks are gone), "h\xc3\xb6undratale \xc3\xa5 fLasken" (hundreds of flasks), "he var fLasken \xc3\xa5llostans" (there were flasks everywhere), "whiskeyfLasken" (wiskey flasks), "we h\xc3\xa5:\xc3\xa5 la:kfLasken \xc3\xa5 r\xc3\xb6undfLasken" (we have long flasks and round flasks), "di ha:ar sn\xc3\xb6rt fLasken \xc3\xa5llostans" (they had thrown flasks everywhere).</li>\n<li>Definite "-en": "ta ve de fLasken d\xc3\xa5 do gja \xc3\xb6ut" (take the flasks with you when you go out)</li>\n</ul>\n<p>For masculine nouns, the four forms are e.g. "in bi:l" (a car) "to bi:il" (two cars) "naer bi:lo" (some cars), "mytji bi:lan" (many cars), and "bi:lan" (the cars). Neuter definitive plural ending is "-a". Non-enumerative words e.g. "i h\xc3\xb6us" (a house), "i g\xc3\xa5Lv" (a floor) are exceptions lacking the "-o" form.</p>\n<h3><span class="mw-headline" id="Case">Case</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=Kalix_dialect&amp;action=edit&amp;section=12" title="Edit section: Case">edit</a><span class="mw-editsection-bracket">]</span></span></h3>\n<p><a href="/wiki/Dative_case" title="Dative case">Dative</a> is separated from the <a href="/wiki/Accusative_case" title="Accusative case">Accusative</a> and <a href="/wiki/Nominative_case" title="Nominative case">Nominative case</a>, e.g. feminine: "Din j\xc3\xa4r Sk\xc3\xa5oLa, je siti ini sk\xc3\xa5:oLn" (there is the school, I am sitting in the school), masculine: "je sei tj\xc3\xa4larn, he lik na ini tj\xc3\xa4laro" (I see the basement, it\'s something in the basement).</p>\n<p>Several forms of <a href="/wiki/Genitive_case" title="Genitive case">Genitive cases</a> exists, e.g. "Je ha ons Enok bi:l" (I have Enok\'s car), "je fick bre:ve \xc3\xa4n Anna" (I got Anna\'s letter), "kL\xc3\xa5k:a gran:o" (The neighbor\xe2\x80\x99s clock).</p>\n<h3><span class="mw-headline" id="Verbs">Verbs</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=Kalix_dialect&amp;action=edit&amp;section=13" title="Edit section: Verbs">edit</a><span class="mw-editsection-bracket">]</span></span></h3>\n<p>Verbs are conjugated in singular and plural, unlike modern standard Swedish: "h\xc3\xa5n j\xc3\xa4r" (he is) but "di j\xc3\xa5ra" (they are), "h\xc3\xb6 l\xc3\xb6ut se" (she leans herself) but "di l\xc3\xb6:ut se" (they lean themselves), "je far" (I go) but "we f\xc3\xa5ra" (we go), "je vil" (I want) but "di vili" (they want). But there are irregular verbs which does not differ, e.g. "je liot f\xc3\xa5ra" (I have to go) / "we liot f\xc3\xa5ra" (we have to go).</p>\n<h3><span class="mw-headline" id="Adjectives">Adjectives</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=Kalix_dialect&amp;action=edit&amp;section=14" title="Edit section: Adjectives">edit</a><span class="mw-editsection-bracket">]</span></span></h3>\n<p>Most <a href="/wiki/Adjective" title="Adjective">Adjectives</a> are equal in singular and plural, similar to English but distinct from many other Scandinavian languages, e.g.: "d\xc3\xb6:rn j\xc3\xa4r ipi" (the door is open) and "doran j\xc3\xa5ra ipi" (the doors are open), "b\xc3\xa5:ne j\xc3\xa4r v\xc3\xb6tchin" (the child is awake) and "b\xc3\xa5:na j\xc3\xa5ra v\xc3\xb6tchin" (the children are awake), "do j\xc3\xa4r w\xc3\xa4lkymin heit" (she is welcome here) and "di j\xc3\xa5ra w\xc3\xa4lkymin heit" (they are welcome here).</p>\n<p>Other adjectives differs in singular and plural, and have two plural forms, e.g. "flaska j\xc3\xa4r r\xc3\xb6:" (the flask is red), "r\xc3\xb6:a flasko, so j\xc3\xa5ra r\xc3\xb6:\xc3\xb6" (red flasks, that are red).</p>\n<p>Adjectives can also be joined with nouns, e.g. "r\xc3\xa5Lkaf\xc3\xb6tren" (dirty feet), or serially joined, e.g. "lilv\xc3\xa5ckerst\xc3\xa4inta" (the little beautiful girl).</p>\n<h3><span class="mw-headline" id="Pleonastic_article">Pleonastic article</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=Kalix_dialect&amp;action=edit&amp;section=15" title="Edit section: Pleonastic article">edit</a><span class="mw-editsection-bracket">]</span></span></h3>\n<p>The pleonastic article is widespread among languages in the area, as far north as Troms.<sup id="cite_ref-nordia_8-1" class="reference"><a href="#cite_note-nordia-8">[8]</a></sup> A "he" / "him" or "she" / "her" is always put before people\'s names, pet\'s names, and words like e.g. father and mother. e.g. "on far \xc3\xa5 na m\xc3\xa5or" (mom and dad). The pleonastic article differs in many aspects, by both grammatical gender and case, e.g. "en Erik dji matn \xc3\xa5t o Lars" (Eric gives food to Lars), "a Brit sk\xc3\xa5 t\xc3\xa5La ve en Anna" (Brit will talk to Anna).</p>\n<h2><span class="mw-headline" id="Writing_systems.2C_orthography">Writing systems, orthography</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=Kalix_dialect&amp;action=edit&amp;section=16" title="Edit section: Writing systems, orthography">edit</a><span class="mw-editsection-bracket">]</span></span></h2>\n<p>In early scientific literature, a phonetic alphabet <a href="/wiki/Landsm%C3%A5lsalfabetet" class="mw-redirect" title="Landsm\xc3\xa5lsalfabetet">landsm\xc3\xa5lsalfabetet</a> (LMA), developed by <a href="/wiki/Johan_August_Lundell" title="Johan August Lundell">Johan August Lundell</a> was used to write Kalix dialect, while the most widely used informal form of writing is based on the Latin alphabet with a few added symbols (Kalix variant), including letters \xc3\xa5, \xc3\xa4, \xc3\xb6, capitalized L or bolded <b>l</b>, apostrophe \xc2\xb4 or colon&#160;: for marking long or diacritic accents, etc.<sup id="cite_ref-ol2000_1-2" class="reference"><a href="#cite_note-ol2000-1">[1]</a></sup> Since no formal standard has been developed, slight differences can be found among different writers. Recent language projects have used the Kalix Alphabet,<sup id="cite_ref-tklalpha_10-0" class="reference"><a href="#cite_note-tklalpha-10">[10]</a></sup> a simplified form of the <a href="/wiki/International_Phonetic_Alphabet" title="International Phonetic Alphabet">IPA</a>, compatible with modern Internet technology, making pronunciation more accurate. The community has not yet agreed on an official writing standard. While early scientific LMA writing is the most accurate system for dwelling into Scandinavian language phonetics, it has been used as a reference for the development of an IPA-based script.</p>\n<h2><span class="mw-headline" id="References">References</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=Kalix_dialect&amp;action=edit&amp;section=17" title="Edit section: References">edit</a><span class="mw-editsection-bracket">]</span></span></h2>\n<div class="reflist columns references-column-width" style="-moz-column-width: 30em; -webkit-column-width: 30em; column-width: 30em; list-style-type: decimal;">\n<ol class="references">\n<li id="cite_note-ol2000-1"><span class="mw-cite-backlink">^ <a href="#cite_ref-ol2000_1-0"><sup><i><b>a</b></i></sup></a> <a href="#cite_ref-ol2000_1-1"><sup><i><b>b</b></i></sup></a> <a href="#cite_ref-ol2000_1-2"><sup><i><b>c</b></i></sup></a></span> <span class="reference-text">\xc3\x85OLLEIST OPA K\xc3\x96LISM\xc3\x85LE, F\xc3\xb6reningen Kalix Bygdem\xc3\xa5l.</span></li>\n<li id="cite_note-rutberg1924-2"><span class="mw-cite-backlink">^ <a href="#cite_ref-rutberg1924_2-0"><sup><i><b>a</b></i></sup></a> <a href="#cite_ref-rutberg1924_2-1"><sup><i><b>b</b></i></sup></a> <a href="#cite_ref-rutberg1924_2-2"><sup><i><b>c</b></i></sup></a> <a href="#cite_ref-rutberg1924_2-3"><sup><i><b>d</b></i></sup></a></span> <span class="reference-text">Folkm\xc3\xa5let i Nederkalix och T\xc3\xb6re socknar av Hulda Rutberg, 1924, (174 pages)</span></li>\n<li id="cite_note-kfn02-3-3"><span class="mw-cite-backlink"><b><a href="#cite_ref-kfn02-3_3-0">^</a></b></span> <span class="reference-text">KALIXforskarNYTT, no.3-2002, Kalixbygdens Forskarf\xc3\xb6rening</span></li>\n<li id="cite_note-.C3.96vre_Norrlands_bygdem.C3.A5l-4"><span class="mw-cite-backlink"><b><a href="#cite_ref-.C3.96vre_Norrlands_bygdem.C3.A5l_4-0">^</a></b></span> <span class="reference-text">Dahlstedt &amp; \xc3\x85gren, \xc3\x96vre Norrlands bygdem\xc3\xa5l: ber\xc3\xa4ttelser p\xc3\xa5 bygdem\xc3\xa5l med f\xc3\xb6rklaringar och en dialekt\xc3\xb6versikt, utg. av Vetenskapliga biblioteket i Ume\xc3\xa5 1954</span></li>\n<li id="cite_note-sofi-5"><span class="mw-cite-backlink"><b><a href="#cite_ref-sofi_5-0">^</a></b></span> <span class="reference-text"><a rel="nofollow" class="external text" href="http://www.sofi.se/1602">Institutet f\xc3\xb6r spr\xc3\xa5k och folkminnen</a></span></li>\n<li id="cite_note-ol90-6"><span class="mw-cite-backlink"><b><a href="#cite_ref-ol90_6-0">^</a></b></span> <span class="reference-text">Kalixm\xc3\xa5let, s\xc3\xa5dant det talades p\xc3\xa5 1990-talet, F\xc3\xb6reningen Kalix Bygdem\xc3\xa5l.</span></li>\n<li id="cite_note-tkl-7"><span class="mw-cite-backlink"><b><a href="#cite_ref-tkl_7-0">^</a></b></span> <span class="reference-text"><a rel="nofollow" class="external text" href="http://thekalixlanguage.org">The Kalix Language .org \xe2\x80\x93 An online Kalix language course that includes lessons covering pronunciation, vocabulary, and grammar.</a></span></li>\n<li id="cite_note-nordia-8"><span class="mw-cite-backlink">^ <a href="#cite_ref-nordia_8-0"><sup><i><b>a</b></i></sup></a> <a href="#cite_ref-nordia_8-1"><sup><i><b>b</b></i></sup></a></span> <span class="reference-text">An introduction to Norwegian dialects, Olaf Husby (red), Tapir Akademic Press, Trondheim 2008</span></li>\n<li id="cite_note-dahl_gram-9"><span class="mw-cite-backlink"><b><a href="#cite_ref-dahl_gram_9-0">^</a></b></span> <span class="reference-text"><cite class="citation book">Dahl, \xc3\x96sten (2010). <a rel="nofollow" class="external text" href="http://www2.ling.su.se/staff/oesten/downloadable.htm"><i>Grammaticalization in the North: Noun Phrase Morphosyntax in Scandinavian Vernaculars</i></a>. Stockholm: Institutionen f\xc3\xb6r lingvistik vid Stockholms universitet. <a href="/wiki/International_Standard_Book_Number" title="International Standard Book Number">ISBN</a>&#160;<a href="/wiki/Special:BookSources/978-91-978304-1-6" title="Special:BookSources/978-91-978304-1-6">978-91-978304-1-6</a><span class="reference-accessdate">. Retrieved <span class="nowrap">2011-04-10</span></span>.</cite><span title="ctx_ver=Z39.88-2004&amp;rfr_id=info%3Asid%2Fen.wikipedia.org%3AKalix+dialect&amp;rft.aufirst=%C3%96sten&amp;rft.aulast=Dahl&amp;rft.btitle=Grammaticalization+in+the+North%3A+Noun+Phrase+Morphosyntax+in+Scandinavian+Vernaculars&amp;rft.date=2010&amp;rft.genre=book&amp;rft.isbn=978-91-978304-1-6&amp;rft.place=Stockholm&amp;rft.pub=Institutionen+f%C3%B6r+lingvistik+vid+Stockholms+universitet&amp;rft_id=http%3A%2F%2Fwww2.ling.su.se%2Fstaff%2Foesten%2Fdownloadable.htm&amp;rft_val_fmt=info%3Aofi%2Ffmt%3Akev%3Amtx%3Abook" class="Z3988"><span style="display:none;">&#160;</span></span></span></li>\n<li id="cite_note-tklalpha-10"><span class="mw-cite-backlink"><b><a href="#cite_ref-tklalpha_10-0">^</a></b></span> <span class="reference-text"><a rel="nofollow" class="external text" href="http://thekalixlanguage.org/en_dr/node/30">The Kalix Alphabet</a></span></li>\n</ol>\n</div>\n<div role="navigation" class="navbox" aria-labelledby="Modern_Germanic_languages_and_dialects" style="padding:3px">\n<table class="nowraplinks collapsible autocollapse navbox-inner" style="border-spacing:0;background:transparent;color:inherit">\n<tr>\n<th scope="col" class="navbox-title" colspan="2">\n<div class="plainlinks hlist navbar mini">\n<ul>\n<li class="nv-view"><a href="/wiki/Template:Germanic_languages" title="Template:Germanic languages"><abbr title="View this template" style=";;background:none transparent;border:none;-moz-box-shadow:none;-webkit-box-shadow:none;box-shadow:none;">v</abbr></a></li>\n<li class="nv-talk"><a href="/wiki/Template_talk:Germanic_languages" title="Template talk:Germanic languages"><abbr title="Discuss this template" style=";;background:none transparent;border:none;-moz-box-shadow:none;-webkit-box-shadow:none;box-shadow:none;">t</abbr></a></li>\n<li class="nv-edit"><a class="external text" href="//en.wikipedia.org/w/index.php?title=Template:Germanic_languages&amp;action=edit"><abbr title="Edit this template" style=";;background:none transparent;border:none;-moz-box-shadow:none;-webkit-box-shadow:none;box-shadow:none;">e</abbr></a></li>\n</ul>\n</div>\n<div id="Modern_Germanic_languages_and_dialects" style="font-size:114%;margin:0 4em"><a href="/wiki/Modern_language" title="Modern language">Modern</a> <a href="/wiki/Germanic_languages" title="Germanic languages">Germanic languages and dialects</a></div>\n</th>\n</tr>\n<tr>\n<th scope="row" class="navbox-group" style="width:1%">\n<div style="text-align: center;"><a href="/wiki/North_Germanic_languages" title="North Germanic languages">North<br />\nGermanic</a></div>\n</th>\n<td class="navbox-list navbox-odd hlist" style="text-align:left;border-left-width:2px;border-left-style:solid;width:100%;padding:0px">\n<div style="padding:0em 0.25em"></div>\n<table class="nowraplinks navbox-subgroup" style="border-spacing:0">\n<tr>\n<th scope="row" class="navbox-group" style="width:1%">\n<div style="text-align: center;">West<br />\nScandinavian</div>\n</th>\n<td class="navbox-list navbox-odd" style="text-align:left;border-left-width:2px;border-left-style:solid;width:100%;padding:0px">\n<div style="padding:0em 0.25em">\n<ul>\n<li><a href="/wiki/Bergensk" title="Bergensk">Bergensk</a></li>\n<li><a href="/wiki/Elfdalian" title="Elfdalian">Elfdalian</a></li>\n<li><a href="/wiki/Faroese_language" title="Faroese language">Faroese</a>\n<ul>\n<li><a href="/wiki/G%C3%B8tudanskt_accent" title="G\xc3\xb8tudanskt accent">G\xc3\xb8tudanskt</a></li>\n</ul>\n</li>\n<li><a href="/wiki/Icelandic_language" title="Icelandic language">Icelandic</a></li>\n<li><a href="/wiki/Jamtlandic_dialects" class="mw-redirect" title="Jamtlandic dialects">Jamtlandic</a></li>\n<li><a href="/wiki/Norwegian_language" title="Norwegian language">Norwegian</a>\n<ul>\n<li><a href="/wiki/Bokm%C3%A5l" title="Bokm\xc3\xa5l">Bokm\xc3\xa5l</a></li>\n<li><a href="/wiki/Nynorsk" title="Nynorsk">Nynorsk</a></li>\n</ul>\n</li>\n<li><a href="/wiki/Sognam%C3%A5l_dialect" title="Sognam\xc3\xa5l dialect">Sognam\xc3\xa5l</a></li>\n<li><a href="/wiki/Tr%C3%B8ndersk" title="Tr\xc3\xb8ndersk">Tr\xc3\xb8ndersk</a></li>\n<li><a href="/wiki/Valdris" class="mw-redirect" title="Valdris">Valdris</a></li>\n<li><a href="/wiki/Vestlandsk" title="Vestlandsk">Vestlandsk</a></li>\n<li><a href="/wiki/Vikv%C3%A6rsk" title="Vikv\xc3\xa6rsk">Vikv\xc3\xa6rsk</a></li>\n</ul>\n</div>\n</td>\n</tr>\n<tr>\n<th scope="row" class="navbox-group" style="width:1%">\n<div style="text-align: center;">East<br />\nScandinavian</div>\n</th>\n<td class="navbox-list navbox-even" style="text-align:left;border-left-width:2px;border-left-style:solid;width:100%;padding:0px">\n<div style="padding:0em 0.25em">\n<ul>\n<li><a href="/wiki/Bornholmsk_dialect" title="Bornholmsk dialect">Bornholmsk</a></li>\n<li><a href="/wiki/Dalecarlian_dialects" title="Dalecarlian dialects">Dalecarlian</a></li>\n<li><a href="/wiki/Danish_language" title="Danish language">Danish</a></li>\n<li><a href="/wiki/Insular_Danish" title="Insular Danish">Insular Danish</a></li>\n<li><a href="/wiki/Jutlandic_dialect" title="Jutlandic dialect">Jutlandic</a>\n<ul>\n<li><a href="/wiki/South_Jutlandic" title="South Jutlandic">South</a></li>\n</ul>\n</li>\n<li><a href="/wiki/Perkerdansk" title="Perkerdansk">Perkerdansk</a></li>\n<li><a href="/wiki/Gutnish" title="Gutnish">Gutnish</a></li>\n<li><a class="mw-selflink selflink">Kalix</a></li>\n<li><a href="/wiki/Kebabnorsk" title="Kebabnorsk">Kebabnorsk</a></li>\n<li><a href=