b'<!DOCTYPE html>\n<html class="client-nojs" lang="en" dir="ltr">\n<head>\n<meta charset="UTF-8"/>\n<title>Common Era - Wikipedia</title>\n<script>document.documentElement.className = document.documentElement.className.replace( /(^|\\s)client-nojs(\\s|$)/, "$1client-js$2" );</script>\n<script>(window.RLQ=window.RLQ||[]).push(function(){mw.config.set({"wgCanonicalNamespace":"","wgCanonicalSpecialPageName":false,"wgNamespaceNumber":0,"wgPageName":"Common_Era","wgTitle":"Common Era","wgCurRevisionId":797271181,"wgRevisionId":797271181,"wgArticleId":6088,"wgIsArticle":true,"wgIsRedirect":false,"wgAction":"view","wgUserName":null,"wgUserGroups":["*"],"wgCategories":["All articles with unsourced statements","Articles with unsourced statements from November 2016","CS1 Latin-language sources (la)","CS1 maint: Extra text: editors list","CS1 maint: Multiple names: authors list","All articles with dead external links","Articles with dead external links from January 2016","Webarchive template wayback links","Articles containing Latin-language text","Vague or ambiguous geographic scope from January 2017","Articles with unsourced statements from October 2015","Articles needing additional references from February 2012","All articles needing additional references","Articles with unsourced statements from November 2012","Articles with unsourced statements from July 2012","Articles containing Japanese-language text","Articles containing Korean-language text","Articles containing Macedonian-language text","Calendar eras","Chronology","Political correctness","Secularism"],"wgBreakFrames":false,"wgPageContentLanguage":"en","wgPageContentModel":"wikitext","wgSeparatorTransformTable":["",""],"wgDigitTransformTable":["",""],"wgDefaultDateFormat":"dmy","wgMonthNames":["","January","February","March","April","May","June","July","August","September","October","November","December"],"wgMonthNamesShort":["","Jan","Feb","Mar","Apr","May","Jun","Jul","Aug","Sep","Oct","Nov","Dec"],"wgRelevantPage