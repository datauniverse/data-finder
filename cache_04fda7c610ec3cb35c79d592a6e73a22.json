b'<!DOCTYPE html>\n<html class="client-nojs" lang="en" dir="ltr">\n<head>\n<meta charset="UTF-8"/>\n<title>Video on demand - Wikipedia</title>\n<script>document.documentElement.className = document.documentElement.className.replace( /(^|\\s)client-nojs(\\s|$)/, "$1client-js$2" );</script>\n<script>(window.RLQ=window.RLQ||[]).push(function(){mw.config.set({"wgCanonicalNamespace":"","wgCanonicalSpecialPageName":false,"wgNamespaceNumber":0,"wgPageName":"Video_on_demand","wgTitle":"Video on demand","wgCurRevisionId":796265757,"wgRevisionId":796265757,"wgArticleId":147143,"wgIsArticle":true,"wgIsRedirect":false,"wgAction":"view","wgUserName":null,"wgUserGroups":["*"],"wgCategories":["Webarchive template wayback links","Pages using citations with accessdate and no URL","Use dmy dates from June 2016","Articles needing additional references from August 2008","All articles needing additional references","All articles with unsourced statements","Articles with unsourced statements from December 2008","NPOV disputes from August 2016","All NPOV disputes","Articles with limited geographic scope from February 2015","Film and video technology","Television terminology","Video on demand","Internet television","Internet broadcasting","Streaming","Video on demand services","Online services","Network service","New media"],"wgBreakFrames":false,"wgPageContentLanguage":"en","wgPageContentModel":"wikitext","wgSeparatorTransformTable":["",""],"wgDigitTransformTable":["",""],"wgDefaultDateFormat":"dmy","wgMonthNames":["","January","February","March","April","May","June","July","August","September","October","November","December"],"wgMonthNamesShort":["","Jan","Feb","Mar","Apr","May","Jun","Jul","Aug","Sep","Oct","Nov","Dec"],"wgRelevantPageName":"Video_on_demand","wgRelevantArticleId":147143,"wgRequestId":"WaIOqwpAMCUAAFm7teUAAABH","wgIsProbablyEditable":true,"wgRelevantPageIsProbablyEditable":true,"wgRestrictionEdit":[],"wgRestrictionMove":[],"wgFlaggedRevsParams":{"tags":{}},"wgStableRevis