ite","mediawiki.page.startup","mediawiki.user","mediawiki.hidpi","mediawiki.page.ready","mediawiki.toc","mediawiki.searchSuggest","ext.gadget.teahouse","ext.gadget.ReferenceTooltips","ext.gadget.watchlist-notice","ext.gadget.DRN-wizard","ext.gadget.charinsert","ext.gadget.refToolbar","ext.gadget.extra-toolbar-buttons","ext.gadget.switcher","ext.gadget.featured-articles-links","ext.centralauth.centralautologin","mmv.head","mmv.bootstrap.autostart","ext.visualEditor.desktopArticleTarget.init","ext.visualEditor.targetLoader","ext.eventLogging.subscriber","ext.wikimediaEvents","ext.navigationTiming","ext.uls.eventlogger","ext.uls.init","ext.uls.interface","ext.centralNotice.geoIP","ext.centralNotice.startUp","skins.vector.js"]);});</script>\n<link rel="stylesheet" href="/w/load.php?debug=false&amp;lang=en&amp;modules=ext.cite.styles%7Cext.uls.interlanguage%7Cext.visualEditor.desktopArticleTarget.noscript%7Cext.wikimediaBadges%7Cmediawiki.legacy.commonPrint%2Cshared%7Cmediawiki.sectionAnchor%7Cmediawiki.skinning.interface%7Cskins.vector.styles%7Cwikibase.client.init&amp;only=styles&amp;skin=vector"/>\n<script async="" src="/w/load.php?debug=false&amp;lang=en&amp;modules=startup&amp;only=scripts&amp;skin=vector"></script>\n<meta name="ResourceLoaderDynamicStyles" content=""/>\n<link rel="stylesheet" href="/w/load.php?debug=false&amp;lang=en&amp;modules=ext.gadget.charinsert-styles&amp;only=styles&amp;skin=vector"/>\n<link rel="stylesheet" href="/w/load.php?debug=false&amp;lang=en&amp;modules=site.styles&amp;only=styles&amp;skin=vector"/>\n<meta name="generator" content="MediaWiki 1.30.0-wmf.14"/>\n<meta name="referrer" content="origin-when-cross-origin"/>\n<meta property="og:image" content="https://upload.wikimedia.org/wikipedia/commons/thumb/c/cc/Resume.pdf/page1-1200px-Resume.pdf.jpg"/>\n<link rel="alternate" href="android-app://org.wikipedia/http/en.m.wikipedia.org/wiki/R%C3%A9sum%C3%A9"/>\n<link rel="alternate" type="application/x-wiki" title="Edit this page" href="/w/index.php?title=R%C3%A9sum%C3%A9&amp;action=edit"/>\n<link rel="edit" title="Edit this page" href="/w/index.php?title=R%C3%A9sum%C3%A9&amp;action=edit"/>\n<link rel="apple-touch-icon" href="/static/apple-touch/wikipedia.png"/>\n<link rel="shortcut icon" href="/static/favicon/wikipedia.ico"/>\n<link rel="search" type="application/opensearchdescription+xml" href="/w/opensearch_desc.php" title="Wikipedia (en)"/>\n<link rel="EditURI" type="application/rsd+xml" href="//en.wikipedia.org/w/api.php?action=rsd"/>\n<link rel="license" href="//creativecommons.org/licenses/by-sa/3.0/"/>\n<link rel="canonical" href="https://en.wikipedia.org/wiki/R%C3%A9sum%C3%A9"/>\n<link rel="dns-prefetch" href="//login.wikimedia.org"/>\n<link rel="dns-prefetch" href="//meta.wikimedia.org" />\n<!--[if lt IE 9]><script src="/resources/lib/html5shiv/html5shiv.min.js"></script><![endif]-->\n</head>\n<body class="mediawiki ltr sitedir-ltr mw-hide-empty-elt ns-0 ns-subject page-R\xc3\xa9sum\xc3\xa9 rootpage-R\xc3\xa9sum\xc3\xa9 skin-vector action-view">\t\t<div id="mw-page-base" class="noprint"></div>\n\t\t<div id="mw-head-base" class="noprint"></div>\n\t\t<div id="content" class="mw-body" role="main">\n\t\t\t<a id="top"></a>\n\n\t\t\t\t\t\t\t<div id="siteNotice" class="mw-body-content"><!-- CentralNotice --></div>\n\t\t\t\t\t\t<div class="mw-indicators mw-body-content">\n</div>\n\t\t\t<h1 id="firstHeading" class="firstHeading" lang="en">R\xc3\xa9sum\xc3\xa9</h1>\n\t\t\t\t\t\t\t\t\t<div id="bodyContent" class="mw-body-content">\n\t\t\t\t\t\t\t\t\t<div id="siteSub" class="noprint">From Wikipedia, the free encyclopedia</div>\n\t\t\t\t\t\t\t\t<div id="contentSub"><span class="mw-redirectedfrom">\xc2\xa0\xc2\xa0(Redirected from <a href="/w/index.php?title=Resume&amp;redirect=no" class="mw-redirect" title="Resume">Resume</a>)</span></div>\n\t\t\t\t\t\t\t\t\t\t\t\t<div id="jump-to-nav" class="mw-jump">\n\t\t\t\t\tJump to:\t\t\t\t\t<a href="#mw-head">navigation</a>, \t\t\t\t\t<a href="#p-search">search</a>\n\t\t\t\t</div>\n\t\t\t\t<div id="mw-content-text" lang="en" dir="ltr" class="mw-content-ltr"><div class="mw-parser-output"><div role="note" class="hatnote navigation-not-searchable">"Resume" redirects here. For a definition of the verb "resume", see the Wiktionary entry <a href="https://en.wiktionary.org/wiki/resume" class="extiw" title="wikt:resume">resume</a>.</div>\n<div role="note" class="hatnote navigation-not-searchable">This article is about the personal marketing tool. For the music album, see <a href="/w/index.php?title=R%C3%A9sum%C3%A9%C2%BF_(album)&amp;action=edit&amp;redlink=1" class="new" title="R\xc3\xa9sum\xc3\xa9\xc2\xbf (album) (page does not exist)">R\xc3\xa9sum\xc3\xa9\xc2\xbf (album)</a>. For the Swedish newspaper, see <a href="/wiki/Resum%C3%A9_(magazine)" title="Resum\xc3\xa9 (magazine)">Resum\xc3\xa9 (magazine)</a>.</div>\n<table class="plainlinks metadata ambox ambox-content ambox-Refimprove" role="presentation">\n<tr>\n<td class="mbox-image">\n<div style="width:52px"><a href="/wiki/File:Question_book-new.svg" class="image"><img alt="" src="//upload.wikimedia.org/wikipedia/en/thumb/9/99/Question_book-new.svg/50px-Question_book-new.svg.png" width="50" height="39" srcset="//upload.wikimedia.org/wikipedia/en/thumb/9/99/Question_book-new.svg/75px-Question_book-new.svg.png 1.5x, //upload.wikimedia.org/wikipedia/en/thumb/9/99/Question_book-new.svg/100px-Question_book-new.svg.png 2x" data-file-width="512" data-file-height="399" /></a></div>\n</td>\n<td class="mbox-text"><span class="mbox-text-span">This article <b>needs additional citations for <a href="/wiki/Wikipedia:Verifiability" title="Wikipedia:Verifiability">verification</a></b>. <span class="hide-when-compact">Please help <a class="external text" href="//en.wikipedia.org/w/index.php?title=R%C3%A9sum%C3%A9&amp;action=edit">improve this article</a> by <a href="/wiki/Help:Introduction_to_referencing_with_Wiki_Markup/1" title="Help:Introduction to referencing with Wiki Markup/1">adding citations to reliable sources</a>. Unsourced material may be challenged and removed.</span> <small><i>(December 2009)</i></small> <small class="hide-when-compact"><i>(<a href="/wiki/Help:Maintenance_template_removal" title="Help:Maintenance template removal">Learn how and when to remove this template message</a>)</i></small></span></td>\n</tr>\n</table>\n<div class="thumb tright">\n<div class="thumbinner" style="width:222px;"><a href="/wiki/File:Resume.pdf" class="image"><img alt="" src="//upload.wikimedia.org/wikipedia/commons/thumb/c/cc/Resume.pdf/page1-220px-Resume.pdf.jpg" width="220" height="285" class="thumbimage" srcset="//upload.wikimedia.org/wikipedia/commons/thumb/c/cc/Resume.pdf/page1-330px-Resume.pdf.jpg 1.5x, //upload.wikimedia.org/wikipedia/commons/thumb/c/cc/Resume.pdf/page1-440px-Resume.pdf.jpg 2x" data-file-width="1275" data-file-height="1650" /></a>\n<div class="thumbcaption">\n<div class="magnify"><a href="/wiki/File:Resume.pdf" class="internal" title="Enlarge"></a></div>\nSample r\xc3\xa9sum\xc3\xa9 outline for a college student</div>\n</div>\n</div>\n<p>A <b>r\xc3\xa9sum\xc3\xa9</b> (<span class="nowrap"><span class="IPA nopopups noexcerpt"><a href="/wiki/Help:IPA/English" title="Help:IPA/English">/<span style="border-bottom:1px dotted"><span title="/\xcb\x88/: primary stress follows">\xcb\x88</span><span title="\'r\' in \'rye\'">r</span><span title="/\xc9\x9b/: \'e\' in \'dress\'">\xc9\x9b</span><span title="\'z\' in \'zoom\'">z</span><span title="/\xca\x8a/: \'oo\' in \'foot\'">\xca\x8a</span><span title="\'m\' in \'my\'">m</span><span title="/e\xc9\xaa/: \'a\' in \'face\'">e\xc9\xaa</span></span>/</a></span></span>, <a href="/wiki/Help:Pronunciation_respelling_key" title="Help:Pronunciation respelling key"><small title="English pronunciation respelling"><i><b>REZ</b>-u-may</i></small></a> or <span class="nowrap"><span class="IPA nopopups noexcerpt"><a href="/wiki/Help:IPA/English" title="Help:IPA/English">/<span style="border-bottom:1px dotted"><span title="\'r\' in \'rye\'">r</span><span title="/\xc9\x9b/: \'e\' in \'dress\'">\xc9\x9b</span><span title="\'z\' in \'zoom\'">z</span><span title="/\xca\x8a/: \'oo\' in \'foot\'">\xca\x8a</span><span title="/\xcb\x88/: primary stress follows">\xcb\x88</span><span title="\'m\' in \'my\'">m</span><span title="/e\xc9\xaa/: \'a\' in \'face\'">e\xc9\xaa</span></span>/</a></span></span>; less frequently <span class="nowrap"><span class="IPA nopopups noexcerpt"><a href="/wiki/Help:IPA/English" title="Help:IPA/English">/<span style="border-bottom:1px dotted"><span title="/\xcb\x88/: primary stress follows">\xcb\x88</span><span title="\'r\' in \'rye\'">r</span><span title="/\xc9\x9b/: \'e\' in \'dress\'">\xc9\x9b</span><span title="\'z\' in \'zoom\'">z</span><span title="/j/: \'y\' in \'yes\'">j</span><span title="/\xca\x8a/: \'oo\' in \'foot\'">\xca\x8a</span><span title="\'m\' in \'my\'">m</span><span title="/e\xc9\xaa/: \'a\' in \'face\'">e\xc9\xaa</span></span>/</a></span></span> or <span class="nowrap"><span class="IPA nopopups noexcerpt"><a href="/wiki/Help:IPA/English" title="Help:IPA/English">/<span style="border-bottom:1px dotted"><span title="\'r\' in \'rye\'">r</span><span title="/\xc9\x9b/: \'e\' in \'dress\'">\xc9\x9b</span><span title="\'z\' in \'zoom\'">z</span><span title="/j/: \'y\' in \'yes\'">j</span><span title="/\xca\x8a/: \'oo\' in \'foot\'">\xca\x8a</span><span title="/\xcb\x88/: primary stress follows">\xcb\x88</span><span title="\'m\' in \'my\'">m</span><span title="/e\xc9\xaa/: \'a\' in \'face\'">e\xc9\xaa</span></span>/</a></span></span>; <small>French:&#160;</small><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA"><a href="/wiki/Help:IPA/French" title="Help:IPA/French">[\xca\x81ezyme]</a></span>),<sup id="cite_ref-1" class="reference"><a href="#cite_note-1">[a]</a></sup> also spelled <b>resume</b>,<sup id="cite_ref-2" class="reference"><a href="#cite_note-2">[1]</a></sup> is a document used by a person to present their backgrounds and skills. R\xc3\xa9sum\xc3\xa9s can be used for a variety of reasons, but most often they are used to secure new employment.<sup id="cite_ref-3" class="reference"><a href="#cite_note-3">[2]</a></sup></p>\n<p>A typical r\xc3\xa9sum\xc3\xa9 contains a "summary" of relevant job <a href="/wiki/Experience" title="Experience">experience</a> and <a href="/wiki/Education" title="Education">education</a>, as its French origin (and its translation into Spanish as "resumen") implies. The r\xc3\xa9sum\xc3\xa9 is usually one of the first items, along with a <a href="/wiki/Cover_letter" title="Cover letter">cover letter</a> and sometimes an application for employment, which a potential <a href="/wiki/Employer" class="mw-redirect" title="Employer">employer</a> sees regarding the job seeker and is typically used to <a href="/wiki/Screening_(economics)" title="Screening (economics)">screen</a> applicants, often followed by an <a href="/wiki/Interview" title="Interview">interview</a>.</p>\n<p>The <a href="/wiki/Curriculum_vitae" title="Curriculum vitae">curriculum vitae</a> (CV) used for employment purposes in the UK (and in other European countries) is more akin to the r\xc3\xa9sum\xc3\xa9\xe2\x80\x94a shorter, summary version of one\'s education and experience\xe2\x80\x94than to the longer and more detailed CV that is expected in U.S. academic circles.</p>\n<p>Generally, the r\xc3\xa9sum\xc3\xa9 is substantially shorter than a CV in <a href="/wiki/English_Canada" title="English Canada">English Canada</a>, the <a href="/wiki/United_States" title="United States">U.S.</a> and <a href="/wiki/Australia" title="Australia">Australia</a>.<sup id="cite_ref-4" class="reference"><a href="#cite_note-4">[3]</a></sup></p>\n<p></p>\n<div id="toc" class="toc">\n<div class="toctitle">\n<h2>Contents</h2>\n</div>\n<ul>\n<li class="toclevel-1 tocsection-1"><a href="#History"><span class="tocnumber">1</span> <span class="toctext">History</span></a></li>\n<li class="toclevel-1 tocsection-2"><a href="#Description"><span class="tocnumber">2</span> <span class="toctext">Description</span></a></li>\n<li class="toclevel-1 tocsection-3"><a href="#Styles"><span class="tocnumber">3</span> <span class="toctext">Styles</span></a>\n<ul>\n<li class="toclevel-2 tocsection-4"><a href="#Reverse_chronological_r.C3.A9sum.C3.A9"><span class="tocnumber">3.1</span> <span class="toctext">Reverse chronological r\xc3\xa9sum\xc3\xa9</span></a></li>\n<li class="toclevel-2 tocsection-5"><a href="#Functional_r.C3.A9sum.C3.A9"><span class="tocnumber">3.2</span> <span class="toctext">Functional r\xc3\xa9sum\xc3\xa9</span></a></li>\n<li class="toclevel-2 tocsection-6"><a href="#Hybrid_r.C3.A9sum.C3.A9"><span class="tocnumber">3.3</span> <span class="toctext">Hybrid r\xc3\xa9sum\xc3\xa9</span></a></li>\n<li class="toclevel-2 tocsection-7"><a href="#Online_r.C3.A9sum.C3.A9s"><span class="tocnumber">3.4</span> <span class="toctext">Online r\xc3\xa9sum\xc3\xa9s</span></a></li>\n<li class="toclevel-2 tocsection-8"><a href="#Infographic.2C_video_and_website_r.C3.A9sum.C3.A9s"><span class="tocnumber">3.5</span> <span class="toctext">Infographic, video and website r\xc3\xa9sum\xc3\xa9s</span></a></li>\n</ul>\n</li>\n<li class="toclevel-1 tocsection-9"><a href="#R.C3.A9sum.C3.A9_evaluation"><span class="tocnumber">4</span> <span class="toctext">R\xc3\xa9sum\xc3\xa9 evaluation</span></a></li>\n<li class="toclevel-1 tocsection-10"><a href="#R.C3.A9sum.C3.A9_as_one_part_of_a_personal_branding_mix"><span class="tocnumber">5</span> <span class="toctext">R\xc3\xa9sum\xc3\xa9 as one part of a personal branding mix</span></a></li>\n<li class="toclevel-1 tocsection-11"><a href="#See_also"><span class="tocnumber">6</span> <span class="toctext">See also</span></a></li>\n<li class="toclevel-1 tocsection-12"><a href="#Notes"><span class="tocnumber">7</span> <span class="toctext">Notes</span></a></li>\n<li class="toclevel-1 tocsection-13"><a href="#References"><span class="tocnumber">8</span> <span class="toctext">References</span></a></li>\n<li class="toclevel-1 tocsection-14"><a href="#Bibliography"><span class="tocnumber">9</span> <span class="toctext">Bibliography</span></a></li>\n</ul>\n</div>\n<p></p>\n<h2><span class="mw-headline" id="History">History</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=R%C3%A9sum%C3%A9&amp;action=edit&amp;section=1" title="Edit section: History">edit</a><span class="mw-editsection-bracket">]</span></span></h2>\n<p>As has been indicated above, the word r\xc3\xa9sum\xc3\xa9 comes from the French word <i>r\xc3\xa9sum\xc3\xa9</i> meaning "summary".<sup id="cite_ref-5" class="reference"><a href="#cite_note-5">[4]</a></sup> <a href="/wiki/Leonardo_da_Vinci" title="Leonardo da Vinci">Leonardo da Vinci</a> is credited with the first r\xc3\xa9sum\xc3\xa9 though his "r\xc3\xa9sum\xc3\xa9" takes the form of a letter written about 1481\xe2\x80\x931482 to a potential employer, Ludovico Sforza.<sup id="cite_ref-6" class="reference"><a href="#cite_note-6">[5]</a></sup><sup id="cite_ref-7" class="reference"><a href="#cite_note-7">[6]</a></sup> For the next roughly 450 years, the r\xc3\xa9sum\xc3\xa9 continued to be a mere description of a person, and included their abilities and past employment. In the early 1900s, r\xc3\xa9sum\xc3\xa9s listed things like weight, height, marital status, and religion. It wasn\'t until 1950 that the r\xc3\xa9sum\xc3\xa9 evolved into something more than words written on scraps of paper. By then, r\xc3\xa9sum\xc3\xa9s were considered very much mandatory, and started to include things like personal interests and hobbies. It wasn\'t until the 1970s, the beginning of the digital age, that r\xc3\xa9sum\xc3\xa9s took on a more professional look in terms of presentation and content.<sup id="cite_ref-8" class="reference"><a href="#cite_note-8">[7]</a></sup></p>\n<h2><span class="mw-headline" id="Description">Description</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=R%C3%A9sum%C3%A9&amp;action=edit&amp;section=2" title="Edit section: Description">edit</a><span class="mw-editsection-bracket">]</span></span></h2>\n<p>In many contexts, a r\xc3\xa9sum\xc3\xa9 is typically limited to one or two pages of size <a href="/wiki/ISO_216" title="ISO 216">A4</a> or <a href="/wiki/Letter_paper" class="mw-redirect" title="Letter paper">letter-size</a>, highlighting only those experiences and qualifications that the author considers most relevant to the desired position. Many r\xc3\xa9sum\xc3\xa9s contain <a href="/wiki/Index_term" title="Index term">keywords</a> or <a href="/wiki/Skills" class="mw-redirect" title="Skills">skills</a> that potential employers are looking for via <a href="/wiki/Applicant_tracking_system" title="Applicant tracking system">applicant tracking systems</a>, make heavy use of active verbs, and display content in a flattering manner. Acronyms and credentials after the applicant\'s name should be spelled out fully in the appropriate section of the resume, greater chance of being found in a computerized keyword scan.<sup id="cite_ref-9" class="reference"><a href="#cite_note-9">[8]</a></sup> R\xc3\xa9sum\xc3\xa9s can vary in style and length, but should always contain accurate contact information of the job seeker.</p>\n<p>A r\xc3\xa9sum\xc3\xa9 is a marketing tool in which the content should be adapted to suit each individual job application or applications aimed at a particular industry. The transmission of r\xc3\xa9sum\xc3\xa9s directly to employers became increasingly popular as late as 2002.<sup class="noprint Inline-Template Template-Fact" style="white-space:nowrap;">[<i><a href="/wiki/Wikipedia:Citation_needed" title="Wikipedia:Citation needed"><span title="This claim needs references to reliable sources. (February 2010)">citation needed</span></a></i>]</sup> Job seekers were able to circumvent the <a href="/wiki/Application_for_employment" title="Application for employment">job application</a> process and reach employers through direct email contact and r\xc3\xa9sum\xc3\xa9 blasting, a term meaning the mass distribution of r\xc3\xa9sum\xc3\xa9s to increase personal visibility within the job market. However, the mass distribution of r\xc3\xa9sum\xc3\xa9s to employers can often have a negative effect on the applicant\'s chances of securing employment as the r\xc3\xa9sum\xc3\xa9s tend not to be tailored for the specific positions the applicant is applying for. It is usually, therefore, more sensible to optimize the r\xc3\xa9sum\xc3\xa9 for each position applied for and its keywords. In order to keep track of all experiences, keeping a \'master r\xc3\xa9sum\xc3\xa9\' document is recommended, providing job-seekers with the ability to customize a tailored r\xc3\xa9sum\xc3\xa9 while making sure extraneous information is easily accessible for future use if needed.</p>\n<p>The complexity or simplicity of various r\xc3\xa9sum\xc3\xa9 formats tends to produce results varying from person to person, for the occupation, and to the industry. R\xc3\xa9sum\xc3\xa9s or CVs used by medical professionals, professors, artists and people in other specialized fields may be comparatively longer. For example, an artist\'s r\xc3\xa9sum\xc3\xa9, typically excluding any non-art-related employment, may include extensive lists of <a href="/wiki/Solo_exhibition" class="mw-redirect" title="Solo exhibition">solo</a> and group <a href="/wiki/Art_exhibitions" class="mw-redirect" title="Art exhibitions">exhibitions</a>.</p>\n<h2><span class="mw-headline" id="Styles">Styles</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=R%C3%A9sum%C3%A9&amp;action=edit&amp;section=3" title="Edit section: Styles">edit</a><span class="mw-editsection-bracket">]</span></span></h2>\n<p>R\xc3\xa9sum\xc3\xa9s may be organized in different ways. The following are some of the more common r\xc3\xa9sum\xc3\xa9 formats:</p>\n<h3><span class="mw-headline" id="Reverse_chronological_r.C3.A9sum.C3.A9">Reverse chronological r\xc3\xa9sum\xc3\xa9</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=R%C3%A9sum%C3%A9&amp;action=edit&amp;section=4" title="Edit section: Reverse chronological r\xc3\xa9sum\xc3\xa9">edit</a><span class="mw-editsection-bracket">]</span></span></h3>\n<p>A reverse chronological r\xc3\xa9sum\xc3\xa9 lists a candidate\'s job experiences in chronological order, generally covering the previous 10 to 15 years. Positions are listed with starting and ending dates. Current positions on a r\xc3\xa9sum\xc3\xa9 typically list the starting date to the present. The reverse chronological r\xc3\xa9sum\xc3\xa9 format<sup id="cite_ref-10" class="reference"><a href="#cite_note-10">[9]</a></sup> is most commonly used by those who are not professional r\xc3\xa9sum\xc3\xa9 writers. In using this format, the main body of the document becomes the Professional Experience section, starting from the most recent experience and moving chronologically backwards through a succession of previous experience. The reverse chronological r\xc3\xa9sum\xc3\xa9 works to build credibility through experience gained, while illustrating career growth over time and filling all gaps in a career trajectory. A chronological r\xc3\xa9sum\xc3\xa9 is not recommended to job seekers with gaps in their career summaries. In the United Kingdom the chronological r\xc3\xa9sum\xc3\xa9 tends to extend only as far back as the applicant\'s <a href="/wiki/GCSE" class="mw-redirect" title="GCSE">GCSE</a>/<a href="/wiki/Standard_Grade" title="Standard Grade">Standard Grade</a> qualifications.</p>\n<h3><span class="mw-headline" id="Functional_r.C3.A9sum.C3.A9">Functional r\xc3\xa9sum\xc3\xa9</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=R%C3%A9sum%C3%A9&amp;action=edit&amp;section=5" title="Edit section: Functional r\xc3\xa9sum\xc3\xa9">edit</a><span class="mw-editsection-bracket">]</span></span></h3>\n<p>A functional r\xc3\xa9sum\xc3\xa9 lists work experience and skills sorted by skill area or job function.</p>\n<p>The functional r\xc3\xa9sum\xc3\xa9 is used to focus on skills that are specific to the type of position being sought. This format directly emphasizes specific professional capabilities and utilizes experience summaries as its primary means of communicating professional competency. In contrast, the chronological r\xc3\xa9sum\xc3\xa9 format will briefly highlight these competencies prior to presenting a comprehensive timeline of career growth through reverse chronological listings, with the most recent experience listed first. The functional r\xc3\xa9sum\xc3\xa9 works well for those making a career change, having a varied work history or with little work experience. A functional r\xc3\xa9sum\xc3\xa9 is also preferred for applications to jobs that require very specific skills or clearly defined personality traits. A functional r\xc3\xa9sum\xc3\xa9 is a good method for highlighting particular skills or experiences, especially when those particular skills or experiences may have derived from a role which was held some time ago. Rather than focus on the length of time that has passed, the functional r\xc3\xa9sum\xc3\xa9 allows the reader to identify those skills quickly.</p>\n<h3><span class="mw-headline" id="Hybrid_r.C3.A9sum.C3.A9">Hybrid r\xc3\xa9sum\xc3\xa9</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=R%C3%A9sum%C3%A9&amp;action=edit&amp;section=6" title="Edit section: Hybrid r\xc3\xa9sum\xc3\xa9">edit</a><span class="mw-editsection-bracket">]</span></span></h3>\n<p>The hybrid r\xc3\xa9sum\xc3\xa9 balances the functional and <a href="/wiki/Chronological" class="mw-redirect" title="Chronological">chronological</a> approaches. A r\xc3\xa9sum\xc3\xa9 organized this way typically leads with a functional list of job skills, followed by a chronological list of employers. The hybrid has a tendency to repeat itself and is, therefore, less widely used than the other two.</p>\n<h3><span class="mw-headline" id="Online_r.C3.A9sum.C3.A9s">Online r\xc3\xa9sum\xc3\xa9s</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=R%C3%A9sum%C3%A9&amp;action=edit&amp;section=7" title="Edit section: Online r\xc3\xa9sum\xc3\xa9s">edit</a><span class="mw-editsection-bracket">]</span></span></h3>\n<p>As the search for employment has become more electronic, it is common for employers only to accept r\xc3\xa9sum\xc3\xa9s electronically, either out of practicality or preference. This has changed much about the way r\xc3\xa9sum\xc3\xa9s are written, read, and processed. Some career experts are pointing out that today a paper-based resume is an exception rather than the rule.<sup id="cite_ref-11" class="reference"><a href="#cite_note-11">[10]</a></sup></p>\n<p>Many employers now find candidates\' r\xc3\xa9sum\xc3\xa9s through <a href="/wiki/Search_engine" class="mw-redirect" title="Search engine">search engines</a>, which makes it more important for candidates to use appropriate keywords when writing a r\xc3\xa9sum\xc3\xa9.<sup id="cite_ref-12" class="reference"><a href="#cite_note-12">[11]</a></sup> Larger employers use <a href="/wiki/Applicant_tracking_system" title="Applicant tracking system">Applicant Tracking Systems</a> to search, filter, and manage high volumes of r\xc3\xa9sum\xc3\xa9s. Job ads may direct applicants to email a r\xc3\xa9sum\xc3\xa9 to a company or visit its website and submit a r\xc3\xa9sum\xc3\xa9 in an electronic format.</p>\n<p>Many employers, and recruitment agencies working on their behalf, insist on receiving r\xc3\xa9sum\xc3\xa9s in a particular <a href="/wiki/File_format" title="File format">file format</a>. Some require <a href="/wiki/Microsoft_Word" title="Microsoft Word">Microsoft Word</a> documents, while others will only accept r\xc3\xa9sum\xc3\xa9s formatted in <a href="/wiki/HTML" title="HTML">HTML</a>, <a href="/wiki/PDF" class="mw-redirect" title="PDF">PDF</a>, or plain <a href="/wiki/ASCII" title="ASCII">ASCII</a> text.</p>\n<p>Another consideration for electronic r\xc3\xa9sum\xc3\xa9 documents is that they are parsed with <a href="/wiki/Natural_language_processing" title="Natural language processing">natural language processors</a>. Resume parsers may correctly interpret some parts of the content of the r\xc3\xa9sum\xc3\xa9 but not other parts. The best r\xc3\xa9sum\xc3\xa9 parsers capture a high percentage of information regarding location, names, titles, but are less accurate with skills, industries and other less structured or rapidly changing data.<sup id="cite_ref-13" class="reference"><a href="#cite_note-13">[12]</a></sup> R\xc3\xa9sum\xc3\xa9 written in a standard format are more likely to be correctly interpreted by r\xc3\xa9sum\xc3\xa9 parsers, and thereby will make the candidate more findable.</p>\n<p>One advantage for employers to online r\xc3\xa9sum\xc3\xa9s is the significant cost saving compared to traditional hiring methods.<sup id="cite_ref-14" class="reference"><a href="#cite_note-14">[13]</a></sup> Another is that potential employers no longer have to sort through massive stacks of paper.</p>\n<h3><span class="mw-headline" id="Infographic.2C_video_and_website_r.C3.A9sum.C3.A9s">Infographic, video and website r\xc3\xa9sum\xc3\xa9s</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=R%C3%A9sum%C3%A9&amp;action=edit&amp;section=8" title="Edit section: Infographic, video and website r\xc3\xa9sum\xc3\xa9s">edit</a><span class="mw-editsection-bracket">]</span></span></h3>\n<p>As the Internet becomes more driven by multimedia, job-seekers have sought to take advantage of the trend by moving their r\xc3\xa9sum\xc3\xa9s away from the traditional paper and email media.</p>\n<p><a href="/wiki/Video_resume" title="Video resume">Video</a>, infographic, and even <a href="/wiki/Vine_(service)" title="Vine (service)">Vine</a> r\xc3\xa9sum\xc3\xa9s have gained popularity, though mainly in the creative and media industries.<sup id="cite_ref-15" class="reference"><a href="#cite_note-15">[14]</a></sup></p>\n<p>This trend has attracted criticism from <a href="/wiki/Human_resources_management" class="mw-redirect" title="Human resources management">human resources management</a> professionals, who warn that this may be a passing fad and point out that multimedia-based r\xc3\xa9sum\xc3\xa9s may be overlooked by recruiters whose <a href="/wiki/Workflow" title="Workflow">workflow</a> is designed only to accommodate a traditional r\xc3\xa9sum\xc3\xa9 format.<sup id="cite_ref-16" class="reference"><a href="#cite_note-16">[15]</a></sup></p>\n<h2><span class="mw-headline" id="R.C3.A9sum.C3.A9_evaluation">R\xc3\xa9sum\xc3\xa9 evaluation</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=R%C3%A9sum%C3%A9&amp;action=edit&amp;section=9" title="Edit section: R\xc3\xa9sum\xc3\xa9 evaluation">edit</a><span class="mw-editsection-bracket">]</span></span></h2>\n<p>Many r\xc3\xa9sum\xc3\xa9 development agencies offer r\xc3\xa9sum\xc3\xa9 evaluation services wherein they evaluate the r\xc3\xa9sum\xc3\xa9 and suggest any necessary changes. Candidates are free to either do those changes themselves or may take help of the agency itself. Some career fields include a special section listing the lifelong works of the author: for computer-related fields, the <a href="/wiki/Softography" class="mw-redirect" title="Softography">softography</a>; for musicians and composers, the <a href="/wiki/Discography" title="Discography">discography</a>; for actors, a <a href="/wiki/Filmography" title="Filmography">filmography</a>.</p>\n<p>Keeping r\xc3\xa9sum\xc3\xa9s online has become increasingly common for people in professions that benefit from the multimedia and rich detail that are offered by an HTML r\xc3\xa9sum\xc3\xa9, such as actors, photographers, graphic designers, developers, dancers, etc.<sup id="cite_ref-17" class="reference"><a href="#cite_note-17">[16]</a></sup> Job seekers are finding an ever-increasing demand to have an electronic version of their r\xc3\xa9sum\xc3\xa9 available to employers and professionals who use <a href="/wiki/Internet_recruiting" title="Internet recruiting">Internet recruiting</a>.<sup id="cite_ref-18" class="reference"><a href="#cite_note-18">[17]</a></sup> Online r\xc3\xa9sum\xc3\xa9 distribution services have emerged to allow job seekers to distribute their r\xc3\xa9sum\xc3\xa9s to numerous employers of their choice through email.<sup id="cite_ref-19" class="reference"><a href="#cite_note-19">[18]</a></sup></p>\n<h2><span class="mw-headline" id="R.C3.A9sum.C3.A9_as_one_part_of_a_personal_branding_mix">R\xc3\xa9sum\xc3\xa9 as one part of a personal branding mix</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=R%C3%A9sum%C3%A9&amp;action=edit&amp;section=10" title="Edit section: R\xc3\xa9sum\xc3\xa9 as one part of a personal branding mix">edit</a><span class="mw-editsection-bracket">]</span></span></h2>\n<p>In some sectors, particularly in the <a href="/wiki/Startup_company" title="Startup company">startup</a> community, use of traditional r\xc3\xa9sum\xc3\xa9 has seen a consistent decline.<sup id="cite_ref-20" class="reference"><a href="#cite_note-20">[19]</a></sup> While standalone r\xc3\xa9sum\xc3\xa9s are still used to apply for jobs, job-seekers may also view their r\xc3\xa9sum\xc3\xa9s as one of a number of assets which form their <a href="/wiki/Personal_branding" title="Personal branding">personal brand</a> and work together to strengthen their job application. In this scenario, r\xc3\xa9sum\xc3\xa9s are generally used to provide a potential employer with factual information (e.g., achievements), while the social media platforms give insight into the job-seekers\' motivations and personality.</p>\n<h2><span class="mw-headline" id="See_also">See also</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=R%C3%A9sum%C3%A9&amp;action=edit&amp;section=11" title="Edit section: See also">edit</a><span class="mw-editsection-bracket">]</span></span></h2>\n<ul>\n<li><a href="/wiki/Background_check" title="Background check">Background check</a></li>\n<li><a href="/wiki/Europass" title="Europass">Europass</a> European Standardized model</li>\n<li><a href="/wiki/Federal_Resume" class="mw-redirect" title="Federal Resume">Federal Resume</a></li>\n<li><a href="/wiki/HResume" title="HResume">hResume</a>&#160;\xe2\x80\x93 a <a href="/wiki/Microformat" title="Microformat">microformat</a> for marking up r\xc3\xa9sum\xc3\xa9s on web pages.</li>\n<li><a href="/wiki/Resume_fraud" class="mw-redirect" title="Resume fraud">R\xc3\xa9sum\xc3\xa9 fraud</a></li>\n<li><a href="/wiki/Curriculum_vitae" title="Curriculum vitae">Curriculum vitae</a></li>\n</ul>\n<h2><span class="mw-headline" id="Notes">Notes</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=R%C3%A9sum%C3%A9&amp;action=edit&amp;section=12" title="Edit section: Notes">edit</a><span class="mw-editsection-bracket">]</span></span></h2>\n<div class="reflist" style="list-style-type: lower-alpha;">\n<div class="mw-references-wrap">\n<ol class="references">\n<li id="cite_note-1"><span class="mw-cite-backlink"><b><a href="#cite_ref-1">^</a></b></span> <span class="reference-text">In French, <i>r\xc3\xa9sum\xc3\xa9</i> means "summary".</span></li>\n</ol>\n</div>\n</div>\n<h2><span class="mw-headline" id="References">References</span><span class="mw-editsection"><span class="mw-editsection-bracket">[</span><a href="/w/index.php?title=R%C3%A9sum%C3%A9&amp;action=edit&amp;section=13" title="Edit section: References">edit</a><span