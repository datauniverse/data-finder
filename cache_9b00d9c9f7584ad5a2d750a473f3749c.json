b'<!DOCTYPE html>\n<html class="client-nojs" lang="en" dir="ltr">\n<head>\n<meta charset="UTF-8"/>\n<title>Physical geography - Wikipedia</title>\n<script>document.documentElement.className = document.documentElement.className.replace( /(^|\\s)client-nojs(\\s|$)/, "$1client-js$2" );</script>\n<script>(window.RLQ=window.RLQ||[]).push(function(){mw.config.set({"wgCanonicalNamespace":"","wgCanonicalSpecialPageName":false,"wgNamespaceNumber":0,"wgPageName":"Physical_geography","wgTitle":"Physical geography","wgCurRevisionId":787516686,"wgRevisionId":787516686,"wgArticleId":23263,"wgIsArticle":true,"wgIsRedirect":false,"wgAction":"view","wgUserName":null,"wgUserGroups":["*"],"wgCategories":["Articles needing cleanup from November 2009","All pages needing cleanup","Cleanup tagged articles without a reason field from November 2009","Wikipedia pages needing cleanup from November 2009","All pages needing factual verification","Wikipedia articles needing factual verification from September 2010","All articles with unsourced statements","Articles with unsourced statements from April 2010","Articles lacking in-text citations from February 2008","All articles lacking in-text citations","Wikipedia articles with GND identifiers","Physical geography","Branches of geography","Earth sciences"],"wgBreakFrames":false,"wgPageContentLanguage":"en","wgPageContentModel":"wikitext","wgSeparatorTransformTable":["",""],"wgDigitTransformTable":["",""],"wgDefaultDateFormat":"dmy","wgMonthNames":["","January","February","March","April","May","June","July","August","September","October","November","December"],"wgMonthNamesShort":["","Jan","Feb","Mar","Apr","May","Jun","Jul","Aug","Sep","Oct","Nov","Dec"],"wgRelevantPageName":"Physical_geography","wgRelevantArticleId":23263,"wgRequestId":"WZxUHwpAAD0AAIlvRx0AAACA","wgIsProbablyEditable":true,"wgRelevantPageIsProbablyEditable":true,"wgRestrictionEdit":[],"wgRestrictionMove":[],"wgFlaggedRevsParams":{"tags":{}},"wgStableRevisionId":null,"wgWikiEdito