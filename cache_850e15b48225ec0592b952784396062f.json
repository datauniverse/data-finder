b'<!DOCTYPE html>\n<html class="client-nojs" lang="en" dir="ltr">\n<head>\n<meta charset="UTF-8"/>\n<title>Italian Wikipedia - Wikipedia</title>\n<script>document.documentElement.className = document.documentElement.className.replace( /(^|\\s)client-nojs(\\s|$)/, "$1client-js$2" );</script>\n<script>(window.RLQ=window.RLQ||[]).push(function(){mw.config.set({"wgCanonicalNamespace":"","wgCanonicalSpecialPageName":false,"wgNamespaceNumber":0,"wgPageName":"Italian_Wikipedia","wgTitle":"Italian Wikipedia","wgCurRevisionId":793921930,"wgRevisionId":793921930,"wgArticleId":1313726,"wgIsArticle":true,"wgIsRedirect":false,"wgAction":"view","wgUserName":null,"wgUserGroups":["*"],"wgCategories":["CS1 errors: external links","CS1 Italian-language sources (it)","Pages using deprecated image syntax","All articles with unsourced statements","Articles with unsourced statements from November 2016","Articles containing Italian-language text","Articles containing potentially dated statements from October 2011","All articles containing potentially dated statements","Articles needing additional references from September 2008","All articles needing additional references","Wikipedias by language","Italian-language encyclopedias","Internet properties established in 2001","Companies established in 2001","Italian-language websites"],"wgBreakFrames":false,"wgPageContentLanguage":"en","wgPageContentModel":"wikitext","wgSeparatorTransformTable":["",""],"wgDigitTransformTable":["",""],"wgDefaultDateFormat":"dmy","wgMonthNames":["","January","February","March","April","May","June","July","August","September","October","November","December"],"wgMonthNamesShort":["","Jan","Feb","Mar","Apr","May","Jun","Jul","Aug","Sep","Oct","Nov","Dec"],"wgRelevantPageName":"Italian_Wikipedia","wgRelevantArticleId":1313726,"wgRequestId":"WaJX-ApAMFMAAHt1OYgAAABM","wgIsProbablyEditable":true,"wgRelevantPageIsProbablyEditable":true,"wgRestrictionEdit":[],"wgRestrictionMove":[],"wgFlaggedRevsParams":{"tags":{}},"wgSt